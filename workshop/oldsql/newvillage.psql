INSERT INTO public.sites_faciles_contentpage VALUES (3, '[{"id": "611a63a2-ab81-4308-a753-13e279acf497", "type": "imageandtext", "value": {"link": {"text": "Village !", "external_url": "https://www.village.org"}, "text": "<p>Bienvenue !</p>\n\n        <p>Vous venez de créer un site utilisant le gestionnaire de contenus de l’État.</p>\n\n        <p>Vous pouvez maintenant vous\n        <a href-=''/cms-admin/''>connecter dans l’administration</a> et personnaliser le site.</p>\n\n        ", "image": 1, "image_ratio": "3"}}, {"id": "e4fee4d4-775c-4c66-967b-21cdc3158299", "type": "paragraph", "value": "\n        <p>En particulier, vous devrez :</p>\n        <ul>\n            <li>Configurer le site dans Configuration > Configuration du site</li>\n            <li>Remplacer le contenu de la page de mentions légales</li>\n            <li>Remplacer le contenu de cette page d’accueil.</li>\n        </ul>\n        "}]', false, NULL, false, false, NULL, NULL, NULL, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (4, '[{"id": "1d7055ce-e5af-48d3-9995-54cb5ba83479", "type": "alert", "value": {"level": "info", "title": "Mentions légales", "description": "Entrez ici les mentions légales du site.<br />\n                    <a href=\"https://www.francenum.gouv.fr/guides-et-conseils/developpement-commercial/site-web/quelles-sont-les-mentions-legales-pour-un-site\">\n                    Que doivent-elles obligatoirement contenir ?</a>", "heading_tag": "h2"}}, {"id": "b7618963-44be-40fa-9e25-93b178573ac0", "type": "paragraph", "value": "\n                <p>D’après la <a href=\"https://www.systeme-de-design.gouv.fr/elements-d-interface/composants/pied-de-page\">documentation du système de design</a>,\n                le pied de page doit contenir a minima les quatre liens suivants :</p>\n                <ul>\n                    <li>Accessibilité : non/partiellement/totalement conforme</li>\n                    <li>Mentions légales</li>\n                    <li>Données personnelles</li>\n                    <li>Gestion des cookies</li>\n                </ul>\n                <p>Ces deux derniers peuvent pointer vers des pages à part entière ou des sections de cette page.</p>\n                "}]', false, NULL, false, false, NULL, NULL, NULL, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (5, '[{"id": "7dd555f8-58fb-405a-9b7e-ce1f26e8bddd", "type": "alert", "value": {"level": "info", "title": "Déclaration d’accessibilité", "description": "Entrez ici la déclaration d’accessibilité.<br />\n                    <a href=\"https://betagouv.github.io/a11y-generateur-declaration/#create\">\n                    Générateur de déclaration d’accessibilité</a>", "heading_tag": "h2"}}]', false, NULL, false, false, NULL, NULL, NULL, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (6, '[{"id": "50557e6a-6c6b-4dd1-ae4a-b5a5103757e6", "type": "paragraph", "value": "<p data-block-key=\"wps84\">This is the Aardvark Page</p>"}]', false, NULL, false, false, NULL, NULL, NULL, NULL);
