INSERT INTO public.wagtailcore_revision VALUES (19, '2024-05-05 16:33:24.278372+00', '{"pk": 7, "body": "[{\"type\": \"imageandtext\", \"value\": {\"image\": 10, \"image_side\": \"right\", \"image_ratio\": \"3\", \"text\": \"<p data-block-key=\\\"8vslz\\\"><i>Changeons le monde ensembles.</i></p><p data-block-key=\\\"cbep\\\">Nous proposons notre 21e Conseil des Voisins le 28 avril \\u00e0 10h dans le 14e arrondissement \\u00e0 Paris et en ligne sur <a href=\\\"https://meet.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">meet.lesgrandsvoisins.com</a></p><p data-block-key=\\\"7c42l\\\">Chez Papa Montparnasse Vavin (RER B Port Royal ou Metro 4 Vavin)<br/>138 Boulevard du Montparnasse, 75014 Paris</p><p data-block-key=\\\"3h4r2\\\">(<a href=\\\"https://maps.app.goo.gl/9F1w2Fam8goZ3ASx9?ref=blog.lesgrandsvoisins.com\\\">lien Maps</a>)</p><p data-block-key=\\\"aqqh1\\\">Ordre du jour :</p><ol><li data-block-key=\\\"d29qa\\\">Comptes annuels (faibles)</li><li data-block-key=\\\"9oc8p\\\">Activit\\u00e9s : arts plastiques, num\\u00e9rique, civisme</li><li data-block-key=\\\"4fef0\\\">Communication</li></ol><p data-block-key=\\\"6p2dh\\\">Nous parlerions des sujets pr\\u00e9cis en lien avec la m\\u00e9moire de Valentin FRANCY, le num\\u00e9rique cr\\u00e9atif des Grands Voisins (Budget Participatif de Paris), efforts pour rendre des h\\u00f4pitaux hybrides aussi en galeries d''art.</p><p data-block-key=\\\"ahns\\\"><a href=\\\"https://www.instagram.com/les_grands_voisins/?utm_source=ig_embed&amp;ig_rid=f6153464-ce6a-41dd-b392-5fc008623cfa\\\">https://www.instagram.com/les_grands_voisins/?utm_source=ig_embed&amp;ig_rid=f6153464-ce6a-41dd-b392-5fc008623cfa</a></p><p data-block-key=\\\"265ce\\\"></p>\", \"link\": {\"page\": null, \"external_url\": \"\", \"text\": \"\"}, \"link_label\": \"\", \"page\": null, \"link_url\": \"\"}, \"id\": \"66ff0747-1ccf-4761-8adc-4a805dd4e883\"}]", "date": "2024-04-19T08:00:00Z", "live": true, "path": "0001000200030001", "slug": "21e-conseil-des-grands-voisins", "depth": 4, "owner": 1, "title": "21e Conseil des Grands Voisins", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/21e-conseil-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "21e Conseil des Grands Voisins", "entry_tags": [{"pk": null, "tag": 9, "content_object": 7}, {"pk": null, "tag": 10, "content_object": 7}], "go_live_at": null, "draft_title": "21e Conseil des Grands Voisins", "content_type": 71, "header_image": 9, "header_large": true, "header_darken": true, "live_revision": 18, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": 18, "translation_key": "bafd634b-2f39-48a2-95a9-d781aa3eb7a1", "entry_categories": [{"pk": 1, "page": 7, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T16:29:53.465Z", "first_published_at": "2024-05-05T16:26:44.202Z", "header_color_class": "blue-france", "search_description": "Ordre du jour :\r\n\r\nComptes annuels (faibles)\r\nActivités : arts plastiques, numérique, civisme\r\nCommunication\r\nNous parlerions des sujets précis en lien avec la mémoire de Valentin FRANCY, le numérique créatif des Grands Voisins (Budget Participatif de Paris), efforts pour rendre des hôpitaux hybrides aussi en galeries d''art.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T16:29:53.438Z"}', NULL, '7', 1, 71, 1, '21e Conseil des Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (22, '2024-05-05 16:53:03.91299+00', '{"pk": 8, "body": "[{\"type\": \"callout\", \"value\": {\"title\": \"\", \"text\": \"Proposition de communiqu\\u00e9 pour la D\\u00e9l\\u00e9gation permanente de la R\\u00e9publique Islamique de Mauritanie \\u00e0 l\\u2019UNESCO \\u00e0 l\\u2019occaison du retour des Etats Unis par l\\u2019artiste Mohamed Ma\\u00ebl A\\u00efnine Sidi Ethmane.\", \"heading_tag\": \"h3\"}, \"id\": \"2bb8edae-aac8-4208-bb68-677a9e5e0b00\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"vq1lp\\\">Paris, le 14 juitllet 2023</p><p data-block-key=\\\"bc251\\\">Ma\\u00ebl A.N.C.<br/>22 Rue Duranton,<br/>75015 Paris,</p><p data-block-key=\\\"eq7hr\\\">Proposition de communiqu\\u00e9 pour la \\u00a0D\\u00e9l\\u00e9gation permanente de la R\\u00e9publique Islamique de Mauritanie \\u00e0 l\\u2019UNESCO \\u00e0 l\\u2019occaison du retour des Etats Unis par l\\u2019artiste Mohamed Ma\\u00ebl A\\u00efnine Sidi Ethmane.</p>\", \"id\": \"a1d22914-8142-4f56-8195-acceaa062969\"}, {\"type\": \"separator\", \"value\": {\"top_margin\": 3, \"bottom_margin\": 3}, \"id\": \"1ff0b627-f8af-403a-86ee-7c48dceeca4d\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"vq1lp\\\">La D\\u00e9l\\u00e9gation permanente de la R\\u00e9publique Islamique de Mauritanie \\u00e0 l''UNESCO</p><p data-block-key=\\\"1ib9p\\\"></p><embed embedtype=\\\"image\\\" format=\\\"fullwidth\\\" id=\\\"13\\\" alt=\\\"\\\"/><p data-block-key=\\\"ebc01\\\"></p><p data-block-key=\\\"abvac\\\">f\\u00e9licite avec gratitude les \\u00c9tats Unis d\\u2019Am\\u00e9rique</p><p data-block-key=\\\"405oi\\\"></p><embed embedtype=\\\"image\\\" format=\\\"fullwidth\\\" id=\\\"14\\\" alt=\\\"flag-usa.pmg\\\"/><p data-block-key=\\\"6vria\\\">de leur retour historique \\u00e0 L\\u2019UNESCO</p><p data-block-key=\\\"fan0p\\\"></p><embed embedtype=\\\"image\\\" format=\\\"fullwidth\\\" id=\\\"12\\\" alt=\\\"\\\"/><p data-block-key=\\\"8hpaf\\\">Nous sommes fiers et conquis par l\\u2019excellent truchement politique de bon aloi de la Directrice G\\u00e9n\\u00e9rale de l\\u2019UNESCO, Madame Audrey Azouley, qui a dignement conduit la r\\u00e9ussite de cet \\u00e9v\\u00e8nement majeur dans l\\u2019histoire de notre organisation onusienne.</p><p data-block-key=\\\"d2qnj\\\">Nous nous joignons \\u00e0 la solidarit\\u00e9 inconditionnelle de tous les \\u00c9tats membres de L\\u2019UNESCO pour accueillir ce grand retour de nos amis communs des U.S.A.</p><p data-block-key=\\\"55kfk\\\">Merci infiniment, tr\\u00e8s cordialement.</p><p data-block-key=\\\"24unt\\\"></p>\", \"id\": \"a5eb0b1f-d5a6-45ff-ba4a-04b697cfc3ad\"}, {\"type\": \"separator\", \"value\": {\"top_margin\": 3, \"bottom_margin\": 3}, \"id\": \"50667e55-09ac-4afe-925a-4a699179611e\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"vq1lp\\\">Je vous prie de consid\\u00e9rer cette expression avec bienveillance.</p><p data-block-key=\\\"fqr5o\\\">Ma\\u00ebl A.N.C.<br/>mael@maelanc.com \\u2013 <a href=\\\"http://www.maelanc.com/?ref=blog.lesgrandsvoisins.com\\\">www.maelanc.com</a></p>\", \"id\": \"3d6b83ce-62cb-4b15-b0ab-13ab6c9a4a5f\"}]", "date": "2023-07-17T13:00:00Z", "live": true, "path": "0001000200030002", "slug": "communique-mauritanie-unesco", "depth": 4, "owner": 1, "title": "Proposition de communiqué pour la Mauritanie à l’UNESCO", "locale": 1, "locked": false, "authors": [2], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/communique-mauritanie-unesco/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Proposition de communiqué pour la Délégation permanente de la République Islamique de Mauritanie à l’UNESCO", "entry_tags": [], "go_live_at": null, "draft_title": "Proposition de communiqué pour la Mauritanie à l’UNESCO", "content_type": 71, "header_image": 12, "header_large": true, "header_darken": true, "live_revision": 21, "show_in_menus": true, "blog_categories": [1, 3], "header_cta_link": null, "header_cta_text": null, "latest_revision": 21, "translation_key": "02059326-e36f-4bd5-b4ae-a29a560e42f4", "entry_categories": [{"pk": 2, "page": 8, "category": 1}, {"pk": 3, "page": 8, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T16:47:37.580Z", "first_published_at": "2024-05-05T16:47:37.580Z", "header_color_class": "red-marianne", "search_description": "Proposition de communiqué pour la Délégation permanente de la République Islamique de Mauritanie à l’UNESCO à l’occaison du retour des Etats Unis par l’artiste Mohamed Maël Aïnine Sidi Ethmane.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T16:47:37.545Z"}', NULL, '8', 1, 71, 1, 'Proposition de communiqué pour la Mauritanie à l’UNESCO');
INSERT INTO public.wagtailcore_revision VALUES (25, '2024-05-05 17:06:04.649637+00', '{"pk": 11, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"ryl3c\\\">Nous allons tenir notre 20e Conseil des voisins <b>le 18 d\\u00e9cembre 2023 de 19h \\u00e0 21h</b>.</p><p data-block-key=\\\"2bddt\\\">Acc\\u00e8s en personne :</p><p data-block-key=\\\"d2eht\\\">Centre commercial \\u00ab Le Square \\u00bb, au 93 ave du G\\u00e9n. Leclerc 75014 Paris (Metro Al\\u00e9sia) 7e boutique sur la gauche.</p><p data-block-key=\\\"dcqhg\\\">Acc\\u00e8s en visio :</p><p data-block-key=\\\"4ln8q\\\"><a href=\\\"https://meet.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">https://meet.lesgrandsvoisins.com</a> (merci de lire les indications \\u00e0 l''avance)</p><p data-block-key=\\\"8td3s\\\">Un sujet principal de d\\u00e9bat sera notre participation au m\\u00e9morial de Valentin FRANCY le 2 mars 2024, potentiellement conjointement avec Le Souvenir Fran\\u00e7ais. M. FRANCY, un \\u00e9tudiant de 21 ans a \\u00e9t\\u00e9 sauvagement assassin\\u00e9 le 2 mars 2023 par une personne qui \\u00e9tait pr\\u00e9sent \\u00e0 notre 17e Conseil des voisins, puis pour la moiti\\u00e9 de notre 18e Conseil des voisins. M. FRANCY et son assassin avaient pour habitude de s''impliquer dans plusieurs associations en tant que b\\u00e9n\\u00e9voles. A la derni\\u00e8re nouvelle, son assassin confess\\u00e9 est toujours en h\\u00f4pital psychiatrique. Il convient de prendre la juste mesure de cette r\\u00e9alit\\u00e9 violente dans ce monde violent.</p><p data-block-key=\\\"3sldd\\\">D''autres sujets peuvent concerner le rejet surpris par la Mairie de Paris d''honorer le budget participatif Le num\\u00e9rique cr\\u00e9atif des Grands Voisins apr\\u00e8s deux confusions ; notre budget et rapports pour l''ann\\u00e9e qui vient ; un nouveau projet pour les Arts voisins par la transformation d''une salle de r\\u00e9union des familles dans un h\\u00f4pital ; une nouvelle plateforme de fund-raising ; des mani\\u00e8res de pouvoir financer un travail pour le dirigeant ; etc.</p>\", \"id\": \"572bbfce-6e7d-4eae-9496-0d1407896b4e\"}]", "date": "2023-12-03T12:00:00Z", "live": true, "path": "0001000200030005", "slug": "20e-conseil-des-voisins", "depth": 4, "owner": 1, "title": "20e Conseil des Voisins lun. 18 déc. Paris 14e et Visio", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/20e-conseil-des-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "20e Conseil des Voisins lun. 18 déc. Paris 14e et Visio", "entry_tags": [{"pk": 11, "tag": 10, "content_object": 11}, {"pk": 12, "tag": 15, "content_object": 11}], "go_live_at": null, "draft_title": "20e Conseil des Voisins lun. 18 déc. Paris 14e et Visio", "content_type": 71, "header_image": 17, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "c607b565-1afb-4811-95b3-977f70662e1b", "entry_categories": [{"pk": 6, "page": 11, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "blue-ecume", "search_description": "Nous avons tenu notre 20e Conseil des voisins le 18 décembre 2023 de 19h à 21h.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '11', 1, 71, 1, '20e Conseil des Voisins lun. 18 déc. Paris 14e et Visio');
INSERT INTO public.wagtailcore_revision VALUES (28, '2024-05-05 17:16:00.096786+00', '{"pk": 13, "body": "[{\"type\": \"video\", \"value\": {\"title\": \"Entretien Association HFSDI et Les Grands Voisins\", \"caption\": \"Entretien Association HFSDI et Les Grands Voisins\", \"url\": \"https://www.youtube.com/embed/vRKpUpNo1m8?si=b7BWvcVMqWorMWEb\"}, \"id\": \"7280a5a3-0f73-4ed5-9951-32195ec06b14\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"27608\\\">Dans ce vid\\u00e9o, c''est pour tester le principe de l''entretien, Abaker, Pr\\u00e9sident de l''Association Handicap France-Soudan Darfour International, et Chris Mann, Pr\\u00e9sident de l''association Les Grands Voisins, se pr\\u00e9sentent. Puis Chris pose des questions \\u00e0 Abaker et nous apprenons sur l''Association HFSDI</p>\", \"id\": \"d6556e84-1051-4405-b030-44ae3664c8c7\"}]", "date": "2023-10-02T13:00:00Z", "live": true, "path": "0001000200030007", "slug": "essai-video-entretien-handicap-france-soudan-darfour-international", "depth": 4, "owner": 1, "title": "Essai Vidéo Entretien Handicap France Soudan Darfour International", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/essai-video-entretien-handicap-france-soudan-darfour-international/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Essai Vidéo Entretien Handicap France Soudan Darfour International", "entry_tags": [{"pk": 13, "tag": 20, "content_object": 13}, {"pk": 14, "tag": 15, "content_object": 13}], "go_live_at": null, "draft_title": "Essai Vidéo Entretien Handicap France Soudan Darfour International", "content_type": 71, "header_image": 20, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3, 2], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "a88d49ba-54d3-4f12-96f2-3824fd53ff80", "entry_categories": [{"pk": 7, "page": 13, "category": 2}, {"pk": 8, "page": 13, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "green-emeraude", "search_description": "Lors des essais pour le numérique créatif des Grands Voisins, Abaker et Chris se sont posés dans la Maison des réfugiés pour parler de leurs associations respectives Handicap France Soudan Darfour International et Les Grands Voisins.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '13', 1, 71, 1, 'Essai Vidéo Entretien Handicap France Soudan Darfour International');
INSERT INTO public.wagtailcore_revision VALUES (67, '2024-05-06 03:22:56.047073+00', '{"pk": 30, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"7uuv2\\\">Ce manifeste, imparfait, refl\\u00e8te les esp\\u00e9rances, les souffrances et les incompr\\u00e9hensions de ceux qui l''ont insuffl\\u00e9. Les Grands Voisins sont n\\u00e9s \\u00e0 l''h\\u00f4pital Saint-Vincent-de-Paul dans le 14e arrondissement \\u00e0 Paris, entre Port royal et les Catacombes. Nous sommes issus des failles dans le b\\u00e9ton et dans les politiques publiques. Nous sommes au service de l''accueil de tous et croyons aux ponts entre utopie et r\\u00e9alit\\u00e9.</p><p data-block-key=\\\"esgvl\\\">Comme toutes bonnes voisines et tous bons voisins, nous ne sommes pas toujours d''accord entre nous, mais nous d\\u00e9fendons :</p><ul><li data-block-key=\\\"2i4sq\\\">la parit\\u00e9 dans la gouvernance</li><li data-block-key=\\\"d2f7m\\\">des solutions collectives en bonne intelligence</li><li data-block-key=\\\"d9u2e\\\">l''expression libre</li><li data-block-key=\\\"ard5v\\\">l''exercice artistique</li><li data-block-key=\\\"1sv90\\\">l''apprentissage tout au long de la vie</li><li data-block-key=\\\"64jcl\\\">la valorisation de toutes formes de travail.</li></ul><p data-block-key=\\\"6q0us\\\">Aux Grands Voisins, nous vivons, travaillons, dormons, mangeons, nous soignons, achetons, pleurons, rions, et tout cela dans toutes les langues. L''inclusion et l''exclusion, toutes les deux, peuvent nous \\u00e9loigner de l''Autre qui est une chance.</p><p data-block-key=\\\"3khnu\\\">Nous r\\u00e9clamons des lieux de mixit\\u00e9 sociale qui permettent la valorisation et le partage des savoirs et des savoir-faire de toutes et de tous. Nous r\\u00e9clamons des lieux de mixit\\u00e9 sociale qui favorisent l''\\u00e9coute et les possibilit\\u00e9s de rencontre, conditions visant \\u00e0 l''abolition de la peur de l''autre. Nous r\\u00e9clamons des lieux de mixit\\u00e9 sociale qui luttent contre \\u00ab l''entre-soi \\u00bb, l''isolement et la r\\u00e9duction des individus \\u00e0 des cases.</p><p data-block-key=\\\"47p68\\\">Nous avons besoin de lieux et de temps pour faire pr\\u00e9c\\u00e9dent, pouvoir inventer d''autres formes de soci\\u00e9t\\u00e9 en harmonie avec les \\u00e9cosyst\\u00e8mes. Nous avons toutes et tous \\u00e0 donner et nous pouvons toutes et tous recevoir.</p>\", \"id\": \"a7fb2ebf-8e0d-401c-b11e-1929d826d8fa\"}]", "live": true, "path": "000100020004", "slug": "mission", "depth": 3, "owner": 1, "title": "Mission des Grands Voisin", "locale": 1, "locked": false, "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/mission-des-grands-voisin/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Mission des Grands Voisins", "go_live_at": null, "draft_title": "Mission des Grands Voisin", "content_type": 25, "header_image": 48, "header_large": true, "header_darken": true, "live_revision": 65, "show_in_menus": true, "header_cta_link": null, "header_cta_text": null, "latest_revision": 65, "translation_key": "64b544ea-4a64-4a1c-8221-d1c40491f993", "contentpage_tags": [], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-06T03:21:08.643Z", "first_published_at": "2024-05-06T03:21:08.643Z", "header_color_class": "blue-ecume", "search_description": "Ce manifeste, imparfait, reflète les espérances, les souffrances et les incompréhensions de ceux qui l''ont insufflé. Les Grands Voisins sont nés à l''hôpital Saint-Vincent-de-Paul dans le 14e arrondissement à Paris, entre Port royal et les Catacombes. Nous sommes issus des failles dans le béton et dans les politiques publiques. Nous sommes au service de l''accueil de tous et croyons aux ponts entre utopie et réalité.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-06T03:21:08.623Z"}', NULL, '30', 1, 25, 1, 'Mission des Grands Voisin');
INSERT INTO public.wagtailcore_revision VALUES (40, '2024-05-05 17:59:33.844295+00', '{"pk": 19, "body": "[{\"type\": \"quote\", \"value\": {\"image\": 29, \"quote\": \"Comment \\u00eatre d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral, vraiment ? Peu apr\\u00e8s notre d\\u00e9claration, nous faillons peut-\\u00eatre d\\u00e9j\\u00e0. C''est un cadeau pour nous apprendre !\", \"author_name\": \"Madame la Ministre avec Ma\\u00ebl, Vice-Pr\\u00e9sident des Grands Voisins, \\u00e0 Inclusiv''Day 2023\", \"author_title\": \"\", \"color\": \"brown-opera\"}, \"id\": \"48c5d31b-88c6-442f-8046-f7c5bb180cb7\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"o42mj\\\">Madame la Maire du 14e arrondissement conseille de laisser tomber l''argumentaire dit \\u00ab de responsabilit\\u00e9 de la marque \\u00bb Les Grands Voisins. J''avais utilis\\u00e9 l''argument le 25 mars 2023 sur l''ancien site Saint Vincent de Paul lorsque un cadre sup\\u00e9rieur de l''association Aurore m''a publiquement snob\\u00e9. Je n''\\u00e9tais pas en forme et l''article de blog \\u00e0 ce sujet \\u00e9tait aussi \\u00e9crit d''une posture d\\u00e9fensive.</p><p data-block-key=\\\"bjvv\\\">Je retiens de la suggestion une posture qui se base plus sur le pr\\u00e9sent et sur l''avenir !</p><p data-block-key=\\\"5gcf3\\\">Nous avons vot\\u00e9 une prise de position forte au 17e Conseil des Voisins pour nous d\\u00e9clarer organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. La d\\u00e9claration d''organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral requiert trois conditions:</p><ol><li data-block-key=\\\"ffudt\\\">une gestion d\\u00e9sint\\u00e9ress\\u00e9e,</li><li data-block-key=\\\"b8cbc\\\">pas d''activit\\u00e9 lucrative,</li><li data-block-key=\\\"eif2t\\\">pas de fonctionnement au profit d''un cercle restreint.</li></ol><p data-block-key=\\\"focnb\\\">Pas plus tard que le 10 mai, d\\u00e9j\\u00e0 nous sommes test\\u00e9s sur les points 1 et 3 de par la visite de Madame la Ministre d\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq \\u00e0 Inclusiv''Day.</p>\", \"id\": \"68fe7661-6097-45c9-b54c-7c670b184e0f\"}, {\"type\": \"image\", \"value\": {\"title\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"heading_tag\": \"h3\", \"image\": 29, \"alt\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"caption\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"url\": \"\"}, \"id\": \"6c1bd7dc-4478-4ecf-8536-e4348a009777\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"503h4\\\">Ma\\u00ebl, Vice Pr\\u00e9sident des Grands Voisins, et \\u00e0 Inclusiv''Day pour Les Grands Voisins a envoy\\u00e9 une lettre de sollicitation pour le compte de son association La Caravane CECAI un soutien, un financement, et un encouragement de financement de tiers.</p><p data-block-key=\\\"5o9uj\\\">Cette action m''incite \\u00e0 proposer urgemment un organe de contr\\u00f4le de notre adh\\u00e9rence \\u00e0 nos d\\u00e9clarations (Contrat pour le Web, Coop, Int\\u00e9r\\u00eat G\\u00e9n\\u00e9ral). Ma\\u00ebl pourrait \\u00eatre alors en erreur au regard de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral.</p><p data-block-key=\\\"blc0r\\\">La discussion \\u00e0 avoir pour Les Grands Voisins avec Mme. la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap tourne autour du <a href=\\\"https://blog.lesgrandsvoisins.com/manifeste\\\">Manifeste des Grands Voisins</a>. C''est pour cela que nous aurions besoin d''un comit\\u00e9 de contr\\u00f4le de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Pour comprendre l''erreur, imaginez que vous faite partie d''un organisme qui pr\\u00e9tend \\u00eatre d\\u00e9sint\\u00e9ress\\u00e9, puis vous apprenez que les dirigeants obtiennent des avantages pour des organismes tiers dans leur gestion de l''organisme.</p><p data-block-key=\\\"e1kvg\\\">Comme toute Grande Voisine et tout Grand Voisin, Ma\\u00ebl A.N.C. et la Caravane CECAI ont des projets louables, mais Les Grands Voisins ne peuvent pas soutenir directement ou indirectement aucune des initiatives. Soutenir une initiative porte automatiquement pr\\u00e9judice, \\u00e0 mon sens, \\u00e0 toutes les initiatives que nous ne soutenons pas. Malheureusement, cette tendance a une historique dans les Grands Voisins.</p>\", \"id\": \"ecf3d596-bcf9-4d59-b8dd-ccd1f6171916\"}, {\"type\": \"image\", \"value\": {\"title\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"heading_tag\": \"h3\", \"image\": 30, \"alt\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"caption\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"url\": \"\"}, \"id\": \"e77fdcd9-a5bb-4b89-8c1d-36f064548769\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"u2nv8\\\">En avril 2019, certains acteurs ont propos\\u00e9 un \\u00ab SitesFaciles des Grands Voisins \\u00bb dont on comptait princiaplement Les Petits D\\u00e9brouillard et Fr\\u00e9d\\u00e9ric G.. Ce projet avait, \\u00e0 mon sens, probablement d\\u00e9j\\u00e0 cette notion de profiter \\u00e0 un cercle restreint de personnes. Il avait pr\\u00e9vu explicitement un budget de 20.000 euros pour un financement de salaire \\u00e0 mi-temps pour Ma\\u00ebl ANC. Les montants de cette initiative l\\u00e0 \\u00e9taient petits.</p><p data-block-key=\\\"c65u5\\\">Notre d\\u00e9fi est grand. L''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral aurait besoin d''un comit\\u00e9 de contr\\u00f4le. Pour cela, la robustesse de la part de la contribution de Ma\\u00ebl ANC a apport\\u00e9 ses fruits.</p><p data-block-key=\\\"2mg4p\\\">Mon autre vice-pr\\u00e9sident me pose une question par rapport \\u00e0 un projet accessoire que nous avions avec Ma\\u00ebl \\u2013 le <a href=\\\"https://web.resdigita.com/?ref=blog.lesgrandsvoisins.com\\\">web fait main fait maison</a> \\u2013 qui \\u00e9tait con\\u00e7u pour financer notre pr\\u00e9sence \\u00e0 Inclusiv''Day. (A savoir que l''avance nous permettant \\u00e0 aller \\u00e0 Inclusv''Day venait de Ma\\u00ebl.) L''observation de Caroline \\u00e9tait que ce projet aurait pu enfreindre la condition 2 d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Chapeau Caroline !</p>\", \"id\": \"4246292d-7239-4a7f-9644-08e26324869c\"}, {\"type\": \"image\", \"value\": {\"title\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"heading_tag\": \"h3\", \"image\": 31, \"alt\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"caption\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"url\": \"\"}, \"id\": \"8f1f3001-6ec5-4ee3-87a4-c1541d48cea1\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"amgnp\\\">L''id\\u00e9e que j''avais pour le web fait main \\u00e9tait plus \\u00e0 l''image de kermesse d''\\u00e9cole. C''\\u00e9tait une activit\\u00e9 plus symbolique. Pourtant, la question de Caroline ouvre la question exactement o\\u00f9 elle doit aller ! (Et, effectivement, l''activit\\u00e9 de b\\u00e9n\\u00e9volat aurait besoin d''\\u00eatre scrut\\u00e9e.)</p><p data-block-key=\\\"73po8\\\">Il n''y auraient peut-\\u00eatre pas de probl\\u00e8me \\u00e0 mon sens que les acteurs se revendiquent des Grands Voisins dans leurs activit\\u00e9s. C''est un honneur pour Les Grands Voisins. Le probl\\u00e8me vient quand Les Grands Voisins utilise sa position pour promouvoir un acteur. La question est d''actualit\\u00e9 pour le sujet du budget participatif de Paris \\u00ab <a href=\\\"https://blog.lesgrandsvoisins.com/lancement-du-budget-participatif/\\\">le num\\u00e9rique cr\\u00e9atif des Grands Voisins</a> \\u00bb.</p><p data-block-key=\\\"7obtv\\\">Je dis que je suis laur\\u00e9at, car il s''agit d''une activit\\u00e9 de b\\u00e9n\\u00e9volat qui m''est personnelle sur le site Saint Vincent de Paul, \\u00e0 une \\u00e9glise depuis 2016 et au sein d''une resourcerie \\u00e0 Arcueil. C''est une chose que j''aime faire: \\u00e9quiper et animer des salles de sociabilit\\u00e9 num\\u00e9rique. On y aborde d''abord la sociabilit\\u00e9 et apr\\u00e8s le num\\u00e9rique afin de d\\u00e9stigmatiser le num\\u00e9rique. C''est vraiment splendide pour moi et pour beaucoup. Pourtant, effectivement, c''est mon kiffe, mon kiffe perso.</p>\", \"id\": \"f0595ab5-89af-44b0-afc5-552f5af5db1e\"}, {\"type\": \"image\", \"value\": {\"title\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"heading_tag\": \"h3\", \"image\": 32, \"alt\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"caption\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"url\": \"\"}, \"id\": \"98a7f474-4a3d-4001-8607-df497475cf88\"}]", "date": "2024-05-05T17:45:00Z", "live": false, "path": "000100020003000D", "slug": "la-suite-des-grands-voisins", "depth": 4, "owner": 1, "title": "La Suite des Grands Voisins", "locale": 1, "locked": false, "authors": [], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/la-suite-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "La Suite des Grands Voisins", "entry_tags": [], "go_live_at": null, "draft_title": "La Suite des Grands Voisins", "content_type": 71, "header_image": null, "header_large": false, "header_darken": false, "live_revision": null, "show_in_menus": false, "blog_categories": [], "header_cta_link": null, "header_cta_text": null, "latest_revision": 39, "translation_key": "100abfb8-1567-430c-8be0-f29103bb14e5", "entry_categories": [], "header_cta_label": null, "header_with_title": false, "last_published_at": null, "first_published_at": null, "header_color_class": null, "search_description": "Comment être d''intérêt général, vraiment ? Peu après notre déclaration, nous faillons peut-être déjà. C''est un cadeau pour nous apprendre !", "wagtail_admin_comments": [], "has_unpublished_changes": true, "latest_revision_created_at": "2024-05-05T17:58:18.930Z"}', NULL, '19', 1, 71, 1, 'La Suite des Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (44, '2024-05-05 18:03:34.833595+00', '{"pk": 19, "body": "[{\"type\": \"quote\", \"value\": {\"image\": 27, \"quote\": \"Comment \\u00eatre d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral, vraiment ? Peu apr\\u00e8s notre d\\u00e9claration, nous faillons peut-\\u00eatre d\\u00e9j\\u00e0. C''est un cadeau pour nous apprendre !\", \"author_name\": \"Etre voisin ?\", \"author_title\": \"\", \"color\": \"brown-opera\"}, \"id\": \"48c5d31b-88c6-442f-8046-f7c5bb180cb7\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"o42mj\\\">Madame la Maire du 14e arrondissement conseille de laisser tomber l''argumentaire dit \\u00ab de responsabilit\\u00e9 de la marque \\u00bb Les Grands Voisins. J''avais utilis\\u00e9 l''argument le 25 mars 2023 sur l''ancien site Saint Vincent de Paul lorsque un cadre sup\\u00e9rieur de l''association Aurore m''a publiquement snob\\u00e9. Je n''\\u00e9tais pas en forme et l''article de blog \\u00e0 ce sujet \\u00e9tait aussi \\u00e9crit d''une posture d\\u00e9fensive.</p><p data-block-key=\\\"bjvv\\\">Je retiens de la suggestion une posture qui se base plus sur le pr\\u00e9sent et sur l''avenir !</p><p data-block-key=\\\"5gcf3\\\">Nous avons vot\\u00e9 une prise de position forte au 17e Conseil des Voisins pour nous d\\u00e9clarer organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. La d\\u00e9claration d''organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral requiert trois conditions:</p><ol><li data-block-key=\\\"ffudt\\\">une gestion d\\u00e9sint\\u00e9ress\\u00e9e,</li><li data-block-key=\\\"b8cbc\\\">pas d''activit\\u00e9 lucrative,</li><li data-block-key=\\\"eif2t\\\">pas de fonctionnement au profit d''un cercle restreint.</li></ol><p data-block-key=\\\"focnb\\\">Pas plus tard que le 10 mai, d\\u00e9j\\u00e0 nous sommes test\\u00e9s sur les points 1 et 3 de par la visite de Madame la Ministre d\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq \\u00e0 Inclusiv''Day.</p>\", \"id\": \"68fe7661-6097-45c9-b54c-7c670b184e0f\"}, {\"type\": \"image\", \"value\": {\"title\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"heading_tag\": \"h3\", \"image\": 29, \"alt\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"caption\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"url\": \"\"}, \"id\": \"6c1bd7dc-4478-4ecf-8536-e4348a009777\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"503h4\\\">Ma\\u00ebl, Vice Pr\\u00e9sident des Grands Voisins, et \\u00e0 Inclusiv''Day pour Les Grands Voisins a envoy\\u00e9 une lettre de sollicitation pour le compte de son association La Caravane CECAI un soutien, un financement, et un encouragement de financement de tiers.</p><p data-block-key=\\\"5o9uj\\\">Cette action m''incite \\u00e0 proposer urgemment un organe de contr\\u00f4le de notre adh\\u00e9rence \\u00e0 nos d\\u00e9clarations (Contrat pour le Web, Coop, Int\\u00e9r\\u00eat G\\u00e9n\\u00e9ral). Ma\\u00ebl pourrait \\u00eatre alors en erreur au regard de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral.</p><p data-block-key=\\\"blc0r\\\">La discussion \\u00e0 avoir pour Les Grands Voisins avec Mme. la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap tourne autour du <a href=\\\"https://blog.lesgrandsvoisins.com/manifeste\\\">Manifeste des Grands Voisins</a>. C''est pour cela que nous aurions besoin d''un comit\\u00e9 de contr\\u00f4le de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Pour comprendre l''erreur, imaginez que vous faite partie d''un organisme qui pr\\u00e9tend \\u00eatre d\\u00e9sint\\u00e9ress\\u00e9, puis vous apprenez que les dirigeants obtiennent des avantages pour des organismes tiers dans leur gestion de l''organisme.</p><p data-block-key=\\\"e1kvg\\\">Comme toute Grande Voisine et tout Grand Voisin, Ma\\u00ebl A.N.C. et la Caravane CECAI ont des projets louables, mais Les Grands Voisins ne peuvent pas soutenir directement ou indirectement aucune des initiatives. Soutenir une initiative porte automatiquement pr\\u00e9judice, \\u00e0 mon sens, \\u00e0 toutes les initiatives que nous ne soutenons pas. Malheureusement, cette tendance a une historique dans les Grands Voisins.</p>\", \"id\": \"ecf3d596-bcf9-4d59-b8dd-ccd1f6171916\"}, {\"type\": \"image\", \"value\": {\"title\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"heading_tag\": \"h3\", \"image\": 30, \"alt\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"caption\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"url\": \"\"}, \"id\": \"e77fdcd9-a5bb-4b89-8c1d-36f064548769\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"u2nv8\\\">En avril 2019, certains acteurs ont propos\\u00e9 un \\u00ab SitesFaciles des Grands Voisins \\u00bb dont on comptait princiaplement Les Petits D\\u00e9brouillard et Fr\\u00e9d\\u00e9ric G.. Ce projet avait, \\u00e0 mon sens, probablement d\\u00e9j\\u00e0 cette notion de profiter \\u00e0 un cercle restreint de personnes. Il avait pr\\u00e9vu explicitement un budget de 20.000 euros pour un financement de salaire \\u00e0 mi-temps pour Ma\\u00ebl ANC. Les montants de cette initiative l\\u00e0 \\u00e9taient petits.</p><p data-block-key=\\\"c65u5\\\">Notre d\\u00e9fi est grand. L''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral aurait besoin d''un comit\\u00e9 de contr\\u00f4le. Pour cela, la robustesse de la part de la contribution de Ma\\u00ebl ANC a apport\\u00e9 ses fruits.</p><p data-block-key=\\\"2mg4p\\\">Mon autre vice-pr\\u00e9sident me pose une question par rapport \\u00e0 un projet accessoire que nous avions avec Ma\\u00ebl \\u2013 le <a href=\\\"https://web.resdigita.com/?ref=blog.lesgrandsvoisins.com\\\">web fait main fait maison</a> \\u2013 qui \\u00e9tait con\\u00e7u pour financer notre pr\\u00e9sence \\u00e0 Inclusiv''Day. (A savoir que l''avance nous permettant \\u00e0 aller \\u00e0 Inclusv''Day venait de Ma\\u00ebl.) L''observation de Caroline \\u00e9tait que ce projet aurait pu enfreindre la condition 2 d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Chapeau Caroline !</p>\", \"id\": \"4246292d-7239-4a7f-9644-08e26324869c\"}, {\"type\": \"image\", \"value\": {\"title\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"heading_tag\": \"h3\", \"image\": 31, \"alt\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"caption\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"url\": \"\"}, \"id\": \"8f1f3001-6ec5-4ee3-87a4-c1541d48cea1\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"amgnp\\\">L''id\\u00e9e que j''avais pour le web fait main \\u00e9tait plus \\u00e0 l''image de kermesse d''\\u00e9cole. C''\\u00e9tait une activit\\u00e9 plus symbolique. Pourtant, la question de Caroline ouvre la question exactement o\\u00f9 elle doit aller ! (Et, effectivement, l''activit\\u00e9 de b\\u00e9n\\u00e9volat aurait besoin d''\\u00eatre scrut\\u00e9e.)</p><p data-block-key=\\\"73po8\\\">Il n''y auraient peut-\\u00eatre pas de probl\\u00e8me \\u00e0 mon sens que les acteurs se revendiquent des Grands Voisins dans leurs activit\\u00e9s. C''est un honneur pour Les Grands Voisins. Le probl\\u00e8me vient quand Les Grands Voisins utilise sa position pour promouvoir un acteur. La question est d''actualit\\u00e9 pour le sujet du budget participatif de Paris \\u00ab <a href=\\\"https://blog.lesgrandsvoisins.com/lancement-du-budget-participatif/\\\">le num\\u00e9rique cr\\u00e9atif des Grands Voisins</a> \\u00bb.</p><p data-block-key=\\\"7obtv\\\">Je dis que je suis laur\\u00e9at, car il s''agit d''une activit\\u00e9 de b\\u00e9n\\u00e9volat qui m''est personnelle sur le site Saint Vincent de Paul, \\u00e0 une \\u00e9glise depuis 2016 et au sein d''une resourcerie \\u00e0 Arcueil. C''est une chose que j''aime faire: \\u00e9quiper et animer des salles de sociabilit\\u00e9 num\\u00e9rique. On y aborde d''abord la sociabilit\\u00e9 et apr\\u00e8s le num\\u00e9rique afin de d\\u00e9stigmatiser le num\\u00e9rique. C''est vraiment splendide pour moi et pour beaucoup. Pourtant, effectivement, c''est mon kiffe, mon kiffe perso.</p>\", \"id\": \"f0595ab5-89af-44b0-afc5-552f5af5db1e\"}, {\"type\": \"image\", \"value\": {\"title\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"heading_tag\": \"h3\", \"image\": 32, \"alt\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"caption\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"url\": \"\"}, \"id\": \"98a7f474-4a3d-4001-8607-df497475cf88\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"a0a0n\\\">Entre le 13 et le 17 mars 2023 a eu lieu le Conseil de Paris. Un d\\u00e9bat a eu lieu qui a, \\u00e0 mon sens, contrari\\u00e9 les conditions du budget participatif du 14e arrondissement. En parall\\u00e8le depuis des ann\\u00e9es, il y aurait, je pense, une activit\\u00e9 contre moi au sujet de la marque Les Grands Voisins. Le d\\u00e9bat au Conseil de Paris semble avoir plac\\u00e9 la Mairie du 14e et/ou la Mairie de Paris dans un effort probable de certains pour \\u00ab tuer la marque \\u00bb (je site un membre du bureau de l''association France Tiers Lieux qui donne une bonne caract\\u00e9risation de la mouvance contre les Grands Voisins telle qu''ici). La d\\u00e9lib\\u00e9ration sign\\u00e9e par Madame la Maire de Paris (Mme. HILDAGO) avait effectivement redirig\\u00e9 (je pense, d''apr\\u00e8s ce que je vois et les d\\u00e9bats) le financement du num\\u00e9rique cr\\u00e9atif des Grands Voisins \\u00e0 un projet diff\\u00e9rent. (Le projet diff\\u00e9rent aurait \\u00e9t\\u00e9 l''agrandissement de l''atelier de la Resourcerie Cr\\u00e9ative, lorsque le projet \\u00ab <a href=\\\"https://blog.lesgrandsvoisins.com/lancement-du-budget-participatif/\\\">le num\\u00e9rique cr\\u00e9atif des Grands Voisins</a> \\u00bb concernant entre autres <a href=\\\"https://blog.lesgrandsvoisins.com/budget-participatif-avec-la-mairie-du-14e-et-la-ressourcerie-creative/\\\">l''adaptation d''un atelier existant de la Ressourcerie Cr\\u00e9ative dans une approche</a> sp\\u00e9cifique.)</p>\", \"id\": \"ee086cc5-3732-4cdc-bbad-6f856660edcb\"}, {\"type\": \"image\", \"value\": {\"title\": \"Extrait du Conseil de Paris\", \"heading_tag\": \"h3\", \"image\": 33, \"alt\": \"Extrait du Conseil de Paris\", \"caption\": \"Extrait du Conseil de Paris\", \"url\": \"\"}, \"id\": \"96a1b111-502f-42a5-91af-90f551ad7879\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"a0a0n\\\">Quand le budget participatif \\u00ab <a href=\\\"https://blog.lesgrandsvoisins.com/lancement-du-budget-participatif/\\\">le num\\u00e9rique cr\\u00e9atif des Grands Voisins</a> \\u00bb \\u00e9tait propos\\u00e9, nous n''avions pas encore pris la partie d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Il convient aussi de regarder le budget participatif pour lequel plus de 4000 parisiens du 14e ont vot\\u00e9 pour v\\u00e9rifier son int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral.</p><p data-block-key=\\\"s40q\\\">Il y a aussi la question du <a href=\\\"https://blog.lesgrandsvoisins.com/16e-conseil-des-voisins/\\\">16e Conseil des voisins</a> qui avait vot\\u00e9 pour nommer \\u00ab <a href=\\\"https://www.resdigita.com/?ref=blog.lesgrandsvoisins.com\\\">ResDigita</a> \\u00bb les activit\\u00e9s num\\u00e9riques des Grands Voisins. Comme je suis moi m\\u00eame d\\u00e9brouillard informatique, et pourrais (je ne le fais pas maintenant) y gagner ma vie, il convient aussi une vigilance \\u00e0 cela. Je pense qu''il convient de d\\u00e9sinvestir Les Grands Voisins de la notion de ResDigita.</p><p data-block-key=\\\"baou6\\\">A mon sens l''avenir des Grands Voisins, association d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral coop\\u00e9rative, tient en une valeur ajout\\u00e9e: la neutralit\\u00e9 et la transparence avec personnalit\\u00e9, la personnalit\\u00e9 de notre manifeste. Voil\\u00e0 ce que je propose pour le <a href=\\\"https://blog.lesgrandsvoisins.com/18e-conseil/\\\">18e Conseil des Voisins</a> !</p>\", \"id\": \"0b8b04a6-0d57-41f3-a36c-d9890eac9f9e\"}]", "date": "2023-05-23T10:00:00Z", "live": true, "path": "000100020003000D", "slug": "la-suite-des-grands-voisins", "depth": 4, "owner": 1, "title": "La Suite des Grands Voisins", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/la-suite-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "La Suite des Grands Voisins", "entry_tags": [{"pk": null, "tag": 25, "content_object": 19}, {"pk": null, "tag": 15, "content_object": 19}], "go_live_at": null, "draft_title": "La Suite des Grands Voisins", "content_type": 71, "header_image": 29, "header_large": true, "header_darken": true, "live_revision": 43, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": 43, "translation_key": "100abfb8-1567-430c-8be0-f29103bb14e5", "entry_categories": [{"pk": 15, "page": 19, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T18:03:03.760Z", "first_published_at": "2024-05-05T18:01:07.129Z", "header_color_class": "red-marianne", "search_description": "Comment être d''intérêt général, vraiment ? Peu après notre déclaration, nous faillons peut-être déjà. C''est un cadeau pour nous apprendre !", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T18:03:03.727Z"}', NULL, '19', 1, 71, 1, 'La Suite des Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (51, '2024-05-05 18:18:18.919925+00', '{"pk": 22, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"r0ve0\\\">Au sens de l''1.b, et accessoirement aussi de l''1.e et de l''1.f bis, de l''article 200 (dons des particuliers), et \\u00e0 l''1.a de l''article 238 bis (dons des entreprises) du code g\\u00e9n\\u00e9ral des imp\\u00f4ts, et selon la jurisprudence de la Cour de cassation du 9 f\\u00e9vrier 2017 (2e chambre civile 16-12423 et 16-11483), \\u00a0l''association coop\\u00e9rative Les Grands Voisins est un organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral en capacit\\u00e9 de d\\u00e9livrer des certificats Cerfa n. 11580*03 qui permet aux individus de r\\u00e9duire leurs imp\\u00f4ts de 66% du montant de leur don et aux entreprises de r\\u00e9duire de 60% de leur don. Notre qualification est \\u00e9tablie par vote du 17e Conseil des voisins le 2 mai 2023 par la nature de nos activit\\u00e9s et nos modes d''excercice ainsi justifi\\u00e9es ci-apr\\u00e8s.</p><h2 data-block-key=\\\"ae6bj\\\">Notre conformit\\u00e9 aux crit\\u00e8res d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral</h2><p data-block-key=\\\"s5m9\\\">Trois crit\\u00e8res sont requis pour les dons des particuliers (<a href=\\\"https://bofip.impots.gouv.fr/bofip/5825-PGP.html/identifiant=BOI-IR-RICI-250-10-10-20170510?ref=blog.lesgrandsvoisins.com\\\">BOI-IR-RICI-250-10-10-20170510</a> 90 - 100) :</p><p data-block-key=\\\"8jgs1\\\">Un organisme est d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral au sens du b du 1 de l''<a href=\\\"https://www.legifrance.gouv.fr/codes/article_lc/LEGIARTI000034597042/2017-05-05?ref=blog.lesgrandsvoisins.com\\\">article 200 du CGI</a> d\\u00e8s lors qu''il remplit cumulativement les trois conditions suivantes :<br/>- il n''exerce pas d''activit\\u00e9 lucrative au sens du 1 de l''<a href=\\\"https://www.legifrance.gouv.fr/codes/article_lc/LEGIARTI000034597017/2017-05-05?ref=blog.lesgrandsvoisins.com\\\">article 206 du CGI</a> (<a href=\\\"https://bofip.impots.gouv.fr/doctrine/pgp/2343-PGP?ref=blog.lesgrandsvoisins.com\\\">BOI-IS-CHAMP-10-50-10</a>) ;<br/>- il ne fait pas l''objet d''une gestion int\\u00e9ress\\u00e9e ;<br/>- il ne fonctionne pas au profit d''un cercle restreint de personnes.<br/>Si la reconnaissance d''utilit\\u00e9 publique constitue un indice du respect de ces trois conditions, les organismes qui b\\u00e9n\\u00e9ficient de cette reconnaissance doivent n\\u00e9anmoins les satisfaire effectivement.</p><p data-block-key=\\\"2s16t\\\">et par des personnes morales (<a href=\\\"https://bofip.impots.gouv.fr/bofip/10963-PGP.html/identifiant=BOI-BIC-RICI-20-30-10-10-20220608?ref=blog.lesgrandsvoisins.com#OEuvres_ou_organismes_dinte_20\\\">BOI-BIC-RICI-20-30-10-10-20220608</a> 80 - 170)</p><p data-block-key=\\\"4bhpv\\\">L''organisme b\\u00e9n\\u00e9ficiaire des dons et versements doit \\u00eatre d\\u2019int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Cette condition est remplie si l''activit\\u00e9 n''est pas lucrative, si la gestion est d\\u00e9sint\\u00e9ress\\u00e9e et si l\\u2019organisme ne fonctionne pas au profit d\\u2019un cercle restreint de personnes.<br/>[...]</p><p data-block-key=\\\"c28gf\\\">Ce sont respectivement les notes d''application respectivement pour les articles 200 et 238 bis du code g\\u00e9n\\u00e9ral des imp\\u00f4ts. La d\\u00e9finition de l''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral pour les dons des individus est produite en int\\u00e9gralit\\u00e9. La d\\u00e9finition pour les dons des entreprises comporte neuf paragraphes en plus divis\\u00e9s en les trois parties : activit\\u00e9 non-lucrative, gestion d\\u00e9sint\\u00e9ress\\u00e9e et organismes ne fonctionnant pas au profit d''un cercle restraint de personnes.</p><p data-block-key=\\\"4hq7f\\\">Le Conseil des voisins du 2 mai 2023 prend acte de la totalit\\u00e9 des exigences et nous d\\u00e9clare en conformit\\u00e9 avec l''ensemble des exigences. Au moment de l''\\u00e9criture de cet article, le 29 avril 2023, ce vote n''a pas encore eu lieu.</p><p data-block-key=\\\"8frri\\\">Il convient aussi d''\\u00e9claircir comment nous sommes en conformit\\u00e9 \\u00e0 la notion fiscale d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. De toute les fa\\u00e7ons, notre qualification peut \\u00eatre contest\\u00e9 devant un juge, et nous en aurions \\u00e0 nous justifier pour continuer \\u00e0 toujours \\u00e9mettre des re\\u00e7us fiscaux. Quand nous aurions plus de 500.000 euros de dons, nous aurions int\\u00e9r\\u00eat \\u00e0 faire une demande de pr\\u00e9caution aupr\\u00e8s des imp\\u00f4ts.</p><h2 data-block-key=\\\"83iim\\\">Notre conformit\\u00e9 \\u00e0 l''absence d''activit\\u00e9 lucrative</h2><p data-block-key=\\\"9ti3r\\\">Tout d''abord, nous sommes une association fran\\u00e7aise de loi 1901. En cons\\u00e9quence, nous ne distribuons pas de dividendes et nous ne faisons pas de profits.</p><p data-block-key=\\\"ej8kv\\\">Ensuite, notre posture est neutre. Notre force vient de cette neutralit\\u00e9.</p><p data-block-key=\\\"e2jbp\\\">D''autres observations m\\u00e9ritent d''\\u00eatre cit\\u00e9es ici prochainement, mais ce qui pr\\u00e9c\\u00e8de me semble satisfaisant. Dans le cas des dons de personnes morales, il convient de reproduire int\\u00e9gralement l''instruction de l''article 238 bis et adresser aussi nos observations, bien que ce qui pr\\u00e9c\\u00e8de donneraient des \\u00e9l\\u00e9ments suffisants.</p><h2 data-block-key=\\\"c9g7l\\\">Notre conformit\\u00e9 \\u00e0 la gestion d\\u00e9sint\\u00e9ress\\u00e9e</h2><p data-block-key=\\\"c8a73\\\">Encore, notre force est notre neutralit\\u00e9 et nous sommes une association fran\\u00e7aise de loi 1901. La remuneration du pr\\u00e9sident est limit\\u00e9 \\u00e0 un seuil inf\\u00e9rieur au SMIC.</p><p data-block-key=\\\"for0p\\\">La question de d\\u00e9sint\\u00e9ressement est profonde et va au-d\\u00e9l\\u00e0 de la gestion de l''association, mais nous allons traiter ici le mode de gestion.</p><p data-block-key=\\\"4q8ki\\\">Tout d''abord, qui est Grand Voisin qui veut bien l''\\u00eatre. Nous avons trois coll\\u00e8ges avec des poids \\u00e9gaux de vote. Chaque personne \\u00e0 l''int\\u00e9rieur de chaque coll\\u00e8ge \\u00e0 un poids \\u00e9gal. C''est notre mani\\u00e8re de prendre nos d\\u00e9cisions.</p><p data-block-key=\\\"hda7\\\">Tout acte de gestion \\u00e9mane obligatoirement d''un vote dans un Conseil des Voisins. En cons\\u00e9quence, la gestion est d\\u00e9sint\\u00e9ress\\u00e9e.</p><p data-block-key=\\\"fsrjb\\\">Il est \\u00e9galement important de noter que notre fonctionnement est reconnu en conformit\\u00e9 par les f\\u00e9d\\u00e9rations internationaux des Coop\\u00e9rative, chose qui garantie une neutralit\\u00e9 en plus dans notre gestion. Cette neutralit\\u00e9, en combination avec notre ouverture totale (voir ci-dessous) rend la gestion compl\\u00e8tement d\\u00e9sint\\u00e9ress\\u00e9e.</p><h2 data-block-key=\\\"bn47\\\">Le b\\u00e9n\\u00e9fice \\u00e0 un cercle ouvert</h2><p data-block-key=\\\"55gkv\\\">Tout d''abord, la d\\u00e9cision ou non d''\\u00eatre membre des Grands Voisins, et donc de disposer d''une voix et de demander des comptes, d\\u00e9pend de la personne. L''association coop\\u00e9rative n''a rien \\u00e0 dire r\\u00e9lative \\u00e0 la d\\u00e9cision de la personne.</p><p data-block-key=\\\"8hjll\\\">Notre manifeste d\\u00e9clare \\\"L''inclusion et l''exclusion, toutes les deux, peuvent nous \\u00e9loigner de l''autre qui est une chance.\\\" Notre d\\u00e9finition m\\u00eame est la notion de Voisins. Par d\\u00e9finition, on est toutes et tous voisins !</p>\", \"id\": \"cebf7f07-4f2c-4787-abfd-946835a59e76\"}, {\"type\": \"image\", \"value\": {\"title\": \"\", \"heading_tag\": \"h3\", \"image\": 37, \"alt\": \"\", \"caption\": \"\", \"url\": \"\"}, \"id\": \"c5a0580a-9ff9-46dd-9a03-1b3d3dc30de0\"}]", "date": "2023-04-29T10:00:00Z", "live": true, "path": "000100020003000G", "slug": "interet-general", "depth": 4, "owner": 1, "title": "Intérêt général", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/interet-general/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Intérêt général", "entry_tags": [{"pk": null, "tag": 10, "content_object": 22}, {"pk": null, "tag": 15, "content_object": 22}], "go_live_at": null, "draft_title": "Intérêt général", "content_type": 71, "header_image": 37, "header_large": true, "header_darken": true, "live_revision": 50, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": 50, "translation_key": "fa822ad1-a035-4fd8-a0a8-450aa52b72f3", "entry_categories": [{"pk": 18, "page": 22, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T18:18:01.912Z", "first_published_at": "2024-05-05T18:18:01.912Z", "header_color_class": "pink-macaron", "search_description": "Article en cours d''élaboration. Nous sommes un organisme d''intérêt général habilité à délivrer des reçus fiscaux aux donateurs individus (art. 200 CGI) et aux organisations (art. 238 bis) permettant des réductions d''impôt de 66% et 60% respectivement de vos dons.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T18:18:01.865Z"}', NULL, '22', 1, 71, 1, 'Intérêt général');
INSERT INTO public.wagtailcore_revision VALUES (36, '2024-05-05 17:44:25.394253+00', '{"pk": 18, "body": "[{\"type\": \"paragraph\", \"value\": \"<h2 data-block-key=\\\"ya9iw\\\">Votes (en cours)</h2><h3 data-block-key=\\\"4u0s6\\\">Vote 1 : Autorisation d''ouverture des comptes en banque</h3><p data-block-key=\\\"6jqs8\\\">Il est autoris\\u00e9 d''ouvrir un compte en banque \\u00e0 Sogexia et un compte en banque \\u00e0 SumUp.</p><p data-block-key=\\\"70uq4\\\">Coll\\u00e8ge A : 1/1<br/>Coll\\u00e8ge B: 2/2<br/>Coll\\u00e8ge C: 3/3</p><h3 data-block-key=\\\"dtnsn\\\">Vote 2 : Epouser les trois principes d''intr\\u00eat g\\u00e9n\\u00e9ral</h3><p data-block-key=\\\"atrp4\\\">Nous ne contournons aucunement et \\u00e9pousons avant toute autre chose \\u00e0 part le manifeste des Grands Voisins les trois principes d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral pour leurs d\\u00e9finitions propres et notre compr\\u00e9hension que ceci:</p><ol><li data-block-key=\\\"5e7lt\\\">Altruisme complet: nos implications dans les Grands Voisins ne nous apporte aucun avantage mat\\u00e9riel, m\\u00eame indirect.</li><li data-block-key=\\\"4i9qv\\\">Gouvernance autonome: nos int\\u00e9r\\u00eats dans notre gouvernance ne prend en consid\\u00e9ration que l''int\\u00e9r\\u00eat des Grands Voisins et non pas des int\\u00e9r\\u00eats ailleurs que nous pourrions avoir ; et la gouvernance des Grands Voisins est uniquement par nous-m\\u00eames.</li><li data-block-key=\\\"7t03s\\\">Externalit\\u00e9s: nous ne pouvons pas pr\\u00e9dire par avance les beneficiaries de nos actions prinicpales car celles-ci sont pour un b\\u00e9n\\u00e9fice large et motiv\\u00e9es pour l''am\\u00e9lioration g\\u00e9n\\u00e9rale au lieu de l''am\\u00e9lioration sp\\u00e9cifique.</li></ol><p data-block-key=\\\"d3nk7\\\">Coll\\u00e8ge A : 1/1<br/>Coll\\u00e8ge B: 2/2<br/>Coll\\u00e8ge C: 3/3</p><h2 data-block-key=\\\"568jn\\\">Information</h2><h3 data-block-key=\\\"echcc\\\">Langement d''un appel \\u00e0 dons (structure)</h3><p data-block-key=\\\"a30so\\\">Il a \\u00e9t\\u00e9 d\\u00e9cid\\u00e9 de lancer un appel \\u00e0 dons sur la base du manifeste et des projets en gestation et sur l''engagement de neutralit\\u00e9, et sur la notion de portail. Notre offre n''est pas exactement au point encore en tant qu''association recevant les dons et nous pouvons am\\u00e9liorer notre visibilit\\u00e9 de notre m\\u00e9tier et ambitions.</p><h3 data-block-key=\\\"2ko0a\\\">Pr\\u00e9sentation prochaine du projet d''Observatoire des Grands Voisins</h3><p data-block-key=\\\"aq6kg\\\">Nous avons privil\\u00e9gi\\u00e9 le terme \\\"observatoire\\\" pour les deux termes propos\\u00e9s au 16e Conseil des Voisins. Il y a d''ailleurs un Observatoire de la D\\u00e9mocratie Locale du 14e auquel nous nous int\\u00e9ressons.</p><h3 data-block-key=\\\"4mcdh\\\">Possible rapproche contre le Conseil de Paris au sujet du Budget participatif de Paris 2022</h3><p data-block-key=\\\"3h2jv\\\">Le conseil de Paris a redirig\\u00e9 25.000 euros vers un projet d''agrandissement de l''atleier de la Ressourcerie cr\\u00e9ative au lieu du Num\\u00e9rique cr\\u00e9atif des Grands Voisins. Dans les d\\u00e9bats au Conseil de Paris, notre association et Chris Mann ont \\u00e9t\\u00e9 \\u00e9cart\\u00e9s, alors les moteurs du laur\\u00e9at. Un avocat, Ma\\u00eetre LEROY, pourra proposer \\u00e0 Chris MANN une requ\\u00eate en annulation de la d\\u00e9lib\\u00e9ration du Conseil de Paris du 17 mars 2023 rapport\\u00e9 par M. Florentin LETISSIER. Les personnes pr\\u00e9sentes sont (un peu trop) enthousiastes.</p><h2 data-block-key=\\\"4it20\\\">Discussion</h2><h3 data-block-key=\\\"24u0s\\\">Engager des Voisins</h3><h3 data-block-key=\\\"8hk6h\\\">Impact de la d\\u00e9claration g\\u00e9n\\u00e9rale</h3><h3 data-block-key=\\\"eh794\\\">Notion de responsable des actions: Les Grands Voisins ou les Grands Voisins</h3><h3 data-block-key=\\\"fro3u\\\">Bin\\u00f4me Chris et Ma\\u00ebl arr\\u00eat\\u00e9 (impact activit\\u00e9 artistique)</h3><h3 data-block-key=\\\"eo7pa\\\">ResDigita</h3><h3 data-block-key=\\\"a7oqp\\\">Vision</h3><h2 data-block-key=\\\"n086\\\">Lettre \\u00e9ditoriale de rappel</h2><p data-block-key=\\\"7hrgb\\\">Bonjour,</p><p data-block-key=\\\"a7qa7\\\">Le 18e Conseil des voisins aura lieu ce soir de <b>20h \\u00e0 21h15</b> \\u00e0 <a href=\\\"https://meet.lgv.coop/?ref=blog.lesgrandsvoisins.com\\\">meet.lgv.coop</a>, et chez moi \\u00e0 Fontenay-aux-Roses. L''accueil, la s\\u00e9curit\\u00e9 et le confort de cette r\\u00e9union d\\u00e9pend de moi et de vous: il n''y a pas d''enregistrement de l''audio ni du vid\\u00e9o, mais du chat. Le fait de vous nommer, puis d''activer vos cam\\u00e9ras et micros augmentent la notion de s\\u00e9curit\\u00e9 pour les autres. Tout devrait \\u00eatre d\\u00e9crit sur <a href=\\\"https://meet.lgv.coop/?ref=blog.lesgrandsvoisins.com\\\">meet.lgv.coop</a>.</p><p data-block-key=\\\"b9n6g\\\">Lors de cette pr\\u00e9paration pour le 18e conseil, il y a effectivement des nouvelles. Le plus important est la parole de Mme. la Maire du 14e arrondissement, Carine PETIT, qui me sugg\\u00e8re de laisser tomber la notion de responsabilit\\u00e9 de la marque Les Grands Voisins. Dans ce sens que nous ne pouvons pas prendre responsabilit\\u00e9 pour les autres, cela a du sens. Je comprends qu''il convient de se concentrer sur le pr\\u00e9sent et l''avenir.</p><p data-block-key=\\\"7ppjm\\\">Les contraintes d''organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral sont:</p><ol><li data-block-key=\\\"5opol\\\">Aucun avantage mat\\u00e9riel pour les individus ou organisations, m\\u00eame indirect, du fait de notre participation</li><li data-block-key=\\\"2vkm6\\\">Aucune influence externe dans notre gouvernance</li><li data-block-key=\\\"cg1f9\\\">B\\u00e9n\\u00e9fices uniquement par externalit\\u00e9s pour donc des populations non-pas d\\u00e9limit\\u00e9es par avance</li></ol><p data-block-key=\\\"3dbtr\\\">Ces contraintes ne sont pas neutres. Elles sont essentielles \\u00e0 la l\\u00e9gitimit\\u00e9 des Grands Voisins. Je pense qu''elles sont n\\u00e9cessaires et peut-\\u00eatre suffisantes pour notre fonctionnement. Elles mettent en question un de nos engagements en tant que coop\\u00e9ratif: l''engagement \\u00e9conomique des coop\\u00e9rants. Je pr\\u00e9senterai pourquoi ma pens\\u00e9e pr\\u00e9liminaire sur la compatibilit\\u00e9 de ces deux engagements.</p><p data-block-key=\\\"6dv8t\\\">Le budget participatif <b>Le num\\u00e9rique cr\\u00e9atif des Grands Voisins</b> (25.000 euros) aurait \\u00e9t\\u00e9 attribu\\u00e9 en grande partie \\u00e0 un autre budget de 55.000 euros avec un autre objectif: remplacer l''atelier de la Ressourcerie Cr\\u00e9ative de 70m\\u00b2 par un atelier de 110 m2. De surcro\\u00eet, il parait que les d\\u00e9bats au Conseil de Paris aurait fait abstraction de notre association et de la vision du num\\u00e9rique cr\\u00e9atif des Grands Voisins depuis 2016. Il y a une suggestion de consid\\u00e9rer un recours en annulation de d\\u00e9lib\\u00e9ration du Conseil de Paris \\u00e0 consid\\u00e9rer. La cible est la rentr\\u00e9e (mais l''infrastructure r\\u00e9seau pourrait y \\u00eatre insuffisant finalement).</p><p data-block-key=\\\"52s8i\\\">La comp\\u00e9tence principale pour faire marcher le num\\u00e9rique cr\\u00e9atif est la mienne, et c''est une comp\\u00e9tence que je peux utiliser pour gagner ma vie: l''informatique. Je pense qu''il convient peut-\\u00eatre de revoir une d\\u00e9cision du 16e Conseil des voisins d''utiliser le terme resdigita.com pour nos activit\\u00e9s num\\u00e9riques. L''id\\u00e9e est de centrer Les Grands Voisins, association d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9rale coop\\u00e9rative. Pour ma vie professionnelle, je dois personnellement m''assurer d''une s\\u00e9paration nette des activit\\u00e9s.</p><p data-block-key=\\\"7tar2\\\">Ma\\u00ebl est actuellement dans une place personnelle qui m''emp\\u00eache personnellement de m''investir \\u00e0 son c\\u00f4t\\u00e9. Si cela change, ce serait avec plaisir de reprendre le bin\\u00f4me d''avant. Cela ne regarde que moi personnellement et non pas l''association. Ma\\u00ebl s''est entretenu bri\\u00e8vement avec la Ministre d\\u00e9l\\u00e9gu\\u00e9e aux personnes en situation de handicap, expose actuellement \\u00e0 Africa Week \\u00e0 l''UNESCO, a fait un don de \\u00ab\\u202fLa Reine\\u202f\\u00bb \\u00e0 l''UNESCO et entretien un rapport pluriannel avec la Directrice de l''UNESCO.</p><p data-block-key=\\\"8pafp\\\">La Mairie du 14e d\\u00e9veloppe un observatoire de la d\\u00e9mocratie locale. J''en ferai un article de blog prochainement. J''y vois la fibre qui a donn\\u00e9 lieu au site Saint Vincent de Paul !</p><p data-block-key=\\\"eg328\\\">Nous avons particip\\u00e9 au salon Inclusiv''Day o\\u00f9 plusieurs autres acteurs ont choisi de vouloir \\u00eatre dans notre annuaire. C''est un honneur.</p><p data-block-key=\\\"5tano\\\">A 20h pour certaines et certains d''entre vous sur <a href=\\\"https://meet.lgv.coop/?ref=blog.lesgrandsvoisins.com\\\">meet.lgv.coop</a>.</p><p data-block-key=\\\"fsvj4\\\">Chris MANN<br/>chris@lesgrandsvoisins.com<br/>07 81 811 811</p><h2 data-block-key=\\\"cd10o\\\">Invitation</h2><p data-block-key=\\\"7g2ls\\\">Notre <a href=\\\"https://blog.lesgrandsvoisins.com/18e-conseil/\\\">18e Conseil des voisins</a> aura donc lieu <a href=\\\"https://www.lesgrandsvoisins.com/event/18e-conseil-des-voisins-33/register?ref=blog.lesgrandsvoisins.com\\\">mardi le 6 juin de 20h \\u00e0 20h15</a>. Ce conseil suit le m\\u00eame cadre des autres conseils: comfort, accueil, s\\u00e9curit\\u00e9. Nous avons ouvert notre compte en banque et il convient de commencer \\u00e0 concr\\u00e9tiser ce que nous voulons dire par nos engagements en tant qu''association de loi 1901 d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral coop\\u00e9rative multisectorielle (temps de mettre en pratique). Nous aurions aussi \\u00e9t\\u00e9 peut-\\u00eatre snob\\u00e9s au Conseil de Paris. Il semble y avoir une mouvance g\\u00e9n\\u00e9rale pour la privatisation de l''ESS. Nous aimerions profiter d''un local. Nous pourrions concr\\u00e9tiser notre mod\\u00e8le et lancer une id\\u00e9e d''universit\\u00e9 populaire des Grands Voisins.</p><p data-block-key=\\\"q8h1\\\">Le CR du 17e Conseil des Voisins est ici:</p>\", \"id\": \"afe3a0ac-fc2b-4387-8b53-55cd57497b25\"}]", "date": "2023-06-06T10:00:00Z", "live": true, "path": "000100020003000C", "slug": "18e-conseil-des-voisins", "depth": 4, "owner": 1, "title": "18e Conseil des Voisins", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/18e-conseil-des-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "18e Conseil des Voisins", "entry_tags": [{"pk": 27, "tag": 10, "content_object": 18}, {"pk": 28, "tag": 15, "content_object": 18}], "go_live_at": null, "draft_title": "18e Conseil des Voisins", "content_type": 71, "header_image": 27, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "c48bccba-bb4d-45d8-b8c2-d3f6e3f28965", "entry_categories": [{"pk": 14, "page": 18, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "blue-ecume", "search_description": "Votes prises au 18e Conseil des voisins : (1) Nouveaux comptes en banque. (2) Articulation de notre intérêt général. Infos partagées : Appel à dons, Observatoire, Reproche éventuel Conseil de Paris.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '18', 1, 71, 1, '18e Conseil des Voisins');
INSERT INTO public.wagtailcore_revision VALUES (58, '2024-05-05 18:50:29.634383+00', '{"pk": 24, "body": "[{\"type\": \"quote\", \"value\": {\"image\": 40, \"quote\": \"Qui est Grands Voisin qui veut bien l''\\u00eatre. Cette Marque b\\u00e9n\\u00e9ficie d''un double-d\\u00e9po\\u00f4t qui ouvre effectivement sa propri\\u00e9t\\u00e9 \\u00e0 toutes et \\u00e0 tous.\", \"author_name\": \"Les Grands Voisins, une marque pour toutes et pour tous\", \"author_title\": \"\", \"color\": \"\"}, \"id\": \"beef652d-7693-4010-b4c4-c5e1b9119295\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"rpyh7\\\">J''ai \\u00e9t\\u00e9 attaqu\\u00e9 pour plusieurs raisons. Souvent, on m''accuse d''instiguer les agresseuses et les agresseurs. \\u00ab Pour qui me prendrai-je ? \\u00bb l''un peu penser. J''ai pass\\u00e9 un bon moment \\u00e0 r\\u00e9fl\\u00e9chir \\u00e0 cette proposition sous formes diff\\u00e9rentes.</p><p data-block-key=\\\"37q43\\\">Au contraire des \\u00e9tiquettes r\\u00e9currentes que je re\\u00e7ois, peut-\\u00eatre m\\u00eame de mani\\u00e8re coordonn\\u00e9e, augment\\u00e9e et accentu\\u00e9e, je pense que mes actions personnelles sont mieux guid\\u00e9es en me consid\\u00e9rant comme \\u00ab n''importe qui \\u00bb. Par hasard, je me retrouve personnellement avec la responsabilit\\u00e9 de la Marque Les Grands Voisins en m\\u00eame temps qu''un d\\u00e9p\\u00f4t collectif de trois organisations puissantes que je nommerai ci-apr\\u00e8s \\u00ab le Consortium \\u00bb. \\u00a0Ce court article permettrait de poser rapidement des observations partiales face \\u00e0 des accusations tr\\u00e8s moches et destructrices contre moi.</p><h2 data-block-key=\\\"7fpqg\\\">Je suis Grand Voisin</h2><p data-block-key=\\\"1vv8q\\\">N''importe qui peut \\u00eatre Grand Voisin. Je suis n''importe qui. Donc, je peux \\u00eatre Grand Voisin. C''est une propri\\u00e9t\\u00e9 transitive. De m\\u00eame, si vous pouvez admettre aussi v\\u00f4tre c\\u00f4t\\u00e9 \\u00ab n''importe qui \\u00bb, en plus de tout d''\\u00eatre particulier et unique, vous aussi, vous pouvez \\u00eatre Grand Voisin.</p><p data-block-key=\\\"2p8h0\\\">Il se trouve que j''ai \\u00e9dit\\u00e9 une version d''un document \\u00ab Manifeste \\u00bb des Grands Voisins : un document avec des contributions de plus de cent personnes sur deux grands ateliers sur un mois en 2017. Tout de suite \\u00e0 cette \\u00e9laboration du Manifeste, et dans un contexte de mutation de culture de management du lieu Saint-Vincent-de-Paul, j''ai mont\\u00e9 et ai annonc\\u00e9 sur les voies de communication internes un wiki en fran\\u00e7ais, en arabe et en anglais de d\\u00e9bat sur le site web du nom de domaine de mon d\\u00e9p\\u00f4t wiki.lesgrandsvoisins.com. \\u00a0Sous trois mois, le wiki a \\u00e9t\\u00e9 cyber-attaqu\\u00e9.</p><p data-block-key=\\\"dblnm\\\"></p><embed embedtype=\\\"image\\\" format=\\\"fullwidth\\\" id=\\\"41\\\" alt=\\\"notalgie\\\"/><p data-block-key=\\\"dufb9\\\"></p><p data-block-key=\\\"8d3rd\\\">Plus d''une ann\\u00e9e passe. Le 19 f\\u00e9vrier 2019, je me reveille avec une impulsion de d\\u00e9poser la marque \\u00ab Les Grands Voisins \\u00bb. Il n''il y a avait jamais eu de discussion avec qui que ce soit sur le sujet de la marque Les Grands Voisins. Je n''y ai pas pens\\u00e9 la veille, de m\\u00eame le jour apr\\u00e8s avoir d\\u00e9pos\\u00e9 la marque. Trois mois plus tard, je re\\u00e7ois la confirmation de l''INPI et je v\\u00e9rifie si une autre personne ne l''aurait pas d\\u00e9pos\\u00e9e entretemps. Il se trouve que le Consortium a d\\u00e9pos\\u00e9 effectivement la m\\u00eame marque (sauf avec un logo) en collectif le m\\u00eame jour que moi. Un employ\\u00e9 de l''INPI me d\\u00e9clare que dans ces 42 ans \\u00e0 l''INPI, il n''a jamais vu ce cas. Nous avons tous les deux \\u2013 le Consortium et moi-m\\u00eame \\u2013 les m\\u00eames droits dans un r\\u00e9gime de simultan\\u00e9it\\u00e9 de la marque (r\\u00e9gime jusqu''\\u00e0 une r\\u00e9forme de d\\u00e9cembre 2019). Moi et le Consortium sont voisins.</p><p data-block-key=\\\"tlkt\\\"></p><embed embedtype=\\\"image\\\" format=\\\"fullwidth\\\" id=\\\"40\\\" alt=\\\"marque-deposee\\\"/><p data-block-key=\\\"1dtbt\\\"></p><p data-block-key=\\\"6aaqf\\\">C''est une bonne chose que nous soyons deux \\u00e0 d\\u00e9tenir la Marque Les Grands Voisins. D''une part, cela rend la marque potentiellement une propri\\u00e9t\\u00e9 ouverte car si un tiers veut s''en servrir, effectivement moi et le Consortium devons effectivement tous deux nous y opposer. C''est parfaitement \\u00e0 l''image des Grands Vosins. D''autre part, pour moi personnellement, je suis amateur de th\\u00e9ologie (par exemple, j''ai c\\u00e9l\\u00e9br\\u00e9 un mariage religieux sur le site Saint-Vincent-de-Paul). L''histoire de ma journ\\u00e9e du 19 f\\u00e9vrier 2019 est romantique et divine. Cette histoire, pour tout \\u00e9tudiant de th\\u00e9ologie, est une histoire belle \\u00e0 cultiver.</p><p data-block-key=\\\"4o49e\\\">La Marque est si forte. Notre h\\u00e9ritage commun est si important. Le monde est meilleur avec Les Grands Voisins. Nous essayons de voir en quelle capacit\\u00e9 cette marque peut \\u00eatre appliqu\\u00e9 au-d\\u00e9l\\u00e0 du lieu contraint qui \\u00e9tait Saint-Vincent-de-Paul. Les autres ne veulent pas utiliser la marque, mais la Marque a sa place de par sa proposition intrins\\u00e8que.</p><p data-block-key=\\\"2ueh4\\\">Il y a une belle juxtaposition d''id\\u00e9es dans la Marque. Tout d''abord, lorsque l''on utilise \\u00ab les \\u00bb, l''on fait r\\u00e9f\\u00e9rence \\u00e0 une d\\u00e9Marquation. On parle des \\u00e9tranger, des autres, des am\\u00e9ricaines, ..., les Autres. Puis, on dit que les Autres sont \\u00ab Grands \\u00bb. C''est d\\u00e9j\\u00e0 une force. Enfin, on parle des autres, car on parle des \\u00ab Voisins \\u00bb, c''est \\u00e0 dire ceux qui sont en dehors de chez soi. La Marque est ing\\u00e9nie. Son manifeste est bien \\u00e0 son image.</p><p data-block-key=\\\"6o7ea\\\">Pour ma part, je suis l\\u00e9gitime uniquement par ma qualit\\u00e9 de n''importe qui. Je suis l\\u00e9gitime car je ne suis pas ill\\u00e9gitime.</p><p data-block-key=\\\"9ajnp\\\"></p><embed embedtype=\\\"image\\\" format=\\\"fullwidth\\\" id=\\\"42\\\" alt=\\\"aujourdhui-orig-1\\\"/><p data-block-key=\\\"ed30q\\\"></p><p data-block-key=\\\"6n6h8\\\">L''id\\u00e9e de l''association coop\\u00e9rative Les Grands Voisins est d''appliquer le concept sans la contrainte des murs.</p><h2 data-block-key=\\\"80r53\\\">Le Civisme serait contagieux</h2><p data-block-key=\\\"5ahj7\\\">Je suis attaqu\\u00e9. Les attaques ont men\\u00e9 des tiers, au moins trois, mais probablement plus. J''ai eu gain de cause dans un proc\\u00e8s civil en 2021, puis certaines actions sont all\\u00e9s loin. Cela fait toujours mal. Ce sera pour une prochaine fois.</p>\", \"id\": \"16e9ae05-58e3-4a2a-b394-7f46a2ec0280\"}]", "date": "2023-04-28T10:00:00Z", "live": true, "path": "000100020003000I", "slug": "la-marque-les-grands-voisins-est-une-responsabilite-pour-nimporte-qui", "depth": 4, "owner": 1, "title": "La Marque Les Grands Voisins est une responsabilité pour n''importe qui", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/la-marque-les-grands-voisins-est-une-responsabilite-pour-nimporte-qui/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Qui est Grands Voisin qui veut bien l''être. Cette Marque bénéficie d''un double-dépôt qui ouvre effectivement sa propriété à toutes et à tous. [Version 2023-04-29-1]", "entry_tags": [{"pk": null, "tag": 31, "content_object": 24}, {"pk": null, "tag": 30, "content_object": 24}, {"pk": null, "tag": 15, "content_object": 24}], "go_live_at": null, "draft_title": "La Marque Les Grands Voisins est une responsabilité pour n''importe qui", "content_type": 71, "header_image": 40, "header_large": true, "header_darken": true, "live_revision": 56, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": 57, "translation_key": "d061e5f7-c600-48ec-a8e6-5abdb484a1d1", "entry_categories": [], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T18:47:04.853Z", "first_published_at": "2024-05-05T18:45:47.884Z", "header_color_class": "blue-france", "search_description": "Qui est Grands Voisin qui veut bien l''être. Cette Marque bénéficie d''un double-dépôt qui ouvre effectivement sa propriété à toutes et à tous.", "wagtail_admin_comments": [], "has_unpublished_changes": true, "latest_revision_created_at": "2024-05-05T18:47:55.603Z"}', NULL, '24', 1, 71, 1, 'La Marque Les Grands Voisins est une responsabilité pour n''importe qui');
INSERT INTO public.wagtailcore_revision VALUES (176, '2024-05-27 18:06:26.139885+00', '{"pk": 50, "body": "[{\"type\": \"imageandtext\", \"value\": {\"image\": 63, \"image_side\": \"right\", \"image_ratio\": \"3\", \"text\": \"<p data-block-key=\\\"h3np9\\\">Venez assister \\u00e0 notre \\u00e9v\\u00e9nement magique de rencontres, d\\u00e9bats et partages, pour notre 22i\\u00e8me Conseils des Grands Voisins. Nous vous attendons nombreux pour ce moment inoubliable au caf\\u00e9 LPP dans le 14\\u00e8me arrondissements de paris! Gr\\u00e2ce \\u00e0 vous nos projets deviendront r\\u00e9alit\\u00e9!</p><p data-block-key=\\\"3rr48\\\">Nous tiendrions le 22e Conseil des Voisins sur trois rencontres :</p><ul><li data-block-key=\\\"9s2me\\\"><a href=\\\"https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins\\\">Lundi le 10 juin de 19h45 \\u00e0 21h25</a></li><li data-block-key=\\\"dofhv\\\"><a href=\\\"https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins-1\\\">Jeudi le 13 juin de 15h15 \\u00e0 16h45</a></li><li data-block-key=\\\"5dfoa\\\"><a href=\\\"https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins-2\\\">Samedi le le 15 juin de 10h15 \\u00e0 11h45</a></li></ul><h3 data-block-key=\\\"3upfj\\\">Sujets \\u00e0 l''ordre du jour</h3><ol><li data-block-key=\\\"77v0b\\\">Num\\u00e9rique</li><li data-block-key=\\\"lf41\\\">Civisme</li><li data-block-key=\\\"cg5q6\\\">Communication</li></ol>\", \"link\": {\"page\": null, \"external_url\": \"https://www.helloasso.com/associations/les-grands-voisins/#event\", \"text\": \"Je m''inscris pour venir \\u00e0 distance ou en pr\\u00e9sence !\"}, \"link_label\": \"\", \"page\": null, \"link_url\": \"\"}, \"id\": \"42657c06-8cf0-48f4-a0e0-9516b7eefacf\"}, {\"type\": \"multicolumns\", \"value\": {\"bg_image\": 63, \"bg_color_class\": \"green-tilleul-verveine\", \"bg_color\": \"\", \"title\": \"Nous tiendrions le 22e Conseil des Voisins sur