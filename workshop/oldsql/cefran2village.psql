INSERT INTO public.wagtail_localize_string VALUES (61, '9561b9be-d221-5ca7-ae6d-e635ddcb8641', 'Le login et le mot de passe sont indépendants et sécurisés comme nous le pouvons.', 1);
INSERT INTO public.wagtail_localize_string VALUES (62, '77331a70-6161-5467-b57b-a5ee0cab697b', 'Ce dispositif est compatible avec tous <a id="a1">les clients BitWarden</a> (télédéchagement pour ios, Android, Windows, Linux et Apple). Mettez « self-hosted » ou « auto-hébergé » avec l''adresse https://vaultwarden.resdigita.com.', 1);
INSERT INTO public.wagtail_localize_string VALUES (63, 'a14a2564-1340-5a88-a49d-c116e6cfbaf1', '<a id="a1">https://vaultwarden.resdigita.com</a>', 1);
INSERT INTO public.wagtail_localize_string VALUES (64, '0e1e434d-a382-5181-a5f9-118b3dd2aab1', 'Administration', 1);
INSERT INTO public.wagtail_localize_string VALUES (65, '012b36b5-cc49-55f1-b3e9-af2a2c4483ff', '<a id="a1">Wagtail.Resdigita.Com</a> : Editer nos sites webs principaux', 1);
INSERT INTO public.wagtail_localize_string VALUES (66, '9fb58063-58cf-57a6-93d2-b4014c3d8fd9', '<a id="a1">blog.lesgrandsvoisins.com/ghost</a> : Editer notre blog', 1);
INSERT INTO public.wagtail_localize_string VALUES (67, '671a0e4e-320d-53d5-a8a1-13a2e7d76a73', '<a id="a1">List Monk list</a> : Envoyer des newsletters', 1);
INSERT INTO public.wagtail_localize_string VALUES (68, '39f452df-82a8-52e9-94ab-8662f4315ab3', '<a id="a1">Uptime Kuma admin</a> : Adminsitrer le tableau des statuts des services', 1);
INSERT INTO public.wagtail_localize_string VALUES (69, '9f4c2586-c97d-53cd-a63c-2af68c3e3f9d', 'Outils régaliens pour des sites citoyens', 1);
INSERT INTO public.wagtail_localize_string VALUES (70, '09d8e0fc-2c6d-5d07-a612-d6a9b4205658', 'Wagtail-Fastoche pour vos chaînes d''édition numériques', 1);
INSERT INTO public.wagtail_localize_string VALUES (71, '2e822e64-fa81-55a4-810d-bd47b09a5430', 'site-exemple : <a id="a1">wagtail.village.org</a><br/>dépôt source : <a id="a2">github.com/chris2fr/wagtail-village</a><br/>bifurqué depuis <a id="a3">github.com/nurmerique-gouv/sites-facile</a>', 1);
INSERT INTO public.wagtail_localize_string VALUES (72, 'b32b3e89-1cfe-5fa8-b7aa-63fa02c8b7af', 'Django-Fastoche pour vos applications webs', 1);
INSERT INTO public.wagtail_localize_string VALUES (73, '19f4ab92-37cc-5217-8139-70f16ed075e3', 'site-exemple : <a id="a1">django.village.org</a><br/>dépôt source : <a id="a2">github.com/chris2fr/django-village</a><br/>bifurqué depuis <a id="a3">github.com/nurmerique-gouv/django-dsfr</a>', 1);
INSERT INTO public.wagtail_localize_string VALUES (74, '1340facb-fd18-5092-b7bb-8cbb78f1de42', 'Fabrique-Fastoche pour vos systèmes de design numériques', 1);
INSERT INTO public.wagtail_localize_string VALUES (75, '6198ec93-d751-51a1-b5de-6a3ff36b1b1b', 'site-exemple : fabrique.village.org/example/ (prochainement)<br/>dépôt source : <a id="a1">github.com/chris2fr/fabrique-village</a><br/>bifurqué depuis <a id="a2">github.com/GouvernementFR/dsfr</a>', 1);
INSERT INTO public.wagtail_localize_string VALUES (76, 'ccd58a4a-db8e-5c6b-bdb2-4427023e71ed', 'ResDigita est le cellule numérique des Grands Voisins qui peut vous permettre un ensemble d''outils en logiciels libres info-gérés par notre association coopérative (ou par vous-mêmes).', 1);
INSERT INTO public.wagtail_localize_string VALUES (77, '5193aed5-9739-5166-a77f-d952b96aebf5', 'Explorer l''alternative ResDigita !', 1);
INSERT INTO public.wagtail_localize_string VALUES (78, 'fce591d5-4991-56e5-a9ac-fd7b75e458ab', 'https://www.resdigita.com', 1);
INSERT INTO public.wagtail_localize_string VALUES (79, '713a68b3-c8ae-574d-80b7-00525be7e87a', 'Les Grands Voisins est une association dirigée par tout individu qui veut bien pour l''entraide et le partage de biens communs entre artistes, artisans, professionnels, associations, institutions et entreprises. Nous nous concentrons sur les arts plastiques, le numérique et le civisme.', 1);
INSERT INTO public.wagtail_localize_string VALUES (80, '3dd21392-bc37-50fc-946f-746d3f3c8bc0', 'Rencontrer Les Grands Voisins !', 1);
INSERT INTO public.wagtail_localize_string VALUES (81, '9a4f477a-d39e-55be-9377-2138b78cb1ab', 'https://www.lesgrandsvoisins.fr', 1);
INSERT INTO public.wagtail_localize_string VALUES (82, '6710171e-0c20-541d-850e-ae8ea1ad4b04', 'Web-Fastoche est le fait de rendre génériques un ensemble de logiciels libres édités et exploités par l''Etat français pour ses sites .gouv.fr (dont certains outils pour plus de 20.000 noms de domaine).', 1);
INSERT INTO public.wagtail_localize_string VALUES (83, '25efd3bd-03f3-5671-9bc6-d558bfbbe57d', 'Développez votre site Web-Fastoche !', 1);
INSERT INTO public.wagtail_localize_string VALUES (84, 'da2920a3-a74d-5d61-b37f-4dc4577cea7e', 'https://web.village.org', 1);
INSERT INTO public.wagtail_localize_string VALUES (85, '61bdd62f-1e69-54df-851c-e69e9af40635', 'L''autre est une chance.', 1);
INSERT INTO public.wagtail_localize_string VALUES (86, 'ed0cf7ad-7e07-5d85-ada0-c84b198dbcaf', 'Lire la mission des Grands Voisins', 1);


