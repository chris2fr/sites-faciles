INSERT INTO public.wagtailcore_revision VALUES (27, '2024-05-05 17:11:32.649809+00', '{"pk": 12, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"vz7ve\\\">Voici les premiers mots de Thimot\\u00e9 sur un ordinateur</p><p data-block-key=\\\"f50ls\\\">Salut mon ami comment vas-tu ? Si tout va bien, DIEU merci. Et les activit\\u00e9s \\u00e7a marche bien ou pas ? Cela \\u00a0fait un bon moment qu\\u2019on ne se voit pas. J\\u2019esp\\u00e8re qu\\u2019on se reverra tr\\u00e8s prochainement.</p><p data-block-key=\\\"649qv\\\">Et Timoth\\u00e9e revient deux semaines plus tard pour ajouter \\u00e0 l''article.</p><p data-block-key=\\\"3iblt\\\">Bonjour les Grands Voisins,</p><p data-block-key=\\\"5mrg4\\\">Je suis ravi d\\u2019entendre parler de vous. je suis encore l\\u00e0 aujourd\\u2019hui pour me connecter au monde de l\\u2019informatique, du num\\u00e9rique. L\\u2019atelier d\\u2019entraide num\\u00e9rique est vraiment important et la ressourcerie cr\\u00e9ative comme son nom l\\u2019indique est une chose a d\\u00e9couvrir \\u00e9tant donn\\u00e9 que c\\u2019est vraiment un endroit pour se ressourcer et cr\\u00e9er.</p><p data-block-key=\\\"6nhgc\\\">Ne dit-on pas que l\\u2019analphab\\u00e8te du 21 si\\u00e8cle n\\u2019est plus celui qui n\\u2019a jamais \\u00e9t\\u00e9 a l\\u2019\\u00e9cole mais c\\u2019est celui qui n\\u2019a pas de notion en informatique.</p>\", \"id\": \"e1c2a258-883d-4473-b38b-6c715235f8fb\"}, {\"type\": \"image\", \"value\": {\"title\": \"Timoth\\u00e9e revient et tape tout seul\", \"heading_tag\": \"h3\", \"image\": 18, \"alt\": \"Timoth\\u00e9e revient et tape tout seul\", \"caption\": \"\", \"url\": \"\"}, \"id\": \"5fc5066d-aa4f-4535-aca9-56b0b6ab9d2e\"}]", "date": "2024-05-05T17:06:00Z", "live": true, "path": "0001000200030006", "slug": "des-premiers-mots", "depth": 4, "owner": 1, "title": "Des premiers mots", "locale": 1, "locked": false, "authors": [], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/des-premiers-mots/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Des premiers mots", "entry_tags": [], "go_live_at": null, "draft_title": "Des premiers mots", "content_type": 71, "header_image": 19, "header_large": true, "header_darken": true, "live_revision": 26, "show_in_menus": true, "blog_categories": [], "header_cta_link": null, "header_cta_text": null, "latest_revision": 26, "translation_key": "f58ba4f8-4a5b-4aed-a2e5-517d270e4503", "entry_categories": [], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T17:11:01.930Z", "first_published_at": "2024-05-05T17:11:01.930Z", "header_color_class": "blue-cumulus", "search_description": "Voici les premiers mots de Thimoté sur un ordinateur Salut mon ami comment vas-tu ? Si tout va bien, DIEU […]", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T17:11:01.908Z"}', NULL, '12', 1, 71, 1, 'Des premiers mots');
INSERT INTO public.wagtailcore_revision VALUES (35, '2024-05-05 17:39:31.540126+00', '{"pk": 17, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"ytcaw\\\">Participez \\u00e0 ces ateliers conviviaux d\\u2019entraide num\\u00e9rique<br/>pour vous-m\\u00eame ou pour votre organisation. Vous pouvez y avancer vos projets et vos savoir-faire aussi en aidant d\\u2019autres avec les leurs.</p><p data-block-key=\\\"8etl9\\\">Tous niveaux, tous \\u00e2ges, tous besoins sont les bienvenus :<br/>+ des craintifs du num\\u00e9rique aux ing\\u00e9nieurs informaticiens ;<br/>+ des sans ordinateurs fixes aux \\u00e9quip\\u00e9s du nec plus ultra ;<br/>+ des h\\u00e9berg\\u00e9s ou migrants aux propri\\u00e9taires ou entreprises du 14e ;<br/>+ des salari\\u00e9s ou non-salari\\u00e9s aux patrons du 14e ;<br/>+ des \\u00e9tudiants aux retrait\\u00e9s ; etc.</p><p data-block-key=\\\"ahk8k\\\"><b>Le lundi de 16h \\u00e0 19h se tient un atelier d\\u2019entraide num\\u00e9rique</b><br/><b>\\u00e0 l\\u2019Atelier de la Ressourcerie Cr\\u00e9ative</b> au 93 ave. du G\\u00e9n. Leclerc et <b>en ligne \\u00e0</b> <a href=\\\"https://meet.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\"><b>meet.lesgrandsvoisins.com</b></a>.<br/>En plus de vous-m\\u00eames et vos souhaits, vous pouvez amener votre \\u00e9quipement (ordinateur, tablette, smartphone, \\u2026), \\u00e9ventuellement un truc \\u00e0 manger et/ou \\u00e0 boire (sans alcool). Nous aurions des ordinateurs sur place et \\u00e9ventuellement d\\u2019autres choses..</p><p data-block-key=\\\"catpb\\\">Programmation mensuelle \\u00e0 explorer : Fondation Valentin Ha\\u00fcy 3 Rue Jacquier (\\u00e9quipement pour mal-voyants) ; La Table des Mati\\u00e8res 51 Rue de l''Abb\\u00e9 Carton ; peut-\\u00eatre la Maison des r\\u00e9fugi\\u00e9s, 50 Bd Jourdan ; Ailleurs ? (la salle est transportable)</p><p data-block-key=\\\"4m4ae\\\">Lancement b\\u00eata \\u00e0 partir du lundi 7 ao\\u00fbt, d\\u00e9finitif \\u00e0 partir du lundi 4 septembre.<br/>Contact chris@lesgrandsvoisins.com au 07 81 81 18 11 pour toute question.<br/><a href=\\\"https://www.lesgrandsvosins.com/resdigita?ref=blog.lesgrandsvoisins.com\\\">https://www.lesgrandsvosins.com/resdigita</a></p>\", \"id\": \"ecb4d132-119b-4148-8d3c-68dbfd7bd6ee\"}]", "date": "2023-06-30T12:00:00Z", "live": true, "path": "000100020003000B", "slug": "salle-de-sociabilite-numerique", "depth": 4, "owner": 1, "title": "Salle de sociabilité numérique", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/salle-de-sociabilite-numerique/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Salle de sociabilité numérique", "entry_tags": [{"pk": 25, "tag": 24, "content_object": 17}, {"pk": 26, "tag": 15, "content_object": 17}], "go_live_at": null, "draft_title": "Salle de sociabilité numérique", "content_type": 71, "header_image": 26, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [2], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "418f8148-8bb7-4dcf-b70b-9055cdb42150", "entry_categories": [{"pk": 13, "page": 17, "category": 2}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "green-archipel", "search_description": "Participez à ces ateliers conviviaux d’entraide numérique\r\npour vous-même ou pour votre organisation. Vous pouvez y avancer vos projets et vos savoir-faire aussi en aidant d’autres avec les leurs.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '17', 1, 71, 1, 'Salle de sociabilité numérique');
INSERT INTO public.wagtailcore_revision VALUES (125, '2024-05-20 18:01:40.465677+00', '{"pk": 47, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"fggdt\\\"><a href=\\\"https://guichet.resdigita.com/\\\">Connecter \\u00e0 son compte</a> </p><p data-block-key=\\\"8gtcb\\\"></p><p data-block-key=\\\"3s4bk\\\"><a href=\\\"https://guichet.resdigita.com/user/new\\\">Cr\\u00e9er un nouveau compte</a></p>\", \"id\": \"0e24b542-6a04-482d-8a1e-ef93d2ee07a9\"}]", "live": true, "path": "0001000200050003", "slug": "mon-compte", "depth": 4, "owner": 1, "title": "Mon compte", "locale": 1, "locked": false, "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/numerique/mon-compte/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "", "go_live_at": null, "draft_title": "Mon compte", "content_type": 25, "header_image": null, "header_large": false, "header_darken": false, "live_revision": null, "show_in_menus": true, "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "b0c0a91a-a412-4935-9d54-b4c61a377914", "contentpage_tags": [], "header_cta_label": null, "header_with_title": false, "last_published_at": null, "first_published_at": null, "header_color_class": null, "search_description": "Connecter à son compte Créer un nouveau compte […]", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '47', 1, 25, 1, 'Mon compte');
INSERT INTO public.wagtailcore_revision VALUES (39, '2024-05-05 17:58:18.930347+00', '{"pk": 19, "body": "[{\"type\": \"quote\", \"value\": {\"image\": 29, \"quote\": \"Comment \\u00eatre d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral, vraiment ? Peu apr\\u00e8s notre d\\u00e9claration, nous faillons peut-\\u00eatre d\\u00e9j\\u00e0. C''est un cadeau pour nous apprendre !\", \"author_name\": \"Madame la Ministre avec Ma\\u00ebl, Vice-Pr\\u00e9sident des Grands Voisins, \\u00e0 Inclusiv''Day 2023\", \"author_title\": \"\", \"color\": \"brown-opera\"}, \"id\": \"48c5d31b-88c6-442f-8046-f7c5bb180cb7\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"o42mj\\\">Madame la Maire du 14e arrondissement conseille de laisser tomber l''argumentaire dit \\u00ab de responsabilit\\u00e9 de la marque \\u00bb Les Grands Voisins. J''avais utilis\\u00e9 l''argument le 25 mars 2023 sur l''ancien site Saint Vincent de Paul lorsque un cadre sup\\u00e9rieur de l''association Aurore m''a publiquement snob\\u00e9. Je n''\\u00e9tais pas en forme et l''article de blog \\u00e0 ce sujet \\u00e9tait aussi \\u00e9crit d''une posture d\\u00e9fensive.</p><p data-block-key=\\\"bjvv\\\">Je retiens de la suggestion une posture qui se base plus sur le pr\\u00e9sent et sur l''avenir !</p><p data-block-key=\\\"5gcf3\\\">Nous avons vot\\u00e9 une prise de position forte au 17e Conseil des Voisins pour nous d\\u00e9clarer organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. La d\\u00e9claration d''organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral requiert trois conditions:</p><ol><li data-block-key=\\\"ffudt\\\">une gestion d\\u00e9sint\\u00e9ress\\u00e9e,</li><li data-block-key=\\\"b8cbc\\\">pas d''activit\\u00e9 lucrative,</li><li data-block-key=\\\"eif2t\\\">pas de fonctionnement au profit d''un cercle restreint.</li></ol><p data-block-key=\\\"focnb\\\">Pas plus tard que le 10 mai, d\\u00e9j\\u00e0 nous sommes test\\u00e9s sur les points 1 et 3 de par la visite de Madame la Ministre d\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq \\u00e0 Inclusiv''Day.</p>\", \"id\": \"68fe7661-6097-45c9-b54c-7c670b184e0f\"}, {\"type\": \"image\", \"value\": {\"title\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"heading_tag\": \"h3\", \"image\": 29, \"alt\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"caption\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"url\": \"\"}, \"id\": \"6c1bd7dc-4478-4ecf-8536-e4348a009777\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"503h4\\\">Ma\\u00ebl, Vice Pr\\u00e9sident des Grands Voisins, et \\u00e0 Inclusiv''Day pour Les Grands Voisins a envoy\\u00e9 une lettre de sollicitation pour le compte de son association La Caravane CECAI un soutien, un financement, et un encouragement de financement de tiers.</p><p data-block-key=\\\"5o9uj\\\">Cette action m''incite \\u00e0 proposer urgemment un organe de contr\\u00f4le de notre adh\\u00e9rence \\u00e0 nos d\\u00e9clarations (Contrat pour le Web, Coop, Int\\u00e9r\\u00eat G\\u00e9n\\u00e9ral). Ma\\u00ebl pourrait \\u00eatre alors en erreur au regard de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral.</p><p data-block-key=\\\"blc0r\\\">La discussion \\u00e0 avoir pour Les Grands Voisins avec Mme. la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap tourne autour du <a href=\\\"https://blog.lesgrandsvoisins.com/manifeste\\\">Manifeste des Grands Voisins</a>. C''est pour cela que nous aurions besoin d''un comit\\u00e9 de contr\\u00f4le de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Pour comprendre l''erreur, imaginez que vous faite partie d''un organisme qui pr\\u00e9tend \\u00eatre d\\u00e9sint\\u00e9ress\\u00e9, puis vous apprenez que les dirigeants obtiennent des avantages pour des organismes tiers dans leur gestion de l''organisme.</p><p data-block-key=\\\"e1kvg\\\">Comme toute Grande Voisine et tout Grand Voisin, Ma\\u00ebl A.N.C. et la Caravane CECAI ont des projets louables, mais Les Grands Voisins ne peuvent pas soutenir directement ou indirectement aucune des initiatives. Soutenir une initiative porte automatiquement pr\\u00e9judice, \\u00e0 mon sens, \\u00e0 toutes les initiatives que nous ne soutenons pas. Malheureusement, cette tendance a une historique dans les Grands Voisins.</p>\", \"id\": \"ecf3d596-bcf9-4d59-b8dd-ccd1f6171916\"}, {\"type\": \"image\", \"value\": {\"title\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"heading_tag\": \"h3\", \"image\": 30, \"alt\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"caption\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"url\": \"\"}, \"id\": \"e77fdcd9-a5bb-4b89-8c1d-36f064548769\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"u2nv8\\\">En avril 2019, certains acteurs ont propos\\u00e9 un \\u00ab SitesFaciles des Grands Voisins \\u00bb dont on comptait princiaplement Les Petits D\\u00e9brouillard et Fr\\u00e9d\\u00e9ric G.. Ce projet avait, \\u00e0 mon sens, probablement d\\u00e9j\\u00e0 cette notion de profiter \\u00e0 un cercle restreint de personnes. Il avait pr\\u00e9vu explicitement un budget de 20.000 euros pour un financement de salaire \\u00e0 mi-temps pour Ma\\u00ebl ANC. Les montants de cette initiative l\\u00e0 \\u00e9taient petits.</p><p data-block-key=\\\"c65u5\\\">Notre d\\u00e9fi est grand. L''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral aurait besoin d''un comit\\u00e9 de contr\\u00f4le. Pour cela, la robustesse de la part de la contribution de Ma\\u00ebl ANC a apport\\u00e9 ses fruits.</p><p data-block-key=\\\"2mg4p\\\">Mon autre vice-pr\\u00e9sident me pose une question par rapport \\u00e0 un projet accessoire que nous avions avec Ma\\u00ebl \\u2013 le <a href=\\\"https://web.resdigita.com/?ref=blog.lesgrandsvoisins.com\\\">web fait main fait maison</a> \\u2013 qui \\u00e9tait con\\u00e7u pour financer notre pr\\u00e9sence \\u00e0 Inclusiv''Day. (A savoir que l''avance nous permettant \\u00e0 aller \\u00e0 Inclusv''Day venait de Ma\\u00ebl.) L''observation de Caroline \\u00e9tait que ce projet aurait pu enfreindre la condition 2 d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Chapeau Caroline !</p>\", \"id\": \"4246292d-7239-4a7f-9644-08e26324869c\"}, {\"type\": \"image\", \"value\": {\"title\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"heading_tag\": \"h3\", \"image\": 31, \"alt\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"caption\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"url\": \"\"}, \"id\": \"8f1f3001-6ec5-4ee3-87a4-c1541d48cea1\"}]", "date": "2024-05-05T17:45:00Z", "live": false, "path": "000100020003000D", "slug": "la-suite-des-grands-voisins", "depth": 4, "owner": 1, "title": "La Suite des Grands Voisins", "locale": 1, "locked": false, "authors": [], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/la-suite-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "La Suite des Grands Voisins", "entry_tags": [], "go_live_at": null, "draft_title": "La Suite des Grands Voisins", "content_type": 71, "header_image": null, "header_large": false, "header_darken": false, "live_revision": null, "show_in_menus": false, "blog_categories": [], "header_cta_link": null, "header_cta_text": null, "latest_revision": 38, "translation_key": "100abfb8-1567-430c-8be0-f29103bb14e5", "entry_categories": [], "header_cta_label": null, "header_with_title": false, "last_published_at": null, "first_published_at": null, "header_color_class": null, "search_description": "Comment être d''intérêt général, vraiment ? Peu après notre déclaration, nous faillons peut-être déjà. C''est un cadeau pour nous apprendre !", "wagtail_admin_comments": [], "has_unpublished_changes": true, "latest_revision_created_at": "2024-05-05T17:55:28.535Z"}', NULL, '19', 1, 71, 1, 'La Suite des Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (43, '2024-05-05 18:03:03.727079+00', '{"pk": 19, "body": "[{\"type\": \"quote\", \"value\": {\"image\": 27, \"quote\": \"Comment \\u00eatre d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral, vraiment ? Peu apr\\u00e8s notre d\\u00e9claration, nous faillons peut-\\u00eatre d\\u00e9j\\u00e0. C''est un cadeau pour nous apprendre !\", \"author_name\": \"Madame la Ministre avec Ma\\u00ebl, Vice-Pr\\u00e9sident des Grands Voisins, \\u00e0 Inclusiv''Day 2023\", \"author_title\": \"\", \"color\": \"brown-opera\"}, \"id\": \"48c5d31b-88c6-442f-8046-f7c5bb180cb7\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"o42mj\\\">Madame la Maire du 14e arrondissement conseille de laisser tomber l''argumentaire dit \\u00ab de responsabilit\\u00e9 de la marque \\u00bb Les Grands Voisins. J''avais utilis\\u00e9 l''argument le 25 mars 2023 sur l''ancien site Saint Vincent de Paul lorsque un cadre sup\\u00e9rieur de l''association Aurore m''a publiquement snob\\u00e9. Je n''\\u00e9tais pas en forme et l''article de blog \\u00e0 ce sujet \\u00e9tait aussi \\u00e9crit d''une posture d\\u00e9fensive.</p><p data-block-key=\\\"bjvv\\\">Je retiens de la suggestion une posture qui se base plus sur le pr\\u00e9sent et sur l''avenir !</p><p data-block-key=\\\"5gcf3\\\">Nous avons vot\\u00e9 une prise de position forte au 17e Conseil des Voisins pour nous d\\u00e9clarer organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. La d\\u00e9claration d''organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral requiert trois conditions:</p><ol><li data-block-key=\\\"ffudt\\\">une gestion d\\u00e9sint\\u00e9ress\\u00e9e,</li><li data-block-key=\\\"b8cbc\\\">pas d''activit\\u00e9 lucrative,</li><li data-block-key=\\\"eif2t\\\">pas de fonctionnement au profit d''un cercle restreint.</li></ol><p data-block-key=\\\"focnb\\\">Pas plus tard que le 10 mai, d\\u00e9j\\u00e0 nous sommes test\\u00e9s sur les points 1 et 3 de par la visite de Madame la Ministre d\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq \\u00e0 Inclusiv''Day.</p>\", \"id\": \"68fe7661-6097-45c9-b54c-7c670b184e0f\"}, {\"type\": \"image\", \"value\": {\"title\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"heading_tag\": \"h3\", \"image\": 29, \"alt\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"caption\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"url\": \"\"}, \"id\": \"6c1bd7dc-4478-4ecf-8536-e4348a009777\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"503h4\\\">Ma\\u00ebl, Vice Pr\\u00e9sident des Grands Voisins, et \\u00e0 Inclusiv''Day pour Les Grands Voisins a envoy\\u00e9 une lettre de sollicitation pour le compte de son association La Caravane CECAI un soutien, un financement, et un encouragement de financement de tiers.</p><p data-block-key=\\\"5o9uj\\\">Cette action m''incite \\u00e0 proposer urgemment un organe de contr\\u00f4le de notre adh\\u00e9rence \\u00e0 nos d\\u00e9clarations (Contrat pour le Web, Coop, Int\\u00e9r\\u00eat G\\u00e9n\\u00e9ral). Ma\\u00ebl pourrait \\u00eatre alors en erreur au regard de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral.</p><p data-block-key=\\\"blc0r\\\">La discussion \\u00e0 avoir pour Les Grands Voisins avec Mme. la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap tourne autour du <a href=\\\"https://blog.lesgrandsvoisins.com/manifeste\\\">Manifeste des Grands Voisins</a>. C''est pour cela que nous aurions besoin d''un comit\\u00e9 de contr\\u00f4le de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Pour comprendre l''erreur, imaginez que vous faite partie d''un organisme qui pr\\u00e9tend \\u00eatre d\\u00e9sint\\u00e9ress\\u00e9, puis vous apprenez que les dirigeants obtiennent des avantages pour des organismes tiers dans leur gestion de l''organisme.</p><p data-block-key=\\\"e1kvg\\\">Comme toute Grande Voisine et tout Grand Voisin, Ma\\u00ebl A.N.C. et la Caravane CECAI ont des projets louables, mais Les Grands Voisins ne peuvent pas soutenir directement ou indirectement aucune des initiatives. Soutenir une initiative porte automatiquement pr\\u00e9judice, \\u00e0 mon sens, \\u00e0 toutes les initiatives que nous ne soutenons pas. Malheureusement, cette tendance a une historique dans les Grands Voisins.</p>\", \"id\": \"ecf3d596-bcf9-4d59-b8dd-ccd1f6171916\"}, {\"type\": \"image\", \"value\": {\"title\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"heading_tag\": \"h3\", \"image\": 30, \"alt\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"caption\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"url\": \"\"}, \"id\": \"e77fdcd9-a5bb-4b89-8c1d-36f064548769\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"u2nv8\\\">En avril 2019, certains acteurs ont propos\\u00e9 un \\u00ab SitesFaciles des Grands Voisins \\u00bb dont on comptait princiaplement Les Petits D\\u00e9brouillard et Fr\\u00e9d\\u00e9ric G.. Ce projet avait, \\u00e0 mon sens, probablement d\\u00e9j\\u00e0 cette notion de profiter \\u00e0 un cercle restreint de personnes. Il avait pr\\u00e9vu explicitement un budget de 20.000 euros pour un financement de salaire \\u00e0 mi-temps pour Ma\\u00ebl ANC. Les montants de cette initiative l\\u00e0 \\u00e9taient petits.</p><p data-block-key=\\\"c65u5\\\">Notre d\\u00e9fi est grand. L''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral aurait besoin d''un comit\\u00e9 de contr\\u00f4le. Pour cela, la robustesse de la part de la contribution de Ma\\u00ebl ANC a apport\\u00e9 ses fruits.</p><p data-block-key=\\\"2mg4p\\\">Mon autre vice-pr\\u00e9sident me pose une question par rapport \\u00e0 un projet accessoire que nous avions avec Ma\\u00ebl \\u2013 le <a href=\\\"https://web.resdigita.com/?ref=blog.lesgrandsvoisins.com\\\">web fait main fait maison</a> \\u2013 qui \\u00e9tait con\\u00e7u pour financer notre pr\\u00e9sence \\u00e0 Inclusiv''Day. (A savoir que l''avance nous permettant \\u00e0 aller \\u00e0 Inclusv''Day venait de Ma\\u00ebl.) L''observation de Caroline \\u00e9tait que ce projet aurait pu enfreindre la condition 2 d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Chapeau Caroline !</p>\", \"id\": \"4246292d-7239-4a7f-9644-08e26324869c\"}, {\"type\": \"image\", \"value\": {\"title\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"heading_tag\": \"h3\", \"image\": 31, \"alt\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"caption\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"url\": \"\"}, \"id\": \"8f1f3001-6ec5-4ee3-87a4-c1541d48cea1\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"amgnp\\\">L''id\\u00e9e que j''avais pour le web fait main \\u00e9tait plus \\u00e0 l''image de kermesse d''\\u00e9cole. C''\\u00e9tait une activit\\u00e9 plus symbolique. Pourtant, la question de Caroline ouvre la question exactement o\\u00f9 elle doit aller ! (Et, effectivement, l''activit\\u00e9 de b\\u00e9n\\u00e9volat aurait besoin d''\\u00eatre scrut\\u00e9e.)</p><p data-block-key=\\\"73po8\\\">Il n''y auraient peut-\\u00eatre pas de probl\\u00e8me \\u00e0 mon sens que les acteurs se revendiquent des Grands Voisins dans leurs activit\\u00e9s. C''est un honneur pour Les Grands Voisins. Le probl\\u00e8me vient quand Les Grands Voisins utilise sa position pour promouvoir un acteur. La question est d''actualit\\u00e9 pour le sujet du budget participatif de Paris \\u00ab <a href=\\\"https://blog.lesgrandsvoisins.com/lancement-du-budget-participatif/\\\">le num\\u00e9rique cr\\u00e9atif des Grands Voisins</a> \\u00bb.</p><p data-block-key=\\\"7obtv\\\">Je dis que je suis laur\\u00e9at, car il s''agit d''une activit\\u00e9 de b\\u00e9n\\u00e9volat qui m''est personnelle sur le site Saint Vincent de Paul, \\u00e0 une \\u00e9glise depuis 2016 et au sein d''une resourcerie \\u00e0 Arcueil. C''est une chose que j''aime faire: \\u00e9quiper et animer des salles de sociabilit\\u00e9 num\\u00e9rique. On y aborde d''abord la sociabilit\\u00e9 et apr\\u00e8s le num\\u00e9rique afin de d\\u00e9stigmatiser le num\\u00e9rique. C''est vraiment splendide pour moi et pour beaucoup. Pourtant, effectivement, c''est mon kiffe, mon kiffe perso.</p>\", \"id\": \"f0595ab5-89af-44b0-afc5-552f5af5db1e\"}, {\"type\": \"image\", \"value\": {\"title\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"heading_tag\": \"h3\", \"image\": 32, \"alt\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"caption\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"url\": \"\"}, \"id\": \"98a7f474-4a3d-4001-8607-df497475cf88\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"a0a0n\\\">Entre le 13 et le 17 mars 2023 a eu lieu le Conseil de Paris. Un d\\u00e9bat a eu lieu qui a, \\u00e0 mon sens, contrari\\u00e9 les conditions du budget participatif du 14e arrondissement. En parall\\u00e8le depuis des ann\\u00e9es, il y aurait, je pense, une activit\\u00e9 contre moi au sujet de la marque Les Grands Voisins. Le d\\u00e9bat au Conseil de Paris semble avoir plac\\u00e9 la Mairie du 14e et/ou la Mairie de Paris dans un effort probable de certains pour \\u00ab tuer la marque \\u00bb (je site un membre du bureau de l''association France Tiers Lieux qui donne une bonne caract\\u00e9risation de la mouvance contre les Grands Voisins telle qu''ici). La d\\u00e9lib\\u00e9ration sign\\u00e9e par Madame la Maire de Paris (Mme. HILDAGO) avait effectivement redirig\\u00e9 (je pense, d''apr\\u00e8s ce que je vois et les d\\u00e9bats) le financement du num\\u00e9rique cr\\u00e9atif des Grands Voisins \\u00e0 un projet diff\\u00e9rent. (Le projet diff\\u00e9rent aurait \\u00e9t\\u00e9 l''agrandissement de l''atelier de la Resourcerie Cr\\u00e9ative, lorsque le projet \\u00ab <a href=\\\"https://blog.lesgrandsvoisins.com/lancement-du-budget-participatif/\\\">le num\\u00e9rique cr\\u00e9atif des Grands Voisins</a> \\u00bb concernant entre autres <a href=\\\"https://blog.lesgrandsvoisins.com/budget-participatif-avec-la-mairie-du-14e-et-la-ressourcerie-creative/\\\">l''adaptation d''un atelier existant de la Ressourcerie Cr\\u00e9ative dans une approche</a> sp\\u00e9cifique.)</p>\", \"id\": \"ee086cc5-3732-4cdc-bbad-6f856660edcb\"}, {\"type\": \"image\", \"value\": {\"title\": \"Extrait du Conseil de Paris\", \"heading_tag\": \"h3\", \"image\": 33, \"alt\": \"Extrait du Conseil de Paris\", \"caption\": \"Extrait du Conseil de Paris\", \"url\": \"\"}, \"id\": \"96a1b111-502f-42a5-91af-90f551ad7879\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"a0a0n\\\">Quand le budget participatif \\u00ab <a href=\\\"https://blog.lesgrandsvoisins.com/lancement-du-budget-participatif/\\\">le num\\u00e9rique cr\\u00e9atif des Grands Voisins</a> \\u00bb \\u00e9tait propos\\u00e9, nous n''avions pas encore pris la partie d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Il convient aussi de regarder le budget participatif pour lequel plus de 4000 parisiens du 14e ont vot\\u00e9 pour v\\u00e9rifier son int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral.</p><p data-block-key=\\\"s40q\\\">Il y a aussi la question du <a href=\\\"https://blog.lesgrandsvoisins.com/16e-conseil-des-voisins/\\\">16e Conseil des voisins</a> qui avait vot\\u00e9 pour nommer \\u00ab <a href=\\\"https://www.resdigita.com/?ref=blog.lesgrandsvoisins.com\\\">ResDigita</a> \\u00bb les activit\\u00e9s num\\u00e9riques des Grands Voisins. Comme je suis moi m\\u00eame d\\u00e9brouillard informatique, et pourrais (je ne le fais pas maintenant) y gagner ma vie, il convient aussi une vigilance \\u00e0 cela. Je pense qu''il convient de d\\u00e9sinvestir Les Grands Voisins de la notion de ResDigita.</p><p data-block-key=\\\"baou6\\\">A mon sens l''avenir des Grands Voisins, association d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral coop\\u00e9rative, tient en une valeur ajout\\u00e9e: la neutralit\\u00e9 et la transparence avec personnalit\\u00e9, la personnalit\\u00e9 de notre manifeste. Voil\\u00e0 ce que je propose pour le <a href=\\\"https://blog.lesgrandsvoisins.com/18e-conseil/\\\">18e Conseil des Voisins</a> !</p>\", \"id\": \"0b8b04a6-0d57-41f3-a36c-d9890eac9f9e\"}]", "date": "2023-05-23T10:00:00Z", "live": true, "path": "000100020003000D", "slug": "la-suite-des-grands-voisins", "depth": 4, "owner": 1, "title": "La Suite des Grands Voisins", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/la-suite-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "La Suite des Grands Voisins", "entry_tags": [{"pk": null, "tag": 25, "content_object": 19}, {"pk": null, "tag": 15, "content_object": 19}], "go_live_at": null, "draft_title": "La Suite des Grands Voisins", "content_type": 71, "header_image": 29, "header_large": true, "header_darken": true, "live_revision": 42, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": 42, "translation_key": "100abfb8-1567-430c-8be0-f29103bb14e5", "entry_categories": [{"pk": 15, "page": 19, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T18:02:12.941Z", "first_published_at": "2024-05-05T18:01:07.129Z", "header_color_class": "red-marianne", "search_description": "Comment être d''intérêt général, vraiment ? Peu après notre déclaration, nous faillons peut-être déjà. C''est un cadeau pour nous apprendre !", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T18:02:12.908Z"}', NULL, '19', 1, 71, 1, 'La Suite des Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (49, '2024-05-05 18:14:47.308385+00', '{"pk": 21, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"mrlyl\\\">Votes</p><h2 data-block-key=\\\"e96k4\\\">D\\u00e9claration et statut d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral</h2><p data-block-key=\\\"ae58o\\\">La <a href=\\\"https://blog.lesgrandsvoisins.com/notre-qualite-dinteret-general/\\\">d\\u00e9claration de conformit\\u00e9 de statut d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral</a> est valid\\u00e9. Au regard de la jurisprudence de 2017, l''association \\u00e9mettra des r\\u00e9cits fiscaux aux individus et aux entreprises pour leurs dons de l''ann\\u00e9e en cours au premier trimestre de l''ann\\u00e9e suivante. La d\\u00e9claration touche et affecte nos engagements de neutralit\\u00e9, d''ind\\u00e9pendance et d''ouverture absolue que nous affirmons.</p><p data-block-key=\\\"co37f\\\">Coll\\u00e8ge A : Oui (1) Non (0)<br/>Coll\\u00e8ge B : Oui (3) Non (0)<br/>Coll\\u00e8ge C : Oui (4) Non (0)</p><p data-block-key=\\\"9bvmd\\\"><a href=\\\"https://blog.lesgrandsvoisins.com/content/files/2023/05/intergen.pdf\\\"><b>Intergenintergen.pdf</b> 1 MB</a></p><h2 data-block-key=\\\"b8b65\\\">D\\u00e9claration et statut d''association coop\\u00e9rative</h2><p data-block-key=\\\"53t5s\\\">La <a href=\\\"https://blog.lesgrandsvoisins.com/coop/\\\">d\\u00e9claration des sept engagements coop\\u00e9ratifs transmis \\u00e0 deux f\\u00e9d\\u00e9rations internationales des Coop\\u00e9ratives</a> est valid\\u00e9e par ce Conseil des Voisins et engage l''association. Cette m\\u00eame d\\u00e9claration est expertis\\u00e9e par Dot Cooperation et nous accorde la qualit\\u00e9 de \\\"Cooperative\\\" avec tout ce qui vient avec. Notre qualit\\u00e9 de coop\\u00e9rative est donc par une reconnaissance internationale et non pas une reconnaissance fran\\u00e7aise (car la coop\\u00e9rative au sens fran\\u00e7ais est une soci\\u00e9t\\u00e9).</p><p data-block-key=\\\"4h063\\\">Coll\\u00e8ge A : Oui (1) Non (0)<br/>Coll\\u00e8ge B : Oui (3) Non (0)<br/>Coll\\u00e8ge C : Oui (4) Non (0)</p><p data-block-key=\\\"b90e3\\\"><a href=\\\"https://blog.lesgrandsvoisins.com/content/files/2023/05/coop-statut-agrement.pdf\\\"><b>Coop statut agrementcoop-statut-agrement.pdf</b> 144 KB</a></p><h2 data-block-key=\\\"2ut1l\\\">Ouverture de compte courant au Cr\\u00e9dit Agricole</h2><p data-block-key=\\\"2c945\\\">Chris MANN est mandat\\u00e9 pour ouvrir un compte au plus t\\u00f4t aupr\\u00e8s du Cr\\u00e9dit Agricole.</p><p data-block-key=\\\"q7ue\\\">Coll\\u00e8ge A : Oui (1) Non (0)<br/>Coll\\u00e8ge B : Oui (3) Non (0)<br/>Coll\\u00e8ge C : Oui (4) Non (0)</p><h2 data-block-key=\\\"c2go\\\">Prise en compte des frais engag\\u00e9s</h2><p data-block-key=\\\"32guq\\\">Chris MANN peut pr\\u00e9senter des factures au nom de l''Association coop\\u00e9rative pour remboursement.</p><p data-block-key=\\\"73kr4\\\">Coll\\u00e8ge A : Oui (1) Non (0)<br/>Coll\\u00e8ge B : Oui (3) Non (0)<br/>Coll\\u00e8ge C : Oui (4) Non (0)</p><p data-block-key=\\\"fvi3q\\\">Participation</p><p data-block-key=\\\"85utk\\\">Sont pr\\u00e9sents lors de la s\\u00e9ance : Ada Matus, Chris Mann, Sviatlana Viarbitskaya, Martha Montenegro, Amadou Gaye, Jamila Oumahi.</p><p data-block-key=\\\"a22n5\\\">Ont exprim\\u00e9 leurs voix dans la semaine apr\\u00e8s la s\\u00e9ance : Mael ANC, Caroline LHOMME.</p><h3 data-block-key=\\\"qo1o\\\">Invitation</h3><p data-block-key=\\\"52eo6\\\">Rendez-vous sur meet.lesgrandsvoisins.com ou par t\\u00e9l\\u00e9phone mardi <a href=\\\"https://www.lesgrandsvoisins.com/event/17e-conseil-des-voisins-en-ligne-26/register?ref=blog.lesgrandsvoisins.com\\\">le 2 mai \\u00e0 20h45 pour le 17e Conseil des voisins</a> de l\\u2019association coop\\u00e9rative Les Grands Voisins. Ces conseils sont les lieux pour la d\\u00e9cision et la transparence collectives, mais pas le seul lieu. Ce conseil est particuli\\u00e8rement n\\u00e9cessaire pour pouvoir assoir les \\u00e9l\\u00e9ments n\\u00e9cessaires en pr\\u00e9paration pour</p><ol><li data-block-key=\\\"f8mc5\\\"><a href=\\\"https://www.lesgrandsvoisins.com/event/inclusiv-day-a-paris-la-defense-arena-25/register?ref=blog.lesgrandsvoisins.com\\\">notre participation \\u00e0 Inclusiv''Day le 10 mai 2023 \\u00e0 la </a>D\\u00e9fense y compris dans notre collaboration pour un <a href=\\\"https://blog.lesgrandsvoisins.com/organisation-du-village-tih-a-inclusivday/\\\">SitesFaciles TIH</a></li><li data-block-key=\\\"a218q\\\">le <a href=\\\"https://blog.lesgrandsvoisins.com/lancement-du-budget-participatif/\\\">lancement de la salle de sociabilit\\u00e9 num\\u00e9rique du budget participatif</a></li><li data-block-key=\\\"8vbg1\\\"><a href=\\\"https://blog.lesgrandsvoisins.com/notre-qualite-dinteret-general/\\\">l''agr\\u00e9ment en tant qu''organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral</a> aux sens des arts. 200 et 238 du code g\\u00e9n\\u00e9ral des imp\\u00f4ts selon la proc\\u00e9dure de la jurisprudence de 2017</li><li data-block-key=\\\"dlv81\\\">Valider la d\\u00e9marche <a href=\\\"https://blog.lesgrandsvoisins.com/fun-d-raising/\\\">d''appel \\u00e0 fonds</a> (appel \\u00e0 fun)</li><li data-block-key=\\\"c1pi3\\\">Passer en revue les d\\u00e9cisions structurelles notamment en termes de communication: rester sur lesgrandsvoisins.com pour tout, ...</li><li data-block-key=\\\"7jp0s\\\">Agr\\u00e9er le Cr\\u00e9dit coop\\u00e9ratif ou une autre banque</li><li data-block-key=\\\"dih96\\\">Mentionner les devis d''avocat et de comptable sous consid\\u00e9ration</li><li data-block-key=\\\"8n4p0\\\">Questions diverses</li></ol><p data-block-key=\\\"9iimj\\\">Inscription ici:</p><p data-block-key=\\\"2vq5k\\\"><a href=\\\"https://www.lesgrandsvoisins.com/event/17e-conseil-des-voisins-en-ligne-26/register/?ref=blog.lesgrandsvoisins.com\\\">https://www.lesgrandsvoisins.com/event/17e-conseil-des-voisins-en-ligne-26/register/?ref=blog.lesgrandsvoisins.com</a></p>\", \"id\": \"57b9bd11-9148-4d53-949c-1ed6957c4457\"}]", "date": "2023-05-02T11:00:00Z", "live": true, "path": "000100020003000F", "slug": "17e-conseil-des-voisins", "depth": 4, "owner": 1, "title": "17e Conseil des Voisins - 2 mai 2023 - CR", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/17e-conseil-des-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "17e Conseil des Voisins - 2 mai 2023 - CR", "entry_tags": [{"pk": 37, "tag": 10, "content_object": 21}, {"pk": 38, "tag": 15, "content_object": 21}], "go_live_at": null, "draft_title": "17e Conseil des Voisins - 2 mai 2023 - CR", "content_type": 71, "header_image": 36, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "7de3d9c6-b964-436e-8faa-e8e3bcc5a11a", "entry_categories": [{"pk": 17, "page": 21, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "blue-ecume", "search_description": "Nous avions eu le 17e Conseil des Voisins le 2 mai 2023. Nous avons acté : 1. Notre statut de coopératif et ses engagements 2. Notre statut d''intérêt général et ses engagements 3. Des questions administratives Compte-rendu version 2023-05-04-01", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '21', 1, 71, 1, '17e Conseil des Voisins - 2 mai 2023 - CR');
INSERT INTO public.wagtailcore_revision VALUES (54, '2024-05-05 18:38:08.198738+00', '{"pk": 23, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"qdho9\\\">Je me permets de citer Pauline du Service D\\u00e9mocratie Locale de la Mairie du 14e qi a tr\\u00e8s bien r\\u00e9sum\\u00e9 notre r\\u00e9union comme suite:</p><p data-block-key=\\\"446bu\\\">Nous avons pu \\u00e9changer sur les 3 aspects de votre projet :</p><ul><li data-block-key=\\\"fp6ap\\\">Du mat\\u00e9riel informatique pour proposer des ateliers d''accessibilit\\u00e9 num\\u00e9rique, qui soit pliable comme l''espace de la Ressourcerie d\\u00e9di\\u00e9 est polyvalent. Il servirait aussi pour \\u00e9ventuellement faire des ateliers dans d''autres structures (Table des Mati\\u00e8res, Moulin \\u00e0 Caf\\u00e9 ?). Ce mat\\u00e9riel pourrait \\u00e9ventuellement \\u00eatre r\\u00e9cup\\u00e9r\\u00e9 aupr\\u00e8s d''entreprises qui n''en ont plus l''utilit\\u00e9</li><li data-block-key=\\\"d5vv\\\">du mat\\u00e9riel de prise vid\\u00e9o, qui remplacerait l''imprimante 3D du projet initial car plus ouvert et accessible</li><li data-block-key=\\\"7ntqd\\\">des serveurs afin de proposer un service solidaire de sauvegarde s\\u00e9curis\\u00e9e des documents num\\u00e9riques, et mat\\u00e9rialiser aussi le \\u00ab cloud \\u00bb</li></ul><p data-block-key=\\\"b3i43\\\">Deux autres \\u00e9l\\u00e9ments sur lesquels Chris Mann, avec l''association les Grands Voisins, avance :</p><ul><li data-block-key=\\\"all88\\\">Chercher des pistes pour demander aux Entreprises de r\\u00e9cup\\u00e9rer une partie de leur mat\\u00e9riel quand il est renouvel\\u00e9</li><li data-block-key=\\\"62pnk\\\">Continuer \\u00e0 faire du lien avec d''autres associations \\u00e0 impliquer, notamment dans l''optique de cr\\u00e9er un socle de b\\u00e9n\\u00e9voles qui s''int\\u00e9ressent \\u00e0 la question (APRIL, fondation Valentin H\\u00fcey)</li></ul><p data-block-key=\\\"5g49b\\\">Il y a aussi des consid\\u00e9rations qui touchent aux aspects administratifs. Notre association coop\\u00e9rative, bien que nous sommes en partenariat avec la Ressourcerie Cr\\u00e9ative, est \\u00e0 l''\\u00e9cart de la dotation lors de l''arr\\u00eat\\u00e9 de Madame la Maire de Paris Hildago le 17 mars 2023.</p>\", \"id\": \"16c0ebce-ee5c-4aec-86ee-cc444b93db47\"}]", "date": "2023-04-28T10:00:00Z", "live": true, "path": "000100020003000H", "slug": "lauriat-du-budget-paritcipatif-de-paris-14e-le-numerique-creatif-des-grands-voisins", "depth": 4, "owner": 1, "title": "Lancement du Budget Participatif de Paris 14e : Le numérique créatif des Grands Voisins", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/lauriat-du-budget-paritcipatif-de-paris-14e-le-numerique-creatif-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Lauriat du Budget Paritcipatif de Paris 14e : Le numérique créatif des Grands Voisins", "entry_tags": [{"pk": null, "tag": 29, "content_object": 23}, {"pk": null, "tag": 15, "content_object": 23}], "go_live_at": null, "draft_title": "Lauriat du Budget Paritcipatif de Paris 14e : Le numérique créatif des Grands Voisins", "content_type": 71, "header_image": 39, "header_large": true, "header_darken": true, "live_revision": 53, "show_in_menus": true, "blog_categories": [3, 2], "header_cta_link": null, "header_cta_text": null, "latest_revision": 53, "translation_key": "717dac9a-d8cb-4da0-bce2-2f04c6ef63f6", "entry_categories": [{"pk": 19, "page": 23, "category": 2}, {"pk": 20, "page": 23, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T18:37:44.634Z", "first_published_at": "2024-05-05T18:37:44.634Z", "header_color_class": "pink-tuile", "search_description": "Il y a aussi des considérations qui touchent aux aspects administratifs. Notre association coopérative, bien que nous sommes en partenariat avec la Ressourcerie Créative, est à l''écart de la dotation lors de l''arrêté de Madame la Maire de Paris Hidalgo le 17 mars 2023.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T18:37:44.605Z"}', NULL, '23', 1, 71, 1, 'Lancement du Budget Participatif de Paris 14e : Le numérique créatif des Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (61, '2024-05-06 03:11:43.592504+00', '{"pk": 27, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"1qwaf\\\">Le 10 mai 2023 \\u00e0 Paris La D\\u00e9fense Arena, venez nous rendre visite ainsi que d''autres acteurs autour du handicap et de la r\\u00e9alisation. Sur notre stand, nous aurions de l''art, le livre de Caroline et des d\\u00e9monstrations de nos axes digital et artistiques. L''organisateur parle d\\u2019un riche programme \\u00e9ditorial (management &amp; strat\\u00e9gie RSE, emploi inclusif, achats responsables, innovations sociales, accessibilit\\u00e9 num\\u00e9rique, actualit\\u00e9s handicap, ateliers de sensibilisation) des conf\\u00e9rences th\\u00e9matiques, des ateliers de formation, des s\\u00e9ances de networking et des animations immersives.</p><p data-block-key=\\\"2dc2f\\\">Inscription ici pour Les Grands Voisins et sur le site de l''organisateur respectivement:</p><p data-block-key=\\\"f72ld\\\"><a href=\\\"https://event.inclusivday.com/registration/inscription?iw_exhibitorid=c88bcbb6-f6dd-ed11-8e89-000d3a4a8203&amp;ref=blog.lesgrandsvoisins.com\\\">https://event.inclusivday.com/registration/inscription?iw_exhibitorid=c88bcbb6-f6dd-ed11-8e89-000d3a4a8203&amp;ref=blog.lesgrandsvoisins.com</a></p><p data-block-key=\\\"dig04\\\"><a href=\\\"https://blog.lesgrandsvoisins.com/nous-a-inclusivday-cnit-10-mai-2023/\\\">https://blog.lesgrandsvoisins.com/nous-a-inclusivday-cnit-10-mai-2023/</a></p><p data-block-key=\\\"e8clf\\\">0 \\u20ac de notre objectif de 650 \\u20ac pour le financement de notre participation Inclusiv''Day, merci de penser \\u00e0 acheter nos services ou <a href=\\\"https://www.lesgrandsvoisins.com/dons?ref=blog.lesgrandsvoisins.com\\\">effectuer un don</a> s''il vous pla\\u00eet.</p><p data-block-key=\\\"9ekg2\\\">DATE ET HEURE</p><p data-block-key=\\\"8njn5\\\">mercredi mai 10, 202308:30 \\u00a019:00 (Europe/Paris) <a href=\\\"https://www.lesgrandsvoisins.com/event/inclusiv-day-au-cnit-defense-paris-25/register?ref=blog.lesgrandsvoisins.com#\\\">Ajouter au calendrier</a></p><p data-block-key=\\\"1greu\\\">LIEU</p><p data-block-key=\\\"7gns1\\\">CNIT la D\\u00e9fense2, parvis de La D\\u00e9fense<br/>92800 Puteaux<br/></p>\", \"id\": \"59753479-bd84-413b-bd65-ccfb0160a5b6\"}]", "date": "2023-04-06T10:08:00Z", "live": true, "path": "000100020003000L", "slug": "inclusivday-au-cnit-10-mai", "depth": 4, "owner": 1, "title": "Inclusiv''Day au CNIT 10 mai", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/inclusivday-au-cnit-10-mai/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Inclusiv''Day au CNIT 10 mai", "entry_tags": [{"pk": 52, "tag": 33, "content_object": 27}, {"pk": 53, "tag": 30, "content_object": 27}], "go_live_at": null, "draft_title": "Inclusiv''Day au CNIT 10 mai", "content_type": 71, "header_image": 45, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "c708ea0b-8162-425c-bf99-5a83c9cf0c0b", "entry_categories": [{"pk": 23, "page": 27, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "yellow-tournesol", "search_description": "Le 10 mai 2023 à Paris La Défense Arena, venez nous rendre visite ainsi que d''autres acteurs autour du handicap et de la réalisation. Sur notre stand, nous aurions de l''art, le livre de Caroline et des démonstrations de nos axes digital et artistiques. L''organisateur parle d’un riche programme éditorial (management & stratégie RSE, emploi inclusif, achats responsables, innovations sociales, accessibilité numérique, actualités handicap, ateliers de sensibilisation) des conférences thématiques, des ateliers de formation, des séances de networking et des animations immersives.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '27', 1, 71, 1, 'Inclusiv''Day au CNIT 10 mai');
INSERT INTO public.wagtailcore_revision VALUES (93, '2024-05-06 12:16:23.161996+00', '{"pk": 34, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"yzz1i\\\">La DINUM a eu la gr\\u00e2ce de publier son moteur CMS pour numerique.gouv.fr sur github. Nous nous donnons comme mission de pouvoir rendre le b\\u00e9n\\u00e9fice de ce moteur aux acteurs priv\\u00e9s par le fait de remplacer la charte graphique de l''Etat par une charte g\\u00e9n\\u00e9rique, des chartes g\\u00e9n\\u00e9riques. Voici le lieu de notre travail:</p><p data-block-key=\\\"9fir4\\\"><a href=\\\"https://github.com/chris2fr/sites-faciles\\\">https://github.com/chris2fr/sites-faciles</a></p><p data-block-key=\\\"f6rl6\\\">Les choix prix par le DINUM, \\u00e0 partir du m\\u00eame socle technologique des Grands Voisins -- Wagtail, Django, Python -- sont structurants et rationnels. Le fait d''ouvrir ces d\\u00e9veloppements \\u00e0 un grand nombre nous permet de reverser \\u00e0 l''Etat nos contributions modestes. C''est un exemple de la puissance du dynamique des logiciels libres.</p><p data-block-key=\\\"8la9t\\\">Ce site en travaux est disponible sur <a href=\\\"https://facile.lesgrandsvoisins.com\\\">https://facile.lesgrandsvoisins.com</a>.</p><p data-block-key=\\\"fmm91\\\">Nous avons aussi essay\\u00e9 un site statique sur <a href=\\\"https://les.gv.coop\\\">https://les.gv.coop</a></p><p data-block-key=\\\"3djgc\\\">Le site actuel est sur <a href=\\\"https://www.lesgrandsvoisins.com\\\">https://www.lesgrandsvoisins.com</a></p>\", \"id\": \"87661a48-af93-4913-b933-f0afb4d285b7\"}, {\"type\": \"stepper\", \"value\": {\"title\": \"Mise en place du nouveau site web sous Sites-Faciles\", \"total\": 3, \"current\": 1, \"steps\": [{\"type\": \"step\", \"value\": {\"title\": \"Installation et tests\", \"detail\": \"Il s''agit d''installer le dispositif depuis sites-faciles, de faire des tests et d''ajuster le look-and-feel.\"}, \"id\": \"9a7f37fb-22e6-4878-80cd-8f8e31925334\"}, {\"type\": \"step\", \"value\": {\"title\": \"Validation et contenu\", \"detail\": \"Il s''agit d''organiser l''information et affiner la pr\\u00e9sentation avec une exp\\u00e9rience utilisateur convenable.\"}, \"id\": \"19234db7-2154-489c-9c57-d116199345c8\"}, {\"type\": \"step\", \"value\": {\"title\": \"Installation et mise en production\", \"detail\": \"Il s''agit de faire en sorte que le site web se charge rapidement et est conforme aux besoins de l''association et du Monde.\"}, \"id\": \"48947ec7-5ab0-48a9-8957-d813fc032715\"}]}, \"id\": \"1eea2c01-8d52-4398-8626-9f5298487fc0\"}, {\"type\": \"multicolumns\", \"value\": {\"bg_image\": null, \"bg_color_class\": \"\", \"bg_color\": \"\", \"title\": \"\", \"heading_tag\": \"h2\", \"columns\": [{\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"l1q4l\\\">Appropriation du dispositif de la DINUM</h3><p data-block-key=\\\"5d2ak\\\">Le dispositif <a href=\\\"https://sites-faciles.beta.numerique.gouv.fr/\\\">sites-faciles</a> de la DINUM est tr\\u00e8s \\u00e9troitement li\\u00e9 aux graphismes de l''Etat, mais a aussi des choix pratiques et communicatives pour des sites webs en France. Nous avons enregistr\\u00e9s <a href=\\\"https://github.com/numerique-gouv/sites-faciles/issues\\\">quelques items</a> avec la DINUM pour travailler sur ces sujets. </p>\", \"id\": \"bb0d1e3a-a505-44bd-ba0b-3663b6ab2b06\"}]}, \"id\": \"70854f40-8c96-413f-b377-d9a1d1b0435a\"}]", "date": "2024-05-06T08:37:00Z", "live": true, "path": "000100020003000O", "slug": "sites-faciles", "depth": 4, "owner": 1, "title": "Refonte web avec Sites Faciles", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/sites-faciles/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Refonte web avec avec Sites Faciles", "entry_tags": [{"pk": null, "tag": 34, "content_object": 34}, {"pk": null, "tag": 35, "content_object": 34}, {"pk": null, "tag": 36, "content_object": 34}], "go_live_at": null, "draft_title": "Refonte web avec Sites Faciles", "content_type": 71, "header_image": 53, "header_large": true, "header_darken": false, "live_revision": 92, "show_in_menus": true, "blog_categories": [2], "header_cta_link": null, "header_cta_text": null, "latest_revision": 92, "translation_key": "53775bc0-3da8-4a48-b082-d4b2cd50bd6b", "entry_categories": [{"pk": 26, "page": 34, "category": 2}], "header_cta_label": null, "header_with_title": false, "last_published_at": "2024-05-06T11:47:13.631Z", "first_published_at": "2024-05-06T08:45:06.274Z", "header_color_class": "grey", "search_description": "La DINUM a eu la grâce de publier son moteur CMS pour numerique.gouv.fr sur github. Nous nous donnons comme mission […]", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-06T11:47:13.556Z"}', NULL, '34', 1, 71, 1, 'Refonte web avec Sites Faciles');
INSERT INTO public.wagtailcore_revision VALUES (83, '2024-05-06 08:49:04.041073+00', '{"pk": 6, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"vkm50\\\">Bienvenue sur le blog des Grands Voisins !</p><p data-block-key=\\\"32ias\\\">Vous trouverez ici des articles sur la vie de notre association.</p>\", \"id\": \"754f3ce1-7bed-41b5-8651-5288f44faeb9\"}]", "live": true, "path": "000100020003", "slug": "blog", "depth": 3, "owner": 1, "title": "Blog des Grands Voisins", "locale": 1, "locked": false, "expired": false, "alias_of": null, "numchild": 24, "url_path": "/home/blog/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Blog des Grands Voisins", "go_live_at": null, "draft_title": "Blog des Grands Voisins", "content_type": 72, "header_image": 8, "header_large": true, "header_darken": false, "live_revision": 48, "show_in_menus": true, "posts_per_page": 40, "header_cta_link": null, "header_cta_text": null, "latest_revision": 48, "translation_key": "8be861af-e7c2-4520-a1e2-f7aa654db661", "header_cta_label": null, "header_with_title": false, "last_published_at": "2024-05-05T18:10:45.555Z", "first_published_at": "2024-05-05T16:24:40.051Z", "header_color_class": "blue-france", "search_description": "Bienvenue sur le blog des Grands Voisins !\r\nVous trouverez ici des articles sur la vie de notre association.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T18:10:45.536Z"}', NULL, '6', 1, 72, 1, 'Blog des Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (121, '2024-05-20 16:20:16.973678+00', '{"pk": 44, "body": "[{\"type\": \"paragraph\", \"value\": \"<h2 data-block-key=\\\"56he6\\\">Les Popup Expos</h2><p data-block-key=\\\"9dlj6\\\">Inviter l''art hors les murs, c''est-\\u00e0-dire dans des lieux o\\u00f9 on ne l''attend pas :</p><ul><li data-block-key=\\\"60bjf\\\">donne une autre vision du quotidien</li><li data-block-key=\\\"1q19v\\\">anime les murs</li><li data-block-key=\\\"dtj5n\\\">propose une \\u00e9vasion</li><li data-block-key=\\\"2mt9r\\\">valorise nos patrimoines, nos cultures et nos artistes</li></ul><p data-block-key=\\\"bhq6a\\\">Animer les murs en organisant une exposition peut avoir de nombreux avantages, \\u00e0 la fois pour les publics, les commer\\u00e7ants, et les artistes.</p><p data-block-key=\\\"2o0l5\\\">Notre d\\u00e9marche de popup expos favorise la vie de quartier par l''invitation de l\\u2019art dans le quotidien des publics en cassant les id\\u00e9es re\\u00e7ues. L''art pour la population cr\\u00e9e une synergie entre le commerce et la culture, met en lumi\\u00e8re les artistes locaux et les aide \\u00e0 gagner en visibilit\\u00e9 tout en dynamisant l\\u2019\\u00e9conomie locale avec l''acc\\u00e8s \\u00e0 la culture, l''apaisement, la d\\u00e9tente, l''\\u00e9tonnement ...</p><p data-block-key=\\\"fg0nm\\\">Les lieux du commer\\u00e7ant se voient affect\\u00e9 aussi un caract\\u00e8re hybride, car ils deviennent parfois, temporairement, aussi des gall\\u00e9ries artistiques.</p><h3 data-block-key=\\\"6fofg\\\">Plus de d\\u00e9tails</h3><p data-block-key=\\\"5hkmj\\\">Inviter l''art hors les murs, c''est-\\u00e0-dire dans des lieux o\\u00f9 on ne l''attend pas :</p><ul><li data-block-key=\\\"b63mj\\\">donne une autre vision du quotidien</li><li data-block-key=\\\"5ov26\\\">anime les murs</li><li data-block-key=\\\"clg4q\\\">propose une \\u00e9vasion</li><li data-block-key=\\\"f3a8u\\\">valorise nos patrimoines, nos cultures et nos artistes</li></ul><p data-block-key=\\\"34dar\\\">Animer les murs en organisant une exposition peut avoir de nombreux avantages, \\u00e0 la fois pour les publics, les commer\\u00e7ants, et les artistes.</p><p data-block-key=\\\"2vg4d\\\">Notre d\\u00e9marche de popup expos favorise la vie de quartier par l''invitation de l\\u2019art dans le quotidien des publics en cassant les id\\u00e9es re\\u00e7ues. L''art pour la population cr\\u00e9e une synergie entre le commerce et la culture, met en lumi\\u00e8re les artistes locaux et les aide \\u00e0 gagner en visibilit\\u00e9 tout en dynamisant l\\u2019\\u00e9conomie locale avec l''acc\\u00e8s \\u00e0 la culture, l''apaisement, la d\\u00e9tente, l''\\u00e9tonnement ...</p><p data-block-key=\\\"c6jbp\\\">Les lieux du commer\\u00e7ant se voient affect\\u00e9 aussi un caract\\u00e8re hybride, car ils deviennent parfois, temporairement, aussi des gall\\u00e9ries artistiques.</p><p data-block-key=\\\"4sdud\\\">Les publics :</p><ul><li data-block-key=\\\"fmsjg\\\">s''engagent avec les \\u0153uvres des popup expos, chose qui peut remuer les m\\u00e9ninges ou apaiser selon les cas</li><li data-block-key=\\\"1sqf9\\\">interagissent avec le m\\u00e9tier du commer\\u00e7ant aussi de mani\\u00e8re augment\\u00e9e par les expos,</li><li data-block-key=\\\"80iom\\\">s''\\u00e9duquent sur un sujet connexe au m\\u00e9tier du commer\\u00e7ant si l''expo en est en lien,</li><li data-block-key=\\\"dttaa\\\">se retrouvent dans un milieu d''avantage cr\\u00e9atif</li><li data-block-key=\\\"fqtcl\\\">ont des outils pour renforcer le bien-\\u00eatre, en compl\\u00e9ment de la prestation du commer\\u00e7ant</li><li data-block-key=\\\"6uc3e\\\">peuvent acqu\\u00e9rir aux \\u0153uvres en personne ou en ligne.</li><li data-block-key=\\\"9coe0\\\">peuvent interagir avec les artistes lors des animations pr\\u00e9vues,</li><li data-block-key=\\\"5m1jv\\\">peuvent rencontrer des nouveaux publics chez le commer\\u00e7ant.</li></ul><p data-block-key=\\\"2uf5j\\\">Les commer\\u00e7ants :</p><ul><li data-block-key=\\\"6ons2\\\">disposent d''un contr\\u00f4le sur ce qui est affich\\u00e9 dans leurs espaces</li><li data-block-key=\\\"ddrd9\\\">diversifient leurs client\\u00e8les</li><li data-block-key=\\\"117ej\\\">permettent l''organisation de vernissages, de d\\u00e9crochages, et/ou \\u00e9ventuellement des ateliers ponctuels</li><li data-block-key=\\\"19htf\\\">s''enracinent plus profond\\u00e9ment dans le tissu local</li><li data-block-key=\\\"5fg7e\\\">rencontrent des artistes g\\u00e9niaux</li><li data-block-key=\\\"2oe1u\\\">valorisent les artistes avec une contribution financi\\u00e8re et \\u00e9ventuellement aussi en nature</li><li data-block-key=\\\"8qms7\\\">pr\\u00e9voient avec les artistes les responsabilit\\u00e9s en cas de (on esp\\u00e8re que cela n''arrive pas) vol ou d\\u00e9gradation des \\u0153uvres</li><li data-block-key=\\\"2he85\\\">enrichissent leurs liens avec toute personne qui vient sur place</li><li data-block-key=\\\"2g1vn\\\">pourraient \\u00e9ventuellement orienter les invitations d''expos pour sensibiliser le public sur les questions sp\\u00e9cifiques \\u00e0 leurs m\\u00e9tiers</li><li data-block-key=\\\"2ig8g\\\">b\\u00e9n\\u00e9ficient d''un cadre agr\\u00e9able des popup expos, y compris des nouvelles rencontres</li></ul><p data-block-key=\\\"5stho\\\">Les artistes :</p><ul><li data-block-key=\\\"db6ph\\\">augmentent la visibilit\\u00e9</li><li data-block-key=\\\"22cup\\\">se soutiennent dans leurs ventes d''\\u0153uvres</li><li data-block-key=\\\"7unud\\\">pr\\u00e9voient les \\u00e9ventualit\\u00e9s en cas de vol ou d\\u00e9gradation (et pour lesquels nous pennons des pr\\u00e9cautions)</li><li data-block-key=\\\"55gai\\\">sont valoris\\u00e9s selon les moyens du commer\\u00e7ant</li><li data-block-key=\\\"5klt0\\\">communiquent autour de leurs \\u0153uvres dans des gall\\u00e9ries hybrides</li><li data-block-key=\\\"16ct8\\\">rencontrent des nouvelles personnes</li><li data-block-key=\\\"es27l\\\">participent au tissu \\u00e9conomique autrement</li><li data-block-key=\\\"6glmd\\\">peuvent vendre leurs \\u0153uvres en ligne ou en personne</li><li data-block-key=\\\"37q01\\\">enrichissent le patrimoine activement avec vos \\u0153uvres</li></ul><p data-block-key=\\\"a890h\\\">Selon le principe de subsidiarit\\u00e9, Les Grands Voisins (Les Arts Voisins) peuvent :</p><ul><li data-block-key=\\\"b9fuc\\\">rechercher d''artistes</li><li data-block-key=\\\"1i6gj\\\">rechercher des commer\\u00e7ants / lieux</li><li data-block-key=\\\"96qha\\\">g\\u00e9rer des candidatures</li><li data-block-key=\\\"8p5gk\\\">proposer d''un projet articul\\u00e9 autour des artistes et leur cr\\u00e9ativit\\u00e9 et le lieu d''exposition</li><li data-block-key=\\\"bf44j\\\">s''occuper de la logistique</li><li data-block-key=\\\"9u5u\\\">organiser les accrochages (vernissages) et d\\u00e9crochages</li><li data-block-key=\\\"6aka3\\\">organiser de rencontres avec les artistes</li><li data-block-key=\\\"ctpjh\\\">interm\\u00e9dier pour faciliter la vente des \\u0153uvres</li><li data-block-key=\\\"d9dd4\\\">\\u00e9crire des articles sur les popup expos, les ateliers, les vernissages et des d\\u00e9crochages</li><li data-block-key=\\\"55hvs\\\">\\u00e9ventuellement souscrire une assurance explicite (aucune garantie implicite n''est assum\\u00e9e)</li></ul>\", \"id\": \"2324c983-6ed1-45c1-9970-2d712a67677c\"}]", "live": true, "path": "0001000200060001", "slug": "popup-expos", "depth": 4, "owner": 1, "title": "Les Popup Expos", "locale": 1, "locked": false, "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/arts-plastiques/popup-expos/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "", "go_live_at": null, "draft_title": "Popup Expos", "content_type": 25, "header_image": null, "header_large": false, "header_darken": false, "live_revision": 120, "show_in_menus": true, "header_cta_link": null, "header_cta_text": null, "latest_revision": 120, "translation_key": "09a49030-562a-4f39-a6f0-2ed603b8270b", "contentpage_tags": [], "header_cta_label": null, "header_with_title": false, "last_published_at": "2024-05-20T16:18:48.551Z", "first_published_at": "2024-05-20T16:18:48.551Z", "header_color_class": null, "search_description": "Les Popup Expos Inviter l''art hors les murs, c''est-à-dire dans des lieux où on ne l''attend pas : donne une […]", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-20T16:18:48.526Z"}', NULL, '44', 1, 25, 1, 'Les Popup Expos');
INSERT INTO public.wagtailcore_revision VALUES (26, '2024-05-05 17:11:01.908941+00', '{"pk": 12, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"vz7ve\\\">Voici les premiers mots de Thimot\\u00e9 sur un ordinateur</p><p data-block-key=\\\"f50ls\\\">Salut mon ami comment vas-tu ? Si tout va bien, DIEU merci. Et les activit\\u00e9s \\u00e7a marche bien ou pas ? Cela \\u00a0fait un bon moment qu\\u2019on ne se voit pas. J\\u2019esp\\u00e8re qu\\u2019on se reverra tr\\u00e8s prochainement.</p><p data-block-key=\\\"649qv\\\">Et Timoth\\u00e9e revient deux semaines plus tard pour ajouter \\u00e0 l''article.</p><p data-block-key=\\\"3iblt\\\">Bonjour les Grands Voisins,</p><p data-block-key=\\\"5mrg4\\\">Je suis ravi d\\u2019entendre parler de vous. je suis encore l\\u00e0 aujourd\\u2019hui pour me connecter au monde de l\\u2019informatique, du num\\u00e9rique. L\\u2019atelier d\\u2019entraide num\\u00e9rique est vraiment important et la ressourcerie cr\\u00e9ative comme son nom l\\u2019indique est une chose a d\\u00e9couvrir \\u00e9tant donn\\u00e9 que c\\u2019est vraiment un endroit pour se ressourcer et cr\\u00e9er.</p><p data-block-key=\\\"6nhgc\\\">Ne dit-on pas que l\\u2019analphab\\u00e8te du 21 si\\u00e8cle n\\u2019est plus celui qui n\\u2019a jamais \\u00e9t\\u00e9 a l\\u2019\\u00e9cole mais c\\u2019est celui qui n\\u2019a pas de notion en informatique.</p>\", \"id\": \"e1c2a258-883d-4473-b38b-6c715235f8fb\"}, {\"type\": \"image\", \"value\": {\"title\": \"Timoth\\u00e9e revient et tape tout seul\", \"heading_tag\": \"h3\", \"image\": 18, \"alt\": \"Timoth\\u00e9e revient et tape tout seul\", \"caption\": \"\", \"url\": \"\"}, \"id\": \"5fc5066d-aa4f-4535-aca9-56b0b6ab9d2e\"}]", "date": "2024-05-05T17:06:00Z", "live": true, "path": "0001000200030006", "slug": "des-premiers-mots", "depth": 4, "owner": 1, "title": "Des premiers mots", "locale": 1, "locked": false, "authors": [], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/