INSERT INTO public.wagtail_localize_string VALUES (52, 'b99cd4bd-5586-52c2-b26e-16d5a202d322', '<a id="a1">Vikunja</a> : Centraliser des informations sur des tous doux lisses', 1);
INSERT INTO public.wagtail_localize_string VALUES (53, '583f7caa-880d-5e82-90f8-9d9393d4dd29', 'Courriel', 1);
INSERT INTO public.wagtail_localize_string VALUES (54, 'bd896440-d4d9-5f2c-a7ee-8c8418391807', '<a id="a1">Roundcube Webmail</a> : Accéder à vos mels par le web', 1);
INSERT INTO public.wagtail_localize_string VALUES (55, 'a2ccb12b-2638-51fc-890d-10ab93b04dfe', 'Le courriel est créé dans <a id="a1">Guichet dans la partie « modifier mon profil »</a>. Chaque courriel que vous ajoutez sera un login distinct avec le même mot de passe que vous utilisez pour Guichet.', 1);
INSERT INTO public.wagtail_localize_string VALUES (56, '011f9cce-143d-5867-a259-dbc20d61f3ac', 'La <a id="a1">documentation dans Quartz</a> peut vous aider à la configuration de votre courriel (sur smartphone par exemple).', 1);
INSERT INTO public.wagtail_localize_string VALUES (57, '198487fd-307f-58af-b294-e9c8fbcbf262', 'La <a id="a1">création de compte sur Guichet</a> crée automatiquement un courriel correspondant.', 1);
INSERT INTO public.wagtail_localize_string VALUES (58, '96da24c3-3c32-5a3e-ad1e-542d8683b3fa', '<a id="a1">ListMonk</a> : Envois de Newsletters et gestions d''abonnements', 1);
