INSERT INTO public.wagtail_localize_string VALUES (48, '9f937e3d-6f74-5740-935e-3381100015dd', '<a id="a1">Quartz</a> : Consulter notre documentation sur les outils numériques (par exemple, configurer votre courriel)', 1);
