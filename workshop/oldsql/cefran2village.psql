INSERT INTO public.wagtailcore_revision VALUES (65, '2024-05-06 03:21:08.623748+00', '{"pk": 30, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"7uuv2\\\">Ce manifeste, imparfait, refl\\u00e8te les esp\\u00e9rances, les souffrances et les incompr\\u00e9hensions de ceux qui l''ont insuffl\\u00e9. Les Grands Voisins sont n\\u00e9s \\u00e0 l''h\\u00f4pital Saint-Vincent-de-Paul dans le 14e arrondissement \\u00e0 Paris, entre Port royal et les Catacombes. Nous sommes issus des failles dans le b\\u00e9ton et dans les politiques publiques. Nous sommes au service de l''accueil de tous et croyons aux ponts entre utopie et r\\u00e9alit\\u00e9.</p><p data-block-key=\\\"esgvl\\\">Comme toutes bonnes voisines et tous bons voisins, nous ne sommes pas toujours d''accord entre nous, mais nous d\\u00e9fendons :</p><ul><li data-block-key=\\\"2i4sq\\\">la parit\\u00e9 dans la gouvernance</li><li data-block-key=\\\"d2f7m\\\">des solutions collectives en bonne intelligence</li><li data-block-key=\\\"d9u2e\\\">l''expression libre</li><li data-block-key=\\\"ard5v\\\">l''exercice artistique</li><li data-block-key=\\\"1sv90\\\">l''apprentissage tout au long de la vie</li><li data-block-key=\\\"64jcl\\\">la valorisation de toutes formes de travail.</li></ul><p data-block-key=\\\"6q0us\\\">Aux Grands Voisins, nous vivons, travaillons, dormons, mangeons, nous soignons, achetons, pleurons, rions, et tout cela dans toutes les langues. L''inclusion et l''exclusion, toutes les deux, peuvent nous \\u00e9loigner de l''Autre qui est une chance.</p><p data-block-key=\\\"3khnu\\\">Nous r\\u00e9clamons des lieux de mixit\\u00e9 sociale qui permettent la valorisation et le partage des savoirs et des savoir-faire de toutes et de tous. Nous r\\u00e9clamons des lieux de mixit\\u00e9 sociale qui favorisent l''\\u00e9coute et les possibilit\\u00e9s de rencontre, conditions visant \\u00e0 l''abolition de la peur de l''autre. Nous r\\u00e9clamons des lieux de mixit\\u00e9 sociale qui luttent contre \\u00ab l''entre-soi \\u00bb, l''isolement et la r\\u00e9duction des individus \\u00e0 des cases.</p><p data-block-key=\\\"47p68\\\">Nous avons besoin de lieux et de temps pour faire pr\\u00e9c\\u00e9dent, pouvoir inventer d''autres formes de soci\\u00e9t\\u00e9 en harmonie avec les \\u00e9cosyst\\u00e8mes. Nous avons toutes et tous \\u00e0 donner et nous pouvons toutes et tous recevoir.</p>\", \"id\": \"a7fb2ebf-8e0d-401c-b11e-1929d826d8fa\"}]", "live": true, "path": "000100020004", "slug": "mission-des-grands-voisin", "depth": 3, "owner": 1, "title": "Mission des Grands Voisin", "locale": 1, "locked": false, "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/mission-des-grands-voisin/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Mission des Grands Voisins", "go_live_at": null, "draft_title": "Mission des Grands Voisin", "content_type": 25, "header_image": 48, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "64b544ea-4a64-4a1c-8221-d1c40491f993", "contentpage_tags": [], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "blue-ecume", "search_description": "Ce manifeste, imparfait, reflète les espérances, les souffrances et les incompréhensions de ceux qui l''ont insufflé. Les Grands Voisins sont nés à l''hôpital Saint-Vincent-de-Paul dans le 14e arrondissement à Paris, entre Port royal et les Catacombes. Nous sommes issus des failles dans le béton et dans les politiques publiques. Nous sommes au service de l''accueil de tous et croyons aux ponts entre utopie et réalité.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '30', 1, 25, 1, 'Mission des Grands Voisin');
INSERT INTO public.wagtailcore_revision VALUES (172, '2024-05-27 18:02:40.246409+00', '{"pk": 3, "body": "[{\"type\": \"multicolumns\", \"value\": {\"bg_image\": null, \"bg_color_class\": \"\", \"bg_color\": \"\", \"title\": \"\", \"heading_tag\": \"h2\", \"columns\": [{\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"wopaa\\\"><a linktype=\\\"page\\\" id=\\\"36\\\">22e Conseil des Voisins du 10 au 16 juin 2024</a></h3><p data-block-key=\\\"9sn6r\\\"></p><p data-block-key=\\\"pj3jf\\\"></p><embed embedtype=\\\"image\\\" format=\\\"right\\\" id=\\\"63\\\" alt=\\\"Flyer Ape\\u0301ro Boutique Ge\\u0301ome\\u0301trique et Moderne Blanc et Noir\\\"/><p data-block-key=\\\"152gd\\\"></p><p data-block-key=\\\"4jn74\\\"></p><p data-block-key=\\\"f38i5\\\">Nous tiendrions le 22e Conseil des Voisins sur trois rencontres :</p><ul><li data-block-key=\\\"sqrq\\\">Lundi le 10 juin de 18h \\u00e0 19h30</li><li data-block-key=\\\"5sba\\\">Jeudi le 13 juin de 19h \\u00e0 20h30</li><li data-block-key=\\\"fabkv\\\">Samedi le le 15 juin de 10h30 \\u00e0 midi</li></ul><p data-block-key=\\\"a6q9u\\\">Ordre du jour :</p><ol><li data-block-key=\\\"43o40\\\">Num\\u00e9rique</li><li data-block-key=\\\"oioh\\\">\\u00c9ventuellement Civisme</li><li data-block-key=\\\"7l6q8\\\">Communication</li></ol>\", \"id\": \"81d80008-6286-46b4-800e-edf8d4fce080\"}, {\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"avd09\\\"><b>Permanence le lundi de midi \\u00e0 17h</b></h3><p data-block-key=\\\"e0hvf\\\"></p><embed embedtype=\\\"image\\\" format=\\\"left\\\" id=\\\"52\\\" alt=\\\"Abaker-Atelier\\\"/><p data-block-key=\\\"5c5gg\\\"></p><p data-block-key=\\\"dqj9a\\\">Venez \\u00e0 notre permanence <b>le lundi de 12h \\u00e0 17h</b><br/>en personne \\u00e0 <b>93 ave. Du G\\u00e9n. Leclerc</b> 75014 Paris (M\\u00e9tro Al\\u00e9sia, Atelier de la Ressourcerie, <i>7e boutique sur la gauche</i>) ou <a href=\\\"https://meet.lesg/\\\">meet.lesgrandsvoisins.com pour participer en hybride</a>.</p>\", \"id\": \"460c495c-476c-43f1-bb69-aff7bf1509ea\"}, {\"type\": \"text\", \"value\": \"<h3 data-block-key=\\\"avd09\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/numerique/numerique-creatif/sociabilite-numerique/\\\"><b>Sociabilit\\u00e9 num\\u00e9rique le lundi de 17h \\u00e0 21h30</b></a></h3><embed embedtype=\\\"image\\\" format=\\\"right\\\" id=\\\"22\\\" alt=\\\"numasso\\\"/><p data-block-key=\\\"cda90\\\"><b>Le lundi de 17h \\u00e0 21h30</b> se tient un atelier d\\u2019entraide num\\u00e9rique \\u00e0 l\\u2019Atelier au <b>93 ave. du G\\u00e9n. Leclerc 75014 PARIS</b> \\u00e0 l\\u2019int\\u00e9rieur du <i>centre commercial 7e boutique sur la gauche</i></p>\", \"id\": \"6fc069f3-372a-447a-8d33-fd26389bdac8\"}]}, \"id\": \"c58f3abe-971d-41a7-ad49-f547da49dd04\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"q7an3\\\"><a href=\\\"https://list.lesgrandsvoisins.com/subscription/form\\\"><b><i>Pensez \\u00e0 vous inscrire sur notre liste d''infos !</i></b></a></p><p data-block-key=\\\"evpog\\\">Nous sommes une association d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral agissant de mani\\u00e8re contr\\u00f4l\\u00e9e en subsidiarit\\u00e9 ouverte pour toute organisation et tout individu qui en fait la d\\u00e9cision pour soi-m\\u00eame principalement en ces trois domaines <a href=\\\"https://www.lesgrandsvoisins.fr/communautes/\\\">de Discussions</a>, <a href=\\\"https://www.lesgrandsvoisins.fr/arts/\\\">d''Arts plastiques</a> et <a href=\\\"https://www.lesgrandsvoisins.fr/numerique/\\\">de Num\\u00e9rique</a> selon <a href=\\\"https://www.lesgrandsvoisins.fr/mission/\\\">notre d\\u00e9claration de mission</a>.</p><p data-block-key=\\\"bj8u6\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/communautes/\\\"><b>Discussions</b></a> :</p><ul><li data-block-key=\\\"61obe\\\">Nous proposons des conditions encourageant la compr\\u00e9hension entre individus et organisations de perspectives diff\\u00e9rentes.</li><li data-block-key=\\\"9oi9e\\\">Nous ouvrons notre gouvernance pour des votes en Conseils des Voisins.</li></ul><p data-block-key=\\\"65va7\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/arts/\\\"><b>Arts plastiques</b></a> :</p><ul><li data-block-key=\\\"a2qag\\\">Nous encourageons l\\u2019utilisation hybrides d\\u2019espace (commerces, h\\u00f4pitaux, caf\\u00e9s, etc.) aussi en gall\\u00e9ries locales.</li><li data-block-key=\\\"2ek3r\\\">Nous accompagnons des institutions pour trouver une juste place pour les arts plastiques dans leurs locaux.</li></ul><p data-block-key=\\\"91unk\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/numerique/\\\"><b>Num\\u00e9rique</b></a> :</p><ul><li data-block-key=\\\"afesu\\\">Nous maintenons une constellation de serveurs et de services internet -- tableau de bord, cr\\u00e9ation et gestion de compte, courriel, rencontres hybrides pr\\u00e9sence-distance, listes de diffusion num\\u00e9riques, blogs, serveurs webs, gestionnaire de mot de passe, enregistrement dans le nuage, forums de discussion, gestion de la connaissance, \\u00e9dition markdown, wikis, \\u00e9dition bueautique, etc. -- pour, par, et de nous-m\\u00eames et d''autres.</li><li data-block-key=\\\"ej6ca\\\">Nous mettons \\u00e0 disposition des talents et des mat\\u00e9riaux dans des salles de sociabilit\\u00e9 num\\u00e9rique et dans des syst\\u00e8mes de pr\\u00eats et \\u00e9changes de mat\\u00e9riels \\u00e0 l''attention d''individus et d''organisations de tous niveaux.</li></ul><p data-block-key=\\\"apjqo\\\">Voici quelques entit\\u00e9s Grandes Voisines qui soutiennent notre d\\u00e9marche: A Good Solution, Banya Tour, Robert Baldridge, BMCP, Breast Freedom, Le Bureau du Cr\\u00e9ateur, Le Chiffon, Les Jardins du Num\\u00e9rique, Olivier Duhec, Inno Cube, Caroline Lhomme, Francesco du 59 Rivoli, Amadou Gaye, Informatruck, Inolib, KaceKode, Ada Matus, L\\u2019Odys\\u00e9e, La Ressourcerie Cr\\u00e9ative, Henry Roy, Peter Vaeth, Waka Conseil, Weezem, et peut-\\u00eatre des v\\u00f4tres \\u2026</p><p data-block-key=\\\"at7dp\\\">Pour plus d\\u2019informations, contactez-nous\\u00a0: chris@lesgrandsvoisins.com +33 7 81 811 811</p><p data-block-key=\\\"4coel\\\"></p>\", \"id\": \"d3e4fb2e-7b28-4da0-99d3-09c2868a013f\"}, {\"type\": \"video\", \"value\": {\"title\": \"Les Grands Voisins, la cit\\u00e9 r\\u00eav\\u00e9e\", \"caption\": \"c 3, 2020 R\\u00e9alisation : Bastien Simon  Pitch : Ma\\u00ebl, artiste sans papier, Adrien, luthier musicien et d''autres r\\u00e9sidents venus de tous horizons, s''organisent pour donner naissance \\u00e0 une utopie moderne en plein coeur de Paris, un village solidaire de pr\\u00e8s de 2000 personnes : Les Grands Voisins. \\u00c0 travers leurs trajectoires et celles des membres fondateurs du lieu, le film interroge notre d\\u00e9sir et notre capacit\\u00e9 \\u00e0 inventer d\\u2019autres mani\\u00e8res de vivre ensemble. Que retiendrons-nous de cette exp\\u00e9rience collective ? Pourra-t-elle perdurer, essaimer, r\\u00e9sonner ailleurs ?\", \"url\": \"https://www.youtube.com/embed/qZqErGfNF_o?feature=oembed\"}, \"id\": \"f53195d5-0de5-479d-aef1-6151fbd266e0\"}, {\"type\": \"accordions\", \"value\": [{\"type\": \"title\", \"value\": \"Les Grands Voisins\", \"id\": \"6c6ec6eb-93af-4f4a-97e4-2f91d2498c57\"}, {\"type\": \"accordion\", \"value\": {\"title\": \"Sites Webs des Grands Voisins\", \"content\": \"<p data-block-key=\\\"h0w28\\\"><a href=\\\"https://www.lesgrandsvoisins.fr\\\">LesGrandsVoisins.fr</a> : Le site public principal des Grands Voisins</p><p data-block-key=\\\"6dm50\\\"><a href=\\\"https://www.resdigita.com\\\">ResDigita.com</a> : Le num\\u00e9rique des Grands Voisins se nomme ResDigita.</p><p data-block-key=\\\"3ntu0\\\"><a href=\\\"https://blog.lesgrandsvoisins.com\\\">Blog.LesGrandsVoisins.com</a> : Notre blog (accessible depuis le site principal)</p>\"}, \"id\": \"d2cecf58-044d-441c-849d-2d98d2085f7f\"}, {\"type\": \"accordion\", \"value\": {\"title\": \"M\\u00e9dias Sociaux des Grands Voisins\", \"content\": \"<p data-block-key=\\\"h0w28\\\"><a href=\\\"https://www.instagram.com/les_grands_voisins/\\\">Instagram@les_grands_voisins</a></p><p data-block-key=\\\"eseqg\\\">Facebook</p><p data-block-key=\\\"2vnko\\\">LinkedIn</p>\"}, \"id\": \"e02f2223-5134-4693-ac2b-639a3ec6f68a\"}], \"id\": \"03449e53-e17e-4ae3-ae63-7d9140be0b08\"}, {\"type\": \"accordions\", \"value\": [{\"type\": \"title\", \"value\": \"ResDigita, le num\\u00e9rique des Grands Voisins\", \"id\": \"05c4ba85-425d-4aa5-8176-489343275054\"}, {\"type\": \"accordion\", \"value\": {\"title\": \"Points de d\\u00e9parts\", \"content\": \"<p data-block-key=\\\"lofgf\\\"><a href=\\\"https://homepage-dashboard.resdigita.com\\\">Homepage-Dashboard</a> : le tableau de bord avec nos applications principales</p><p data-block-key=\\\"38lbi\\\"><a href=\\\"https://guichet.resdigita.com\\\">Guichet (Login)</a> : G\\u00e9rer votre compte (guichet est un logiciel d\\u00e9velopp\\u00e9 par Les Grands Voisins)</p><p data-block-key=\\\"l50c\\\"><a href=\\\"https://guichet.resdigita.com/user/new\\\">Guichet (Inscription)</a> : Cr\\u00e9er votre compte</p><p data-block-key=\\\"bkgrv\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/numerique/\\\">LesGrandsVoisins.fr/numerique</a> : La description de nos activit\\u00e9s num\\u00e9riques</p><p data-block-key=\\\"5s7rb\\\"><a href=\\\"https://quartz.resdigita.com\\\">Quartz</a> : Consulter notre documentation sur les outils num\\u00e9riques (par exemple, configurer votre courriel)</p><p data-block-key=\\\"3j715\\\"><a href=\\\"https://uptime-kuma.resdigita.com/status/gv\\\">Uptime-Kuma</a> : V\\u00e9rifier l''\\u00e9tat de nos services</p>\"}, \"id\": \"8d7c6179-ed2f-4faf-9283-7ef63fe2653d\"}, {\"type\": \"accordion\", \"value\": {\"title\": \"Applications unifi\\u00e9es\", \"content\": \"<p data-block-key=\\\"lofgf\\\"><a href=\\\"https://hedgedoc.resdigita.com\\\">Hedgedoc</a> : Edition en temps r\\u00e9el de documents partag\\u00e9s</p><p data-block-key=\\\"4u0e9\\\"><a href=\\\"https://vikunja.re