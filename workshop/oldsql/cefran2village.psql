INSERT INTO public.wagtail_localize_string VALUES (1, '421d7f2c-b560-5433-8747-581ef25e9cbe', 'SitesFaciles des Grands Voisins', 1);
INSERT INTO public.wagtail_localize_string VALUES (2, 'd6b6148d-7628-5c7b-b603-7ed96dfcb5a4', 'home', 1);
INSERT INTO public.wagtail_localize_string VALUES (3, '6b072eac-528c-50da-84b0-a1b20bd42743', 'L''association Les Grands Voisins', 1);
INSERT INTO public.wagtail_localize_string VALUES (4, 'da2c4959-9fea-5384-b9dc-d5cf0ea01794', 'Nous, aux GV, nous œuvrons pour la compréhension entre peuples qui pourraient ne pas avoir tendance à communiquer habituellement. C''est le sens même de notre nom :
<br/> <br/>« Les » comme les autres
<br/> <br/>« Grands » comme les autres sont grands
<br/> <br/>« Voisins » comme quoi les autres sont proches de nous, et nous sommes aussi des grands autres d''autres', 1);
INSERT INTO public.wagtail_localize_string VALUES (5, 'f535e6e5-ea96-5af2-866b-2ed8c306d1fa', '22e Conseil des Voisins du 10 au 16 juin 2024', 1);
INSERT INTO public.wagtail_localize_string VALUES (6, '0d545263-ecb1-51c7-9fa1-c49471a63800', 'Nous tiendrions le 22e Conseil des Voisins sur trois rencontres :', 1);
INSERT INTO public.wagtail_localize_string VALUES (7, '53f0d101-f9eb-5761-be7c-f4f271ad1876', 'Lundi le 10 juin de 18h à 19h30', 1);
INSERT INTO public.wagtail_localize_string VALUES (8, 'd1b4719f-1a2c-593c-9f0a-eb9261f2d901', 'Jeudi le 13 juin de 19h à 20h30', 1);
INSERT INTO public.wagtail_localize_string VALUES (9, '8f901b02-e493-53be-be9c-548d7c3418f3', 'Samedi le le 15 juin de 10h30 à midi', 1);
INSERT INTO public.wagtail_localize_string VALUES (10, 'dd76bfb8-ee76-596c-8aa1-f49480869af7', 'Ordre du jour :', 1);
INSERT INTO public.wagtail_localize_string VALUES (11, '6d87d2a5-594a-5754-a190-3688ae04167d', 'Numérique', 1);
INSERT INTO public.wagtail_localize_string VALUES (12, 'a2327052-9ca9-5793-8d9d-4a8e235df177', 'Éventuellement Civisme', 1);
INSERT INTO public.wagtail_localize_string VALUES (13, 'd155fb9d-2446-5bc8-8f52-26d1f7a9d47f', 'Communication', 1);
INSERT INTO public.wagtail_localize_string VALUES (14, '91bfa8d0-21e6-58f6-9d75-93549f7c71d8', 'Permanence le lundi de midi à 17h', 1);
INSERT INTO public.wagtail_localize_string VALUES (15, 'caedadd4-8baf-51fb-9050-26c89ea1225b', 'Venez à notre permanence <b>le lundi de 12h à 17h</b><br/>en personne à <b>93 ave. Du Gén. Leclerc</b> 75014 Paris (Métro Alésia, Atelier de la Ressourcerie, <i>7e boutique sur la gauche</i>) ou <a id="a1">meet.lesgrandsvoisins.com pour participer en hybride</a>.', 1);
INSERT INTO public.wagtail_localize_string VALUES (16, 'efa7ba43-3d0a-56c7-b110-8a7c460edc7b', '<a id="a1"><b>Sociabilité numérique le lundi de 17h à 21h30</b></a>', 1);
INSERT INTO public.wagtail_localize_string VALUES (17, 'eb7d9764-5632-55f2-bd2e-39b94f4d5ff1', '<b>Le lundi de 17h à 21h30</b> se tient un atelier d’entraide numérique à l’Atelier au <b>93 ave. du Gén. Leclerc 75014 PARIS</b> à l’intérieur du <i>centre commercial 7e boutique sur la gauche</i>', 1);
INSERT INTO public.wagtail_localize_string VALUES (18, '5847d25a-f7e1-52f2-8768-ad66d06996e5', '<a id="a1"><b><i>Pensez à vous inscrire sur notre liste d''infos !</i></b></a>', 1);
INSERT INTO public.wagtail_localize_string VALUES (19, 'bf8c4046-6f0d-5d63-802e-afba91d45ed4', 'Nous sommes une association d''intérêt général agissant de manière contrôlée en subsidiarité ouverte pour toute organisation et tout individu qui en fait la décision pour soi-même principalement en ces trois domaines <a id="a1">de Discussions</a>, <a id="a2">d''Arts plastiques</a> et <a id="a3">de Numérique</a> selon <a id="a4">notre déclaration de mission</a>.', 1);
INSERT INTO public.wagtail_localize_string VALUES (20, '46656e0f-b62c-5f04-9f21-1365356624f2', '<a id="a1"><b>Discussions</b></a> :', 1);
INSERT INTO public.wagtail_localize_string VALUES (21, '7f385e3e-823e-5e3c-89c9-be7d476a05e4', 'Nous proposons des conditions encourageant la compréhension entre individus et organisations de perspectives différentes.', 1);
INSERT INTO public.wagtail_localize_string VALUES (22, '4d732007-d59d-5e0e-b30a-f7a65b02b85e', 'Nous ouvrons notre gouvernance pour des votes en Conseils des Voisins.', 1);
INSERT INTO public.wagtail_localize_string VALUES (23, 'b9e331ee-8ba7-52c9-9ded-2b6b7d3c7e51', '<a id="a1"><b>Arts plastiques</b></a> :', 1);
INSERT INTO public.wagtail_localize_string VALUES (24, '12eaef79-6b95-544d-be2f-b59a7fc7d0d3', 'Nous encourageons l’utilisation hybrides d’espace (commerces, hôpitaux, cafés, etc.) aussi en galléries locales.', 1);
INSERT INTO public.wagtail_localize_string VALUES (25, 'f518273a-d81f-5a17-812d-f8e3a6f86851', 'Nous accompagnons des institutions pour trouver une juste place pour les arts plastiques dans leurs locaux.', 1);
INSERT INTO public.wagtail_localize_string VALUES (26, '15689669-825f-5455-8e13-6d3f77fc3dc7', '<a id="a1"><b>Numérique</b></a> :', 1);
INSERT INTO public.wagtail_localize_string VALUES (27, '4c04c170-dddc-5a70-a366-f0df9f37ba84', 'Nous maintenons une constellation de serveurs et de services internet -- tableau de bord, création et gestion de compte, courriel, rencontres hybrides présence-distance, listes de diffusion numériques, blogs, serveurs webs, gestionnaire de mot de passe, enregistrement dans le nuage, forums de discussion, gestion de la connaissance, édition markdown, wikis, édition bueautique, etc. -- pour, par, et de nous-mêmes et d''autres.', 1);
INSERT INTO public.wagtail_localize_string VALUES (28, '78438c49-61c4-553e-bff1-87cb7418b982', 'Nous mettons à disposition des talents et des matériaux dans des salles de sociabilité numérique et dans des systèmes de prêts et échanges de matériels à l''attention d''individus et d''organisations de tous niveaux.', 1);
INSERT INTO public.wagtail_localize_string VALUES (29, '808637d0-3624-5eb5-a7d9-9313823f62be', 'Voici quelques entités Grandes Voisines qui soutiennent notre démarche: A Good Solution, Banya Tour, Robert Baldridge, BMCP, Breast Freedom, Le Bureau du Créateur, Le Chiffon, Les Jardins du Numérique, Olivier Duhec, Inno Cube, Caroline Lhomme, Francesco du 59 Rivoli, Amadou Gaye, Informatruck, Inolib, KaceKode, Ada Matus, L’Odysée, La Ressourcerie Créative, Henry Roy, Peter Vaeth, Waka Conseil, Weezem, et peut-être des vôtres …', 1);
INSERT INTO public.wagtail_localize_string VALUES (30, '3294abc4-3005-53c6-8721-b21822e3b23d', 'Pour plus d’informations, contactez-nous : chris@lesgrandsvoisins.com +33 7 81 811 811', 1);
INSERT INTO public.wagtail_localize_string VALUES (31, '643f4e32-ae23-5421-9bd5-cae3bcd2141a', 'Les Grands Voisins, la cité rêvée', 1);
INSERT INTO public.wagtail_localize_string VALUES (32, '5bb4cab3-d1db-53a7-85d8-d9b10bc4f290', 'c 3, 2020 Réalisation : Bastien Simon  Pitch : Maël, artiste sans papier, Adrien, luthier musicien et d''autres résidents venus de tous horizons, s''organisent pour donner naissance à une utopie moderne en plein coeur de Paris, un village solidaire de près de 2000 personnes : Les Grands Voisins. À travers leurs trajectoires et celles des membres fondateurs du lieu, le film interroge notre désir et notre capacité à inventer d’autres manières de vivre ensemble. Que retiendrons-nous de cette expérience collective ? Pourra-t-elle perdurer, essaimer, résonner ailleurs ?', 1);
INSERT INTO public.wagtail_localize_string VALUES (33, 'c6b13099-df8f-5758-8e0c-22dbb512f4c7', 'Les Grands Voisins', 1);
INSERT INTO public.wagtail_localize_string VALUES (34, '622e5c8c-55bb-5f46-8d87-6f4192220913', 'Sites Webs des Grands Voisins', 1);
