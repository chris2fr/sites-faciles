INSERT INTO public.sites_faciles_contentpage VALUES (108, '[{"id": "2bb8edae-aac8-4208-bb68-677a9e5e0b00", "type": "callout", "value": {"text": "Proposition de communiqué pour la Délégation permanente de la République Islamique de Mauritanie à l’UNESCO à l’occaison du retour des Etats Unis par l’artiste Mohamed Maël Aïnine Sidi Ethmane.", "title": "", "heading_tag": "h3"}}, {"id": "a1d22914-8142-4f56-8195-acceaa062969", "type": "paragraph", "value": "<p data-block-key=\"vq1lp\">Paris, le 14 juitllet 2023</p><p data-block-key=\"bc251\">Maël A.N.C.<br/>22 Rue Duranton,<br/>75015 Paris,</p><p data-block-key=\"eq7hr\">Proposition de communiqué pour la  Délégation permanente de la République Islamique de Mauritanie à l’UNESCO à l’occaison du retour des Etats Unis par l’artiste Mohamed Maël Aïnine Sidi Ethmane.</p>"}, {"id": "1ff0b627-f8af-403a-86ee-7c48dceeca4d", "type": "separator", "value": {"top_margin": 3, "bottom_margin": 3}}, {"id": "a5eb0b1f-d5a6-45ff-ba4a-04b697cfc3ad", "type": "paragraph", "value": "<p data-block-key=\"vq1lp\">La Délégation permanente de la République Islamique de Mauritanie à l''UNESCO</p><p data-block-key=\"1ib9p\"></p><embed embedtype=\"image\" format=\"fullwidth\" id=\"13\" alt=\"\"/><p data-block-key=\"ebc01\"></p><p data-block-key=\"abvac\">félicite avec gratitude les États Unis d’Amérique</p><p data-block-key=\"405oi\"></p><embed embedtype=\"image\" format=\"fullwidth\" id=\"14\" alt=\"flag-usa.pmg\"/><p data-block-key=\"6vria\">de leur retour historique à L’UNESCO</p><p data-block-key=\"fan0p\"></p><embed embedtype=\"image\" format=\"fullwidth\" id=\"12\" alt=\"\"/><p data-block-key=\"8hpaf\">Nous sommes fiers et conquis par l’excellent truchement politique de bon aloi de la Directrice Générale de l’UNESCO, Madame Audrey Azouley, qui a dignement conduit la réussite de cet évènement majeur dans l’histoire de notre organisation onusienne.</p><p data-block-key=\"d2qnj\">Nous nous joignons à la solidarité inconditionnelle de tous les États membres de L’UNESCO pour accueillir ce grand retour de nos amis communs des U.S.A.</p><p data-block-key=\"55kfk\">Merci infiniment, très cordialement.</p><p data-block-key=\"24unt\"></p>"}, {"id": "50667e55-09ac-4afe-925a-4a699179611e", "type": "separator", "value": {"top_margin": 3, "bottom_margin": 3}}, {"id": "3d6b83ce-62cb-4b15-b0ab-13ab6c9a4a5f", "type": "paragraph", "value": "<p data-block-key=\"vq1lp\">Je vous prie de considérer cette expression avec bienveillance.</p><p data-block-key=\"fqr5o\">Maël A.N.C.<br/>mael@maelanc.com – <a href=\"http://www.maelanc.com/?ref=blog.lesgrandsvoisins.com\">www.maelanc.com</a></p>"}]', ,, 12, 'red-marianne', NULL, NULL, true, true, true, NULL));
