INSERT INTO public.wagtailcore_revision VALUES (21, '2024-05-05 16:47:37.545957+00', '{"pk": 8, "body": "[]", "date": "2023-07-17T13:00:00Z", "live": true, "path": "0001000200030002", "slug": "communique-mauritanie-unesco", "depth": 4, "owner": 1, "title": "Proposition de communiqué pour la Mauritanie à l’UNESCO", "locale": 1, "locked": false, "authors": [2], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/communique-mauritanie-unesco/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Proposition de communiqué pour la Délégation permanente de la République Islamique de Mauritanie à l’UNESCO", "entry_tags": [], "go_live_at": null, "draft_title": "Proposition de communiqué pour la Mauritanie à l’UNESCO", "content_type": 71, "header_image": 12, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [1, 3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "02059326-e36f-4bd5-b4ae-a29a560e42f4", "entry_categories": [{"pk": 2, "page": 8, "category": 1}, {"pk": 3, "page": 8, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "red-marianne", "search_description": "Proposition de communiqué pour la Délégation permanente de la République Islamique de Mauritanie à l’UNESCO à l’occaison du retour des Etats Unis par l’artiste Mohamed Maël Aïnine Sidi Ethmane.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '8', 1, 71, 1, 'Proposition de communiqué pour la Mauritanie à l’UNESCO');
INSERT INTO public.wagtailcore_revision VALUES (24, '2024-05-05 17:02:55.852761+00', '{"pk": 10, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"wlknb\\\">L\\u2019association Les Grands Voisins se joint \\u00e0 l\\u2019association d\\u2019utilit\\u00e9 publique Le Souvenir Fran\\u00e7ais et \\u00e0 l\\u2019association Polaris 14 pour \\u00a0exprimer nos condol\\u00e9ances et comm\\u00e9morer l\\u2019honneur de Valentin FRANCY, mort assassin\\u00e9 de 18 coups de couteau le 2 mars 2023. Valentin FRANCY et son assassin \\u00e9taient b\\u00e9n\\u00e9voles dans plusieurs associations. Notre association a connu le b\\u00e9n\\u00e9volat sporadique de l\\u2019assassin entre novembre 2022 et mars 2023.</p><p data-block-key=\\\"c50ut\\\">La mis\\u00e8re, l\\u2019alcool\\u00e9mie, la migration, l\\u2019ins\\u00e9curit\\u00e9, sont des sujets auxquels nous faisons face dans nos associations. Les engagements dans des associations de Valentin incluaient la cr\\u00e9ation d''une biographie des SDF d\\u00e9c\\u00e9d\\u00e9s, l''aide alimentaire aux personnes dans le besoin, la participation aux c\\u00e9r\\u00e9monies de comm\\u00e9moration nationale, et la sensibilisation des \\u00e9tudiants par le biais de r\\u00e9dactions journalistiques.</p><p data-block-key=\\\"1appn\\\">Une bonne partie du service social fran\\u00e7ais repose sur les \\u00e9paules du milieu associatif. Le service social, m\\u00eame en sources priv\\u00e9es, ne s\\u2019improvise pas. N\\u00e9anmoins, le monde change, la France change, et nous devons aussi ajuster.</p><p data-block-key=\\\"9co44\\\">J\\u2019imagine que Valentin FRANCY voulait humaniser l\\u2019aide aux personnes marginalis\\u00e9es. Il ne connaissait peut-\\u00eatre pas suffisamment les dangers que cette activit\\u00e9 comporte.</p><p data-block-key=\\\"5p5lj\\\">Valentin FRANCY aurait eu aussi une initiative personnelle d\\u2019h\\u00e9berger des personnes marginalis\\u00e9es dans sa chambre de bonne dans le 16e arrondissement de Paris. Le meurtrier m\\u2019avait demand\\u00e9 dans la semaine qui pr\\u00e9c\\u00e9dait le 2 mars 2023 de l\\u2019h\\u00e9berger. Il est venu \\u00e0 mon domicile la seule fois je pense le 1er mars. Il \\u00e9tait \\u00e9puis\\u00e9, s\\u2019est repos\\u00e9 et est reparti.</p><p data-block-key=\\\"chask\\\">Je sais o\\u00f9 commence et o\\u00f9 s\\u2019arr\\u00eate l\\u2019activit\\u00e9 de l\\u2019association Les Grands Voisins. Je connais aussi les limites de mon engagement en son sein. Une clart\\u00e9 de ma mission m\\u2019a permis d\\u2019indiquer au b\\u00e9n\\u00e9vole futur criminel que je ne pouvais pas l\\u2019h\\u00e9berger. En tant qu\\u2019association, la responsabilit\\u00e9 est autant aux b\\u00e9n\\u00e9voles qu\\u2019aux b\\u00e9n\\u00e9ficiaires.</p><p data-block-key=\\\"4kobt\\\">A Madagascar, le meurtrier avait \\u00e9t\\u00e9 identifi\\u00e9 par une ambassade et une fondation fran\\u00e7aise comme un entrepreneur avec un certain potentiel professionnel. Aujourd\\u2019hui, le meurtrier a confess\\u00e9 et il est en h\\u00f4pital psychiatrique. Si jamais il sort de l\\u2019h\\u00f4pital, il sera jug\\u00e9 pour son crime.</p><p data-block-key=\\\"fh112\\\">La migration am\\u00e8ne une diversit\\u00e9 \\u00e0 toute nation. Une diversit\\u00e9 am\\u00e8ne une robustesse. La robustesse am\\u00e8ne de la r\\u00e9silience.</p><p data-block-key=\\\"evqp\\\">Je connais de premi\\u00e8re main la migration. Mes grands-parents maternels \\u00e9taient des soldats pendant la Seconde Guerre mondiale : capitaine Jim JAMESON sous le G\\u00e9n\\u00e9ral Patton et Betty MORRIS, soldate dans l\\u2019arm\\u00e9e du Mar\\u00e9chal Montgomery. Betty MORRIS \\u00e9tait la premi\\u00e8re \\u00ab \\u00e9pouse de guerre \\u00bb de la Seconde Guerre mondiale \\u00e0 venir aux Etats Unis. Elle a d\\u00fb rester un certain temps en Angleterre pour avoir les autorisations n\\u00e9cessaires pour s\\u2019installer dans le Long Island. Mon immigration en France me coupe d\\u2019une bonne partie de mes rep\\u00e8res.</p><p data-block-key=\\\"8oir2\\\">Je me d\\u00e9die \\u00e0 l\\u2019honneur de Valentin FRANCY et \\u00e0 mes responsabilit\\u00e9s envers les b\\u00e9n\\u00e9voles et les b\\u00e9n\\u00e9ficiaires de l\\u2019association Les Grands Voisins et dans mes autres engagements associatifs. Je souhaiterais continuer avec vous un dialogue autour de la m\\u00e9moire et de l\\u2019honneur de Valentin FRANCY d\\u2019ici au 2 mars 2025.</p><p data-block-key=\\\"eamp6\\\">Chris MANN<br/>Pr\\u00e9sident de l\\u2019association Les Grands Voisins (<a href=\\\"http://lesgrandsvoisins.fr/?ref=blog.lesgrandsvoisins.com\\\">lesgrandsvoisins.fr</a>)<br/>le 2 mars 2024</p>\", \"id\": \"1a6516df-034b-4efd-aa15-4265c98066c2\"}]", "date": "2024-03-02T08:00:00Z", "live": true, "path": "0001000200030004", "slug": "a-la-memoire-de-valentin-francy", "depth": 4, "owner": 1, "title": "A la Mémoire de Valentin FRANCY", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/a-la-memoire-de-valentin-francy/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "A la Mémoire de Valentin FRANCY", "entry_tags": [{"pk": 10, "tag": 15, "content_object": 10}], "go_live_at": null, "draft_title": "A la Mémoire de Valentin FRANCY", "content_type": 71, "header_image": 16, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "2aba00a1-257c-40fe-a15e-7058bd21f2d4", "entry_categories": [{"pk": 5, "page": 10, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "blue-france", "search_description": "L’association Les Grands Voisins se joint à l’association d’utilité publique Le Souvenir Français et à l’association Polaris 14 pour exprimer nos condoléances et commémorer l’honneur de Valentin FRANCY. Cette tragédie nous apporte des leçons. Son honneur nous apporte inspiration.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '10', 1, 71, 1, 'A la Mémoire de Valentin FRANCY');
INSERT INTO public.wagtailcore_revision VALUES (109, '2024-05-08 14:52:30.118149+00', '{"pk": 38, "body": "[{\"type\": \"paragraph\", \"value\": \"<h2 data-block-key=\\\"ptpk7\\\">Coop Application</h2><p data-block-key=\\\"8dc37\\\"><a href=\\\"https://www.lgv.coop/?ref=blog.lesgrandsvoisins.com\\\"><b>Les Grands Voisins</b></a> est une association fran\\u00e7aise de loi 1901 g\\u00e9n\\u00e9rique avec une approche tr\\u00e8s sp\\u00e9cifique codifi\\u00e9e dans nos statuts. Nous demandons le statut de coop\\u00e9rative sur la base de notre adh\\u00e9sion structurelle aux sept principes coop\\u00e9ratifs. Le pouvoir de notre engagement \\u00e0 ces principes r\\u00e9side \\u00e9galement dans le contr\\u00f4le de la communaut\\u00e9, puisque nous baserons toute notre existence en ligne sur le domaine <a href=\\\"http://lgv.coop/?ref=blog.lesgrandsvoisins.com\\\">lgv.coop</a>, permettant ainsi un recours \\u00e0 vous quiconque d\\u00e9montre raisonnablement que nous ne respectons pas ces principes. Notre engagement est plus fort que celui d\\u2019une coop\\u00e9rative commerciale, car nous sommes une association coop\\u00e9rative, et nous prenons ce statut tr\\u00e8s au s\\u00e9rieux. Voici ma description de notre engagement envers les sept principes. Je m\\u2019en remets \\u00e0 vous pour les accepter. J\\u2019ai \\u00e9cout\\u00e9 les autres membres, bien s\\u00fbr, et nous finaliserons notre transition vers <a href=\\\"http://lgv.coop/?ref=blog.lesgrandsvoisins.com\\\">lgv.coop</a>, si vous l\\u2019approuvez, lors de notre prochaine r\\u00e9union du Conseil Open LGV le samedi 25 f\\u00e9vrier. Vous trouverez ci-dessous mon adresse aux Sept Principaux.</p><p data-block-key=\\\"bcpsl\\\">Nous sommes pleinement engag\\u00e9s envers <a href=\\\"https://www.localglobalvillage.org/en/?ref=blog.lesgrandsvoisins.com#good-villagersand39-manifesto\\\">notre propre d\\u00e9claration de mission</a>, le <a href=\\\"https://contractfortheweb.org/?ref=blog.lesgrandsvoisins.com\\\">contrat pour le web</a>, les <a href=\\\"https://identity.coop/co-op-principles/?ref=blog.lesgrandsvoisins.com\\\">sept principes coop\\u00e9ratifs</a>, et en g\\u00e9n\\u00e9ral envers notre responsabilit\\u00e9 envers tous les membres et utilisateurs de nos services.</p><p data-block-key=\\\"bnjq7\\\">Nous maintenons en permanence sur notre site web nos engagements structurels explicites envers les <a href=\\\"https://identity.coop/co-op-principles/?ref=blog.lesgrandsvoisins.com\\\">sept principes coop\\u00e9ratifs</a>. Les voici :</p><ol><li data-block-key=\\\"e9ms9\\\">Adh\\u00e9sion volontaire et ouverte<br/>Nous consid\\u00e9rons comme membre tout \\u00eatre humain physique qui se consid\\u00e8re comme tel.</li><li data-block-key=\\\"buv7t\\\">Contr\\u00f4le d\\u00e9mocratique des membres<br/>Nous avons trois groupes de vote, chacun ayant un pouvoir de vote \\u00e9gal. Au sein de chaque groupe, chaque \\u00eatre humain physique dispose d\\u2019une voix \\u00e9gale.</li><li data-block-key=\\\"6j89s\\\">Participation \\u00e9conomique des membres<br/>Les membres contribuent \\u00e0 la LGV aux niveaux qu\\u2019ils choisissent, mais nous pouvons les conseiller. Tous les niveaux sont accept\\u00e9s, mais sans contributions financi\\u00e8res, la LGV n\\u2019existe pas, alors n\\u2019h\\u00e9sitez pas \\u00e0 contribuer. Les entreprises, les organismes \\u00e0 but non lucratif et les entit\\u00e9s publiques peuvent apporter des fonds, mais ne sont repr\\u00e9sent\\u00e9s que par des personnes physiques qui ont un droit de vote \\u00e9gal.</li><li data-block-key=\\\"ek4o0\\\">Autonomie et ind\\u00e9pendance<br/>Nous sommes enti\\u00e8rement responsables devant nos membres, sans crainte ni faveur entre chacun d\\u2019entre eux.</li><li data-block-key=\\\"2v47d\\\">\\u00c9ducation, formation et information<br/>Nous sommes pleinement engag\\u00e9s dans l\\u2019\\u00e9conomie du partage, comme le montre <a href=\\\"https://www.localglobalvillage.org/en/?ref=blog.lesgrandsvoisins.com#good-villagersand39-manifesto\\\">notre propre d\\u00e9claration de mission</a>. Nous pensons \\u00e9galement que la fa\\u00e7on dont nous faisons quelque chose fait partie de ce que nous faisons. En enseignant et en apprenant, nous consolidons nos comp\\u00e9tences et notre pertinence.</li><li data-block-key=\\\"9qh2i\\\">Coop\\u00e9ration entre coop\\u00e9ratives<br/>Nous rendons le meilleur service en suivant de bons exemples et en donnant un bon exemple \\u00e0 copier aux autres. Nous nous effor\\u00e7ons d\\u2019apporter notre aide de la mani\\u00e8re qui nous est demand\\u00e9e dans le cadre d\\u2019un dialogue constructif. Nous accueillons toute coop\\u00e9rative qui ferait partie de notre SitesFaciles Local-Global.</li><li data-block-key=\\\"ahbk8\\\">Le souci de la Communaut\\u00e9<br/>Nous allons au-del\\u00e0 de l\\u2019inclusion \\u00e0 LGV. Nous comprenons qu\\u2019il existe un endroit au-del\\u00e0 de l\\u2019inclusion et de l\\u2019exclusion. Tout ce que nous faisons est connu et vot\\u00e9 au conseil. Nous faisons ce que nous pouvons pour partager efficacement, surtout lorsque c\\u2019est inconfortable.</li></ol><p data-block-key=\\\"3h57k\\\">Je crois fermement que nous sommes dans la premi\\u00e8re cat\\u00e9gorie, b\\u00e9n\\u00e9ficiant du mouvement coop\\u00e9ratif et y contribuant. <i>Le mouvement coop\\u00e9ratif est tellement n\\u00e9cessaire pour nos vies num\\u00e9riques</i>.</p><p data-block-key=\\\"355v\\\">En France, je crois savoir que le statut officiel de coop\\u00e9rative est bas\\u00e9 exclusivement sur une entreprise \\u00e0 but lucratif. Je suis convaincu que les organisations \\u00e0 but non lucratif peuvent aussi \\u00eatre des coop\\u00e9ratives. La d\\u00e9finition des coop\\u00e9ratives en France peut permettre aux \\u201ccoop\\u00e9ratives\\u201d de s\\u2019\\u00e9loigner des <a href=\\\"https://identity.coop/co-op-principles/?ref=blog.lesgrandsvoisins.com\\\">sept principes coop\\u00e9ratifs</a>.</p><h2 data-block-key=\\\"8ao6h\\\">Agr\\u00e9ment en tant que coop\\u00e9ratif</h2><p data-block-key=\\\"6dtnr\\\">Dear Les Grands Voisins,</p><p data-block-key=\\\"a15ce\\\">Your eligibility for registering a .coop domain has been verified. No further action on your part is needed.</p><p data-block-key=\\\"607jj\\\">If you have any queries, please email verification@identity.coop.</p><p data-block-key=\\\"admg6\\\">Thank you for your support of .COOP!</p><p data-block-key=\\\"9a7uq\\\">Kind regards,</p><p data-block-key=\\\"44de2\\\">DotCoop Verification Team</p>\", \"id\": \"54d3f970-9eb0-4066-8766-8119b4413dfd\"}]", "date": "2023-02-14T11:00:00Z", "live": true, "path": "000100020003000P", "slug": "obtention-du-statut-cooperative", "depth": 4, "owner": 1, "title": "Obtention du statut \"Coopérative\"", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/obtention-du-statut-cooperative/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Obtention du statut \"Coopérative\"", "entry_tags": [{"pk": 76, "tag": 38, "content_object": 38}, {"pk": 77, "tag": 15, "content_object": 38}], "go_live_at": null, "draft_title": "Obtention du statut \"Coopérative\"", "content_type": 71, "header_image": 56, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "cb3bd8ff-df81-404d-a7ea-d8cce289b702", "entry_categories": [{"pk": 27, "page": 38, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "pink-macaron", "search_description": "Les Grands Voisins sont reconnu sur le plan international en tant que coopératif, bien qu''une association coopérative n''existe pas au sens de la législation française.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '38', 1, 71, 1, 'Obtention du statut "Coopérative"');
INSERT INTO public.wagtailcore_revision VALUES (112, '2024-05-08 16:26:18.135337+00', '{"pk": 41, "body": "[{\"type\": \"paragraph\", \"value\": \"<h2 data-block-key=\\\"ketc7\\\">Ordre du Jour</h2><p data-block-key=\\\"fgi40\\\">&gt;1. Se poser et se pr\\u00e9senter</p><p data-block-key=\\\"6sse2\\\">&gt;2. Actu de la communication (<a href=\\\"http://www.commentwowmaintenant.com/?ref=blog.lesgrandsvoisins.com\\\">www.commentwowmaintenant.com</a>)</p><ul><li data-block-key=\\\"6f5j\\\">Nouveau site web <a href=\\\"http://www.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">www.lesgrandsvoisins.com</a></li><li data-block-key=\\\"4hcvu\\\">Reconnaissance en tant que coop\\u00e9rative pour <a href=\\\"http://www.lgv.coop/?ref=blog.lesgrandsvoisins.com\\\">www.lgv.coop</a></li><li data-block-key=\\\"ej7tp\\\">Utilisation de <a href=\\\"http://agoodvillage.com/?ref=blog.lesgrandsvoisins.com\\\">agoodvillage.com</a> en anglais</li><li data-block-key=\\\"ap9l0\\\">Utilisation de noms de domaine en terminaison .com (sauf <a href=\\\"http://lgv.coop/?ref=blog.lesgrandsvoisins.com\\\">lgv.coop</a>)</li></ul><p data-block-key=\\\"89brj\\\">&gt;3. Actu sur les activit\\u00e9s (<a href=\\\"http://www.communititude.com/?ref=blog.lesgrandsvoisins.com\\\">www.communititude.com</a>)</p><ul><li data-block-key=\\\"9iu5p\\\">Lancement des Popup Expos (<a href=\\\"http://www.popup-expos.com/?ref=blog.lesgrandsvoisins.com\\\">www.popup-expos.com</a>)</li><li data-block-key=\\\"4jlfe\\\">Lancement de la salle de sociabilit\\u00e9 num\\u00e9rique (<a href=\\\"http://www.resdigita.com/?ref=blog.lesgrandsvoisins.com\\\">www.resdigita.com</a>)</li><li data-block-key=\\\"126rl\\\">Ateliers sur le num\\u00e9rique des Grands Voisins et ouverture de services (<a href=\\\"http://www.resdigita.com/?ref=blog.lesgrandsvoisins.com\\\">www.resdigita.com</a>)</li><li data-block-key=\\\"etvnb\\\">Orientation vers un observatoire ou un comit\\u00e9 scientifique</li><li data-block-key=\\\"dm13\\\">Lettre aux \\u00e9lues au sujet du tiers lieux Les Arches Citoyennes</li><li data-block-key=\\\"31fja\\\">Les Grands Voisins en Turquie ?</li></ul><p data-block-key=\\\"av5en\\\">&gt;6. Mots de la fin</p><h3 data-block-key=\\\"89ous\\\">Items \\u00e9cart\\u00e9s</h3><p data-block-key=\\\"dl1qf\\\">Les items suivants n''ont pas pu \\u00eatre trait\\u00e9 pour manque de temps.</p><p data-block-key=\\\"7mdiu\\\">&gt;4. Actualit\\u00e9s sur les Voisins (<a href=\\\"http://www.quiquoietc.com/?ref=blog.lesgrandsvoisins.com\\\">www.quiquoietc.com</a>)</p><ul><li data-block-key=\\\"9bmt2\\\">Un besoin de comp\\u00e9tences en communication</li><li data-block-key=\\\"8c052\\\">Un processus d\\u2019inscription</li></ul><p data-block-key=\\\"7dk4a\\\">&gt;5. Actualit\\u00e9s sur la vie de l\\u2019association coop\\u00e9rative (<a href=\\\"http://www.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">www.lesgrandsvoisins.com</a>)</p><ul><li data-block-key=\\\"atspg\\\">R\\u00e9gularisation d\\u2019inscription d\\u2019association loi 1901</li><li data-block-key=\\\"c7960\\\">Budget et ouverture de compte en cours</li><li data-block-key=\\\"3lkpc\\\">Candidature aupr\\u00e8s de Plateau Urbain et recherche de locaux</li><li data-block-key=\\\"egvs2\\\">Agression verbale et menace d\\u2019un directeur d\\u2019Aurore</li></ul><h2 data-block-key=\\\"794pn\\\">D\\u00e9roulement</h2><h3 data-block-key=\\\"9qmdu\\\">1. Tour de table</h3><p data-block-key=\\\"bques\\\">Lors de la Tour de table se sont pr\\u00e9sent\\u00e9s en ordre Ma\\u00ebl, Philippe Lemeur, Sviatlana, Chris, Bruno, Claudie, Philippe Lemeur, Foreman, Pourya.</p><p data-block-key=\\\"1ovk8\\\">Les associations suivantes ont \\u00e9t\\u00e9 pr\\u00e9sent\\u00e9es: Second Souffle, Les Amis du 20e, et une autre. Nous sommes particuli\\u00e8rement reconnaissants \\u00e0 Union Jeuneuse Internationale et Les Oiseaux Migratoires pour leur accueil.</p><h3 data-block-key=\\\"40bmr\\\">2. Communication</h3><p data-block-key=\\\"ce7l3\\\">Pr\\u00e9sentation des nouveaux sites :</p><ul><li data-block-key=\\\"2junp\\\">lesgrandsvoisins.com / agoodvillage.com</li><li data-block-key=\\\"c9rpa\\\">lgv.coop</li><li data-block-key=\\\"7vib9\\\">quiquoietc.com / whowhatetc.com</li><li data-block-key=\\\"1bk07\\\">village.tel (maintenant probablement resdigita.com)</li><li data-block-key=\\\"qd8i\\\">popupos.com</li></ul><p data-block-key=\\\"88bcj\\\">Information sur l''agr\\u00e9ment identity.coop.</p><h3 data-block-key=\\\"f3g32\\\">3. Activit\\u00e9s</h3><h4 data-block-key=\\\"3arna\\\">3.1. Popupos</h4><p data-block-key=\\\"3t5rr\\\">Nous avons pr\\u00e9sent\\u00e9 les Popup Expos (popupos.com) par <a href=\\\"https://old.lesgrandsvoisins.com/documents/5/Pop_Up_Expos-Pr%C3%A9sentation_Le_Bureau_du_Cr%C3%A9ateur.pdf?ref=blog.lesgrandsvoisins.com\\\">un document du Bureau du Createur</a>. Marine du Bureau du Cr\\u00e9ateur \\u00e9tant absente, Chris a fait la pr\\u00e9sentation. Ma\\u00ebl co-anime les Popupos avec Marine et d''autres ont ajout\\u00e9 les observations suivantes.</p><ul><li data-block-key=\\\"1kg7a\\\">Apporter un cadre claire</li><li data-block-key=\\\"ao3mv\\\">Aider l''artiste \\u00e0 vendre</li><li data-block-key=\\\"83tuo\\\">Lister des artistes</li><li data-block-key=\\\"9l6la\\\">Lister les oeuvres des artistes (titre, dimensions, photo, etc.)</li><li data-block-key=\\\"6r28f\\\">Faire des expos collectifs (il y a par exemple 17 oeuvres expos\\u00e9es au-dessus)</li><li data-block-key=\\\"2njfn\\\">Penser \\u00e0 faire la suite des Arts Voisins (Gall\\u00e9rie aux Grands Voisins dont Ma\\u00ebl a \\u00e9t\\u00e9 au coeur)</li><li data-block-key=\\\"eutkj\\\">Faire au nom des Grands Voisins .com</li><li data-block-key=\\\"ait2m\\\">Communiquer, Faire de la visibilit\\u00e9, Penser aux canaux de diffusion</li><li data-block-key=\\\"5omss\\\">Permettre un pourcentage de vente au commer\\u00e7ant</li><li data-block-key=\\\"9ruji\\\">Apporter un visibilit\\u00e9</li><li data-block-key=\\\"abo1s\\\">A quelle \\u00e9chelle est-ce maintenant ? Dans un avenir proche ? Lointain ?</li><li data-block-key=\\\"7e9fa\\\">Faire de la porte \\u00e0 porte avec un document</li><li data-block-key=\\\"es6m2\\\">Faire de la communication croiser, interpeller les gens / populations</li><li data-block-key=\\\"1gl19\\\">Contacter une ressourcerie dans le 18e</li><li data-block-key=\\\"aqjj\\\">D\\u00e9finir un p\\u00e9rim\\u00e8tre et faire prendre la mayonnaise</li><li data-block-key=\\\"3qv9a\\\">Travailler des diff\\u00e9rences, penser au broadcasts</li><li data-block-key=\\\"cgm4l\\\">Faire des reportages sur les expos (1 par semaine?)</li></ul><h4 data-block-key=\\\"l1vp\\\">3.2. Salle de sociabilit\\u00e9 num\\u00e9rique</h4><p data-block-key=\\\"eah42\\\">Le budget participatif de 25.000\\u20ac est obtenu pour un investissement mat\\u00e9riel. Un travail d\\u00e9marre avec la maison des association du 14e en mars.</p><p data-block-key=\\\"9ob26\\\">Prendre en compte les commentaires d''Audrey Azoulay de l''UNESCO sur la p\\u00e9dagogie et la jeunesse, les enfants et le fait d''\\u00e9viter les insultes.</p><p data-block-key=\\\"8kn7o\\\">Promouvoir un lien interg\\u00e9n\\u00e9rationnel. Mener un r\\u00e9flexion sur ce travail. Chris et Tanagra m\\u00e8nent l''effort.</p><p data-block-key=\\\"fkg4t\\\">Penser aux b\\u00e9n\\u00e9voles \\u00e0 recruter. Est-ce que la salle est mobile ?</p><p data-block-key=\\\"9lodb\\\">Penser \\u00e0 finir des dispositifs pour qu''ils soient utilisables par les Grands Voisins. Quelles sont les bases d''utilisation et d''utilisateurs.</p><p data-block-key=\\\"atr7j\\\">Sviatlana aidera Chris sur la mise en priorit\\u00e9 des travaux. J''ai not\\u00e9 quelque chose sur la pens\\u00e9e.</p><p data-block-key=\\\"33j46\\\">Il a aussi \\u00e9t\\u00e9 question du <a href=\\\"https://www.digisemble.com/fr/?ref=blog.lesgrandsvoisins.com\\\">nom attirant pour les activit\\u00e9s num\\u00e9riques</a> des Grands Voisins, un nom qui chapeautera aussi la Salle de sociabilisation num\\u00e9rique. Les deux propositions ont \\u00e9t\\u00e9 mises \\u00e0 voix (votes pouvant \\u00eatre exprim\\u00e9s jusqu''\\u00e0 dimanche 26 f\\u00e9vrier minuit):</p><ol><li data-block-key=\\\"512kp\\\">ResDigita.com</li><li data-block-key=\\\"ehk6j\\\">SitesFaciles.tel (ou telvillage.com ou villagetel.com pour \\u00e9viter des confusions)</li></ol><p data-block-key=\\\"7shbl\\\">Coll\\u00e8ge A : SitesFaciles.tel</p><p data-block-key=\\\"fc9pe\\\">Coll\\u00e8ge B : ResDigita.com (2 pour ResDigita, 1 pour SitesFaciles.tel)</p><p data-block-key=\\\"f22sa\\\">Coll\\u00e8ge C : ResDigita.com (4 pour ResDigita, 2 pour SitesFaciles.tel, 2 absentions)</p><h4 data-block-key=\\\"obd6\\\">3.3. Cr\\u00e9ation d''observatoire ou comit\\u00e9 scientifique</h4><p data-block-key=\\\"2c1bf\\\">Le Conseil des Voisins accorde le principe et oriente un groupe pour r\\u00e9fl\\u00e9chir de Tanagra, Chris et Ma\\u00ebl pour proposer des objectifs, un p\\u00e9riode et un p\\u00e9rim\\u00e8tre pour le prochain Conseil des Voisins. Le travail de ce groupe sera \\u00e9galement sur le forum.</p><p data-block-key=\\\"1ovn2\\\">C''est bien pour les GV et c''est bien pour la soci\\u00e9t\\u00e9 civile.</p><h4 data-block-key=\\\"e5jrn\\\">3.4. Les Grands Voisins avec d''autres pays (la Turquie, etc.)</h4><p data-block-key=\\\"amabi\\\">Si d''autres pays veulent faire Grands Voisins, oui, mais nous n''allons pas les chercher.</p><p data-block-key=\\\"52bq0\\\">On consolide d''abord le mod\\u00e8le en France.</p><p data-block-key=\\\"78lbt\\\">Je ressens du potentiel. Je pense qu''il y a un besoin. J''aimerais voir connecter les gens et le besoin. Il convient d''orienter un travail dans ce sens.</p><p data-block-key=\\\"2ak1g\\\">La force et la faiblesse des Grands Voisins est que \\u00e7a concerne tout le monde.</p><p data-block-key=\\\"fc71b\\\">Quel d\\u00e9lai ? Comment commencer \\u00e0 faire vivre ?</p><p data-block-key=\\\"1ouaj\\\">Les gens peuvent avoir du mal \\u00e0 se retrouver. Il faut un travail de communication. Envergure.</p><h3 data-block-key=\\\"509nh\\\">4. et 5. Ecart\\u00e9s</h3><p data-block-key=\\\"fkctj\\\">Les sujets 4 et 5 sont \\u00e9cart\\u00e9s pour ne pas prolonger la r\\u00e9union. Tenons les r\\u00e9unions \\u00e0 1h15 nous permettra d''en faire plus souvent.</p><h3 data-block-key=\\\"1fdd1\\\">6. Mots de la fin</h3><p data-block-key=\\\"4u8n5\\\">Bien que les commentaires sont prises en compte, les mots de la fin ne font pas partie du CR, s''agissant de la nature et la libert\\u00e9 d''expression.</p><p data-block-key=\\\"11riq\\\">[En voici un fichier <a href=\\\"https://old.lesgrandsvoisins.com/documents/7/2023-03-25-conseil.pdf?ref=blog.lesgrandsvoisins.com\\\">PDF A4 </a>et un fichier <a href=\\\"https://old.lesgrandsvoisins.com/documents/8/2023-03-25-conseil-livret.pdf?ref=blog.lesgrandsvoisins.com\\\">PDF A4 Livret</a> de ce CR.]</p><h2 data-block-key=\\\"aavj5\\\">Invitation</h2><p data-block-key=\\\"2rt0f\\\">Paris 18e, ancien magasin Tati Barb\\u00e8s, <b>samedi le 25 f\\u00e9vrier de 16h \\u00e0 17h15</b> se tient le <b>16e Conseil des Voisins</b> pour suivre et diriger notre association coop\\u00e9rative (voir <a href=\\\"https://wiki.lgv.coop//fr/votes/conseils/16e-conseil-2023-02-25?ref=blog.lesgrandsvoisins.com#ordre-du-jour\\\">notre avancement dans l\\u2019ordre du jour</a> ci-dessous). <a href=\\\"https://wiki.lgv.coop//fr/votes/conseils/16e-conseil-2023-02-25?ref=blog.lesgrandsvoisins.com#le-lieu\\\">Le lieux est magnifique</a> et \\u00e9ph\\u00e9m\\u00e8re, vers sa fin. Ce serait court, cordial, convivial, engag\\u00e9 et tol\\u00e9rant.</p><p data-block-key=\\\"frhci\\\">2 bd Marguerite de Rochechouart 75018 Paris. 16h \\u00e0 17h15<br/>Ordre du jour</p><p data-block-key=\\\"d2hfe\\\">Nous essayons de tenir nos Conseils \\u00e0 1h15 pour pouvoir encourager plus de Conseils des Voisins.</p><p data-block-key=\\\"cvvgn\\\">Les sujets \\u00e0 traiter sont :</p><ol><li data-block-key=\\\"2ecjo\\\">Se poser et se pr\\u00e9senter</li><li data-block-key=\\\"djkh\\\">Actu de la communication (<a href=\\\"http://www.commentwowmaintenant.com/?ref=blog.lesgrandsvoisins.com\\\">www.commentwowmaintenant.com</a>)</li></ol><ul><li data-block-key=\\\"e1p7r\\\">Nouveau site web <a href=\\\"http://www.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">www.lesgrandsvoisins.com</a></li><li data-block-key=\\\"erumi\\\">Reconnaissance en tant que coop\\u00e9rative pour <a href=\\\"http://www.lgv.coop/?ref=blog.lesgrandsvoisins.com\\\">www.lgv.coop</a></li><li data-block-key=\\\"5halk\\\">Utilisation de <a href=\\\"http://agoodvillage.com/?ref=blog.lesgrandsvoisins.com\\\">agoodvillage.com</a> en anglais</li><li data-block-key=\\\"dc0bq\\\">Utilisation de noms de domaine en terminaison .com (sauf <a href=\\\"http://lgv.coop/?ref=blog.lesgrandsvoisins.com\\\">lgv.coop</a>)</li></ul><ol><li data-block-key=\\\"b53fo\\\">Actu sur les activit\\u00e9s (<a href=\\\"http://www.communititude.com/?ref=blog.lesgrandsvoisins.com\\\">www.communititude.com</a>)</li></ol><ul><li data-block-key=\\\"d6ood\\\">Lancement des Popup Expos (<a href=\\\"http://www.popup-expos.com/?ref=blog.lesgrandsvoisins.com\\\">www.popup-expos.com</a>)</li><li data-block-key=\\\"bud8t\\\">Lancement de la salle de sociabilit\\u00e9 num\\u00e9rique (<a href=\\\"http://www.libregood.com/?ref=blog.lesgrandsvoisins.com\\\">www.libregood.com</a>)</li><li data-block-key=\\\"e686l\\\">Ateliers sur le num\\u00e9rique des Grands Voisins et ouverture de services (<a href=\\\"http://www.libregood.com/?ref=blog.lesgrandsvoisins.com\\\">www.libregood.com</a>)</li><li data-block-key=\\\"bj33j\\\">Orientation vers un observatoire ou un comit\\u00e9 scientifique</li><li data-block-key=\\\"4dpu8\\\">Lettre aux \\u00e9lues au sujet du tiers lieux Les Arches Citoyennes</li><li data-block-key=\\\"cujbu\\\">Les Grands Voisins en Turquie ?</li></ul><ol><li data-block-key=\\\"2vhib\\\">Actualit\\u00e9s sur les Voisins (<a href=\\\"http://www.quiquoietc.com/?ref=blog.lesgrandsvoisins.com\\\">www.quiquoietc.com</a>)</li></ol><ul><li data-block-key=\\\"5uj0n\\\">Un besoin de comp\\u00e9tences en communication</li><li data-block-key=\\\"fd18l\\\">Un processus d\\u2019inscription</li></ul><ol><li data-block-key=\\\"4evtt\\\">Actualit\\u00e9s sur la vie de l\\u2019association coop\\u00e9rative (<a href=\\\"http://www.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">www.lesgrandsvoisins.com</a>)</li></ol><ul><li data-block-key=\\\"cas8i\\\">R\\u00e9gularisation d\\u2019inscription d\\u2019association loi 1901</li><li data-block-key=\\\"3c1s5\\\">Budget et ouverture de compte en cours</li><li data-block-key=\\\"9ucqq\\\">Candidature aupr\\u00e8s de Plateau Urbain et recherche de locaux</li><li data-block-key=\\\"e5d1s\\\">Aggression verbale et menace d\\u2019un directeur d\\u2019Aurore</li></ul><ol><li data-block-key=\\\"car1h\\\">Mots de la fin</li></ol><p data-block-key=\\\"75lo8\\\">Contacter Chris chris@lesgrandsvoisins.com +33781811811 pour toutes questions.</p><h2 data-block-key=\\\"c76lq\\\">Supports</h2><p data-block-key=\\\"crb66\\\">V\\u00e9rifiez la discussion pour <a href=\\\"https://forum.goodv.org/t/16e-conseil-des-voisins-samedi-le-25-fevrier-2023-a-16h00/478?ref=blog.lesgrandsvoisins.com\\\">ce 16e Conseil des Voisins des Grands Voisins, l\\u2019association</a> pour les derni\\u00e8res nouvelles au sujet de notre 16e Conseil des Voisins.</p><p data-block-key=\\\"aga4p\\\">Le <a href=\\\"https://forum.gdvoisins.com/t/conseil-des-voisins-du-15-decembre-2022-compte-rendu/472?ref=blog.lesgrandsvoisins.com\\\">CR du 15e conseil des Voisins se trouve ici</a>.</p><h2 data-block-key=\\\"cntv7\\\">Le Lieu</h2><p data-block-key=\\\"7lbv8\\\">Ce sera au rez-de chauss\\u00e9e, soit dans la salle derri\\u00e8re la table ronde d\\u2019accueil, soit dans le hall d\\u2019entr\\u00e9e. Merci \\u00e0 l\\u2019association <a href=\\\"https://www.lesoiseauxmigrateurs.com/?ref=blog.lesgrandsvoisins.com\\\">Les Oiseaux Migratoires</a>.</p><p data-block-key=\\\"fefoq\\\">\\u00a9 <a href=\\\"https://www.timeout.fr/paris/actualites/on-a-visite-le-centre-culturel-experimental-installe-dans-lancien-tati-de-barbes-092322?ref=blog.lesgrandsvoisins.com\\\">L\\u2019Union de la Jeunesse Internationale</a> (<b>Ancien Magasin Tati Barb\\u00e8s</b>)</p><p data-block-key=\\\"eapv7\\\">Voici o\\u00f9 aura lieu le Conseil des Voisins</p><p data-block-key=\\\"7bnlu\\\">2 bd Marguerite de Rochechouart<br/>75018 Paris<br/>Ancien magasin \\u00ab TATI \\u00bb</p><h2 data-block-key=\\\"4g6ma\\\">Participation en Visio</h2><p data-block-key=\\\"5h16i\\\">Trois mots clefs sont m\\u00e2itres pour la participation en visio:</p><ol><li data-block-key=\\\"9ivpn\\\">Accueil</li><li data-block-key=\\\"ferlg\\\">Confort</li><li data-block-key=\\\"2ibt6\\\">S\\u00e9curit\\u00e9</li></ol><p data-block-key=\\\"cqaqs\\\">Aucun enregistrement ni audio ni vid\\u00e9o n\\u2019est permis, ni par nous, ni par vous.</p><p data-block-key=\\\"cm53m\\\">Merci d\\u2019allumer vos micros et cam\\u00e9ras car cela renforce le sentiment de s\\u00e9curit\\u00e9 de tout le monde.</p><p data-block-key=\\\"ejth9\\\">Venez au \\u00e0 l\\u2019ancien magasin Tati Barb\\u00e8s r\\u00e9habilit\\u00e9 avant que ce lieu magnifique ne ferme ! Nous allons aussi tenir une r\\u00e9union au moins audio sur et sur <a href=\\\"https://meet.lgv.coop/?ref=blog.lesgrandsvoisins.com\\\">meet.lgv.coop</a>.</p><h2 data-block-key=\\\"a3587\\\">Invitation</h2><p data-block-key=\\\"ehj8j\\\">Voici l\\u2019invitation telle qu\\u2019elle vient d\\u2019\\u00eatre envoy\\u00e9e</p><h2 data-block-key=\\\"kpm\\\">16e Conseil des Voisins sam. 25 f\\u00e9v. de 16h \\u00e0 17h15 !</h2><p data-block-key=\\\"20p0f\\\"><i>Bonjour Les Grands Voisins !</i></p><p data-block-key=\\\"7nvhk\\\">Venez nombreux au 16e Conseil des Voisins <b>samedi le 25 f\\u00e9vrier de 16h \\u00e0 17h15</b> et sur <a href=\\\"https://meet.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">meet.lesgrandsvoisins.com</a> !</p><p data-block-key=\\\"qmve\\\">V\\u00e9rifiez la discussion pour <a href=\\\"https://forum.goodv.org/t/16e-conseil-des-voisins-samedi-le-25-fevrier-2023-a-16h00/478?ref=blog.lesgrandsvoisins.com\\\">ce 16e Conseil des Voisins des Grands Voisins, l\\u2019association</a> pour les derni\\u00e8res nouvelles au sujet de notre 16e Conseil des Voisins.</p><p data-block-key=\\\"b18nb\\\">Le <a href=\\\"https://forum.gdvoisins.com/t/conseil-des-voisins-du-15-decembre-2022-compte-rendu/472?ref=blog.lesgrandsvoisins.com\\\">CR du 15e conseil des Voisins se trouve ici</a>.</p><h2 data-block-key=\\\"96v88\\\">Ordre du jour</h2><p data-block-key=\\\"5ln2c\\\">Nous essayons de tenir nos Conseils \\u00e0 1h15 pour pouvoir encourager plus de Conseils des Voisins.</p><p data-block-key=\\\"ba6au\\\">Les sujets \\u00e0 traiter sont :</p><ol><li data-block-key=\\\"4silv\\\">Les Popup Expos</li><li data-block-key=\\\"5p9f\\\">La Salle de sociabilit\\u00e9 num\\u00e9rique du budget participatif Paris 14e</li><li data-block-key=\\\"9gra3\\\">La Communication et une annonce nouvelle</li><li data-block-key=\\\"7tr01\\\">Les Services digitaux offerts par l\\u2019association</li><li data-block-key=\\\"7f8pb\\\">Notre recherche de locaux, l\\u2019\\u00e9tat administratif et la consid\\u00e9ration d\\u2019association coop\\u00e9rative</li><li data-block-key=\\\"fmo75\\\">Une proposition de comit\\u00e9 scientifique</li><li data-block-key=\\\"6lf33\\\">Ce que vous voulez</li></ol><p data-block-key=\\\"3t2ul\\\">Venez nombreux !</p><p data-block-key=\\\"etthn\\\">Contacter Chris chris@lesgrandsvoisins.com +33781811811 pour toutes questions.</p><h2 data-block-key=\\\"4431n\\\">Organisation</h2><p data-block-key=\\\"2aku9\\\">Lors de ma pr\\u00e9paration de ce Conseil, j\\u2019ai not\\u00e9 les id\\u00e9es suivantes.</p><h2 data-block-key=\\\"ocni\\\">Comment \\u00e7a marche ?</h2><p data-block-key=\\\"4ir6c\\\">Si vous venez en personne physique, vous \\u00eates membre votant de l\\u2019association par votre volont\\u00e9 de l\\u2019\\u00eatre et avez une voix dans votre coll\\u00e8ge.</p><p data-block-key=\\\"ddpnj\\\">Si vous venez en vocation pour une profession, pour une communaut\\u00e9, ou pour une personne morale, autant que vous pouvez contribuer aux GV dans cette capacit\\u00e9 dans de multiples autres activit\\u00e9s des GV.</p><h2 data-block-key=\\\"4c2p7\\\">Pourquoi participer ?</h2><p data-block-key=\\\"e3f3s\\\">A d\\u00e9faut d\\u2019une pr\\u00e9sentation commune arr\\u00eat\\u00e9e, pour moi, Chris, je pense que Les Grands Voisins proposent une fondation solide \\u00e0 partir d\\u2019un manifeste bien r\\u00e9fl\\u00e9chie (100 personnes sur 2 ateliers sur 1 mois). Ce manifeste passe bien un peu partout et conf\\u00e8re \\u00e0 notre association une responsabilit\\u00e9 importante pour permettre \\u00e0 un plus grand nombre de personnes, de communaut\\u00e9s, et de personnes morales \\u00e0 s\\u2019en approprier comme si elles sont toutes voisines.</p><p data-block-key=\\\"a6hss\\\">La force, pour moi toujours, est dans la variabilit\\u00e9 d\\u2019engagement dans une structure stable. C\\u2019est comme dans la vie de tous les jours, en tant que voisine ou voisin, c\\u2019est vous qui d\\u00e9cidez le d\\u00e9gr\\u00e9 d\\u2019engagement et les partages communs que vous voulez avec vos voisins. Aux GV, c\\u2019est pareil: vous d\\u00e9cidez. Etre avec vos voisins par choix change tout.</p><p data-block-key=\\\"ccfd9\\\">Nous nous inspirons d\\u2019un lieu, d\\u2019un contexte, et d\\u2019une date pr\\u00e9cis qui nous m\\u00e8nent \\u00e0 nous orienter envers:</p><ol><li data-block-key=\\\"aj4nd\\\">les institutions de services sociaux y compris dans leurs interactions avec leurs b\\u00e9n\\u00e9ficiaires</li><li data-block-key=\\\"e355n\\\">les artistes, artesans, professionnels, associations et entreprises, y compris dans leurs interactions avec leurs prospects et clients</li><li data-block-key=\\\"8mmb9\\\">les lieux et les activit\\u00e9s de rassemblement public tels que les caf\\u00e9s, restaurants, bars, th\\u00e9atres, etc. y compris avec leurs consommateurs et leurs spectateurs</li><li data-block-key=\\\"68it2\\\">les pouvoirs publics</li></ol><p data-block-key=\\\"ci98k\\\">Notre m\\u00e9tier principal est l\\u2019\\u00e9coute. Nous fournissons une structure dans laquelle plusieurs activit\\u00e9s peuvent na\\u00eetre. Nous nous efforcons d\\u2019une posture \\u00e0 pouvoir toujours rendre des comptes systematiquement et efficacement sur toutes nos activit\\u00e9s.</p><p data-block-key=\\\"cloom\\\">Je pense que notre mani\\u00e8re de vouloir sinc\\u00e8rement \\u00eatre au service des utilisateurs \\u00e0 qui nous nous effor\\u00e7ons de rendre des comptes est particuli\\u00e8rement pratique pour le domaine digital. Quand vous utilisez Google ou Microsoft, ces soci\\u00e9t\\u00e9s ne vous rend pas des comptes. Pour moi, \\u00e7a change quelque chose. De surcroit, nous utilisons comme guide non pas seulement la loi, mais aussi <a href=\\\"https://www.contractfortheweb.org/fr?ref=blog.lesgrandsvoisins.com\\\">le Contrat pour le web</a>.</p><p data-block-key=\\\"b30rb\\\">Pour moi, une Grande Voisine ou un Grand Voisin, c\\u2019est l\\u2019\\u00eatre humain en vocation. Par exemple, une boulang\\u00e9rie est une voisine gr\\u00e2ce \\u00e0 la pr\\u00e9sence de la personne morale et aussi la pr\\u00e9sence des \\u00eatres humains \\u00e0 l\\u2019int\\u00e9rieur (de tous les statuts). Par exemple, les personnes qui habitent \\u00e0 c\\u00f4t\\u00e9 de moi on une vocation de propri\\u00e9taire ou locataire. Les Voisins ne sont pas les personnes ni les r\\u00f4les, mais les personnes dans des r\\u00f4les. Les r\\u00f4les peuvent aller de consommateur, \\u00e0 salari\\u00e9, \\u00e0 b\\u00e9n\\u00e9vole, \\u00e0 mandataire sociale, \\u00e0 \\u00e9lu, \\u00e0 citoyen du monde, etc.</p><p data-block-key=\\\"3kv5u\\\">Une partie de la responsabilit\\u00e9 des Grands Voisins est aussi d\\u2019entendre un large panel de d\\u00e9finitions des Grands Voisins qui ne sont pas toutes compatibles mais toutes aussi valables. C\\u2019est une clef pour l\\u2019appropriation par un plus grand nombre, et pourquoi la marque Les Grands Voisins est mieux en marque ouverte. Soyez Grands Voisins.</p><p data-block-key=\\\"dau4k\\\">Inversement, l\\u2019association Les Grands Voisins (.com) a besoin de l\\u2019engagement des vocations, des entreprises, des professionnels, des artistes, des artesans, des pouvoirs publics, des lieux et activit\\u00e9s de cultivation publique, des donnateurs, etc. pour vivre. Les choses que nous partageons viennent et sont confi\\u00e9es pour ces acteurs. Bref: notre productivit\\u00e9 sociale et \\u00e9conomique correctement encadr\\u00e9es ont leurs places dans l\\u2019association.</p><p data-block-key=\\\"6f54n\\\">Etre membre votant, c\\u2019est \\u00eatre une personne physique. Le m\\u00eame votant est notre unit\\u00e9 de respect de base. Chaque membre votant peut s\\u2019identifier \\u00e0 des r\\u00f4les diff\\u00e9rants, et peut participer aussi avec ces r\\u00f4les. De m\\u00eame, les acteurs professionnels, commerciaux, culturels, artistiques, artisanaux peuvent distinctement s\\u2019identifier en tant que Grands Voisins dans notre association de par diff\\u00e9rantes activit\\u00e9s. Notre association essaye d\\u2019\\u00eatre digne de tout cela.</p><p data-block-key=\\\"cec7m\\\">Cela peut semble long, ce qui est \\u00e9crit dessus. Cela peut m\\u00eame sembler \\u00eatre complexe. Pourtant, la proposition de base me semble \\u00eatre <i>compelling</i> et simple. Nous utilisons l\\u2019image d\\u2019un voisinage qui comporte des activit\\u00e9s socio-\\u00e9conomiques diverses pour mettre la priorit\\u00e9 sur chaque \\u00eatre humain dans son r\\u00f4le ou ses r\\u00f4les multiples afin d\\u2019\\u00eatre et de mettre en commun de mani\\u00e8re intentionnel et libre, le tout sur une fondation fiable, explicite et stable.</p><h2 data-block-key=\\\"778hf\\\">Mise \\u00e0 jour du lieu</h2><p data-block-key=\\\"clvri\\\">Attention <b><i>changement de lieu</i></b>:</p><p data-block-key=\\\"4aeph\\\"><a href=\\\"https://www.timeout.fr/paris/actualites/on-a-visite-le-centre-culturel-experimental-installe-dans-lancien-tati-de-barbes-092322?ref=blog.lesgrandsvoisins.com\\\">L\\u2019Union de la Jeunesse Internationale</a> (<b>Ancien Magasin Tati Barb\\u00e8s</b>)</p><p data-block-key=\\\"ersiu\\\">2 bd Marguerite de Rochechouart<br/>75018 Paris<br/>Ancien magasin \\u00ab TATI \\u00bb<br/>Metro Barbes, Lignes 2 et 4</p><p data-block-key=\\\"9n662\\\">Merci \\u00e0 l\\u2019association Les Oiseaux Migratoires.</p><p data-block-key=\\\"3p0ng\\\">Ce sera au rez-de chauss\\u00e9e, soit dans la salle derri\\u00e8re la table ronde d\\u2019accueil, soit dans le hall d\\u2019entr\\u00e9e.</p><h2 data-block-key=\\\"2keed\\\">Invitation par SMS</h2><p data-block-key=\\\"1ph02\\\">Conseil des Grands Voisins Samedi le 25 f\\u00e9v. Paris 18e, 16h \\u00e0 17h15, ancien magasin Tati Barb\\u00e8s, et \\u00e0 distance. Nous essayons d\\u2019avancer dans la diversit\\u00e9 et dans la transparence. Inscription et liens vers infos ici: <a href=\\\"https://www.lgv.coop/fr/coop/inscription?ref=blog.lesgrandsvoisins.com\\\">https://www.lgv.coop/fr/coop/inscription</a></p><p data-block-key=\\\"97rgh\\\">Les Grands Voisins Town Hall meeting Sat Feb 25 in Paris (18th) and distance from 4PM to 5:15 PM Paris time. Wonderful place. We strive for diversity and accountability. Signup and info here: <a href=\\\"https://www.lgv.coop/fr/coop/inscription?ref=blog.lesgrandsvoisins.com\\\">https://www.lgv.coop/fr/coop/inscription</a></p>\", \"id\": \"c34f93f0-766b-48d8-94cc-0170f9c0ebd2\"}, {\"type\": \"image\", \"value\": {\"title\": \"\", \"heading_tag\": \"h3\", \"image\": 59, \"alt\": \"\", \"caption\": \"\", \"url\": \"\"}, \"id\": \"854696fa-9977-449d-855b-d87f4aef1667\"}, {\"type\": \"paragraph\", \"value\": \"<h2 data-block-key=\\\"ketc7\\\">Logistique</h2><p data-block-key=\\\"6j1or\\\">Au plaisir de vous retrouver au Conseil \\u00e0 Paris 18e, ancien magasin Tati Barb\\u00e8s, <b>samedi le 25 f\\u00e9vrier de 16h \\u00e0 17h15</b> se tient le <a href=\\\"https://wiki.lgv.coop/fr/votes/conseils/16e-conseil-2023-02-25?ref=blog.lesgrandsvoisins.com\\\"><b>16e Conseil des Voisins</b></a> pour suivre et diriger notre association coop\\u00e9rative (voir <a href=\\\"https://wiki.lgv.coop/fr/votes/conseils/16e-conseil-2023-02-25?ref=blog.lesgrandsvoisins.com#ordre-du-jour\\\">notre avancement dans l\\u2019ordre du jour</a> ci-dessous). <a href=\\\"https://wiki.lgv.coop/fr/votes/conseils/16e-conseil-2023-02-25?ref=blog.lesgrandsvoisins.com#le-lieu\\\">Le lieux est magnifique</a> et \\u00e9ph\\u00e9m\\u00e8re, vers sa fin. Ce serait court, cordial, convivial, engag\\u00e9 et tol\\u00e9rant.<br/><a href=\\\"https://wiki.lgv.coop/fr/votes/conseils/16e-conseil-2023-02-25?ref=blog.lesgrandsvoisins.com\\\">Cliquer ici pour avoir le programme complet.</a><br/></p><p data-block-key=\\\"5sfr6\\\">2 bd Marguerite de Rochechouart 75018 Paris 16h \\u00e0 17h15 Si vous venez \\u00e0 distance, merci de consulter notre <a href=\\\"https://wiki.lgv.coop/fr/votes/conseils/16e-conseil-2023-02-25?ref=blog.lesgrandsvoisins.com#participation-en-visio\\\">note sur la participation visio</a>.Lien d\\u2019inscription :</p><h2 data-block-key=\\\"9vqap\\\"><b>Ordre du jour</b>Nous essayons de tenir nos Conseils \\u00e0 1h15 pour pouvoir encourager plus de Conseils des Voisins.Les sujets \\u00e0 traiter sont :</h2><ol><li data-block-key=\\\"b1ruc\\\">Se poser et se pr\\u00e9senter</li><li data-block-key=\\\"6nr2g\\\">Actu de la communication (<a href=\\\"https://www.commentwowmaintenant.com/?ref=blog.lesgrandsvoisins.com\\\">www.commentwowmaintenant.com</a>)</li></ol><ul><li data-block-key=\\\"d3g66\\\">Nouveau site web <a href=\\\"https://www.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">www.lesgrandsvoisins.com</a></li><li data-block-key=\\\"78fcp\\\">Reconnaissance en tant que coop\\u00e9rative pour <a href=\\\"https://www.lgv.coop/?ref=blog.lesgrandsvoisins.com\\\">www.lgv.coop</a></li><li data-block-key=\\\"2uaa1\\\">Utilisation de <a href=\\\"https://www.agoodvillage.com/en?ref=blog.lesgrandsvoisins.com\\\">agoodvillage.com</a> en anglais</li><li data-block-key=\\\"efvk1\\\">Utilisation de noms de domaine en terminaison .com (sauf <a href=\\\"https://www.lgv.coop/fr?ref=blog.lesgrandsvoisins.com\\\">lgv.coop</a>)</li></ul><ol><li data-block-key=\\\"qrg3\\\">Actu sur les activit\\u00e9s (<a href=\\\"https://www.communititude.com/?ref=blog.lesgrandsvoisins.com\\\">www.communititude.com</a>)</li></ol><ul><li data-block-key=\\\"dov8a\\\">Lancement des Popup Expos (<a href=\\\"https://www.popup-expos.com/fr?ref=blog.lesgrandsvoisins.com\\\">www.popup-expos.com</a>)</li><li data-block-key=\\\"en6ac\\\">Lancement de la salle de sociabilit\\u00e9 num\\u00e9rique (<a href=\\\"https://www.libregood.com/fr?ref=blog.lesgrandsvoisins.com\\\">www.libregood.com</a>)</li><li data-block-key=\\\"69sig\\\">Ateliers sur le num\\u00e9rique des Grands Voisins et ouverture de services (<a href=\\\"https://www.libregood.com/fr?ref=blog.lesgrandsvoisins.com\\\">www.libregood.com</a>)</li><li data-block-key=\\\"e4r3e\\\">Orientation vers un observatoire ou un comit\\u00e9 scientifique ?</li><li data-block-key=\\\"ffhka\\\">Lettre aux \\u00e9lues au sujet du tiers lieux Les Arches Citoyennes</li><li data-block-key=\\\"f5v09\\\">Les Grands Voisins en Turquie ?</li></ul><ol><li data-block-key=\\\"9h33j\\\">Actualit\\u00e9s sur les Voisins (<a href=\\\"https://www.quiquoietc.com/?ref=blog.lesgrandsvoisins.com\\\">www.quiquoietc.com</a>)</li></ol><ul><li data-block-key=\\\"1cfe\\\">Un besoin de comp\\u00e9tences en communication</li><li data-block-key=\\\"4ok43\\\">Un processus d\\u2019inscription</li></ul><ol><li data-block-key=\\\"9eijf\\\">Actualit\\u00e9s sur la vie de l\\u2019association coop\\u00e9rative (<a href=\\\"https://www.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">www.lesgrandsvoisins.com</a>)</li></ol><ul><li data-block-key=\\\"614in\\\">R\\u00e9gularisation d\\u2019inscription d\\u2019association loi 1901</li><li data-block-key=\\\"ac3fm\\\">Budget et ouverture de compte en cours</li><li data-block-key=\\\"a9hj\\\">Candidature aupr\\u00e8s de Plateau Urbain et recherche de locaux</li><li data-block-key=\\\"9ievq\\\">Aggression verbale et menace d\\u2019un directeur d\\u2019Aurore</li><li data-block-key=\\\"5b7ah\\\">Demande d\\u2019agr\\u00e9ment d\\u2019int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral</li></ul><ol><li data-block-key=\\\"6eihn\\\">Mots de la finContacter Chris chris@lesgrandsvoisins.com +33781811811 pour toutes questions.</li></ol><h2 data-block-key=\\\"1agno\\\"><b>Supports</b>V\\u00e9rifiez la discussion pour <a href=\\\"https://forum.goodv.org/t/16e-conseil-des-voisins-samedi-le-25-fevrier-2023-a-16h00/478?ref=blog.lesgrandsvoisins.com\\\">ce 16e Conseil des Voisins des Grands Voisins, l\\u2019association</a> pour les derni\\u00e8res nouvelles au sujet de notre 16e Conseil des Voisins.Le <a href=\\\"https://forum.gdvoisins.com/t/conseil-des-voisins-du-15-decembre-2022-compte-rendu/472?ref=blog.lesgrandsvoisins.com\\\">CR du 15e conseil des Voisins se trouve ici</a>.</h2><h2 data-block-key=\\\"4d32i\\\"><b>Le Lieu</b>Ce sera au rez-de chauss\\u00e9e, soit dans la salle derri\\u00e8re la table ronde d\\u2019accueil, soit dans le hall d\\u2019entr\\u00e9e. Merci \\u00e0 l\\u2019association <a href=\\\"https://www.lesoiseauxmigrateurs.com/?ref=blog.lesgrandsvoisins.com\\\">Les Oiseaux Migratoires</a>.\\u00a9 <a href=\\\"https://www.timeout.fr/paris/actualites/on-a-visite-le-centre-culturel-experimental-installe-dans-lancien-tati-de-barbes-092322?ref=blog.lesgrandsvoisins.com\\\">L\\u2019Union de la Jeunesse Internationale</a> (<b>Ancien Magasin Tati Barb\\u00e8s</b>)Voici o\\u00f9 aura lieu le Conseil des Voisins2 bd Marguerite de Rochechouart 75018 Paris Ancien magasin \\u201cTATI\\u201dVenez au \\u00e0 l\\u2019ancien magasin Tati Barb\\u00e8s r\\u00e9habilit\\u00e9 avant que ce lieu magnifique ne ferme !</h2><h2 data-block-key=\\\"433gu\\\"><b>Participation en Visio</b>Trois mots clefs sont ma\\u00eetres pour la participation en visio:</h2><ol><li data-block-key=\\\"51pu2\\\">Accueil</li><li data-block-key=\\\"1tgjm\\\">Confort</li><li data-block-key=\\\"e4p4r\\\">S\\u00e9curit\\u00e9Aucun enregistrement ni audio ni vid\\u00e9o n\\u2019est permis, ni par nous, ni par vous.Merci d\\u2019allumer vos micros et cam\\u00e9ras car cela renforce le sentiment de s\\u00e9curit\\u00e9 de tout le monde.Nous allons aussi tenir une r\\u00e9union au moins audio sur et sur <a href=\\\"https://meet.lgv.coop/?ref=blog.lesgrandsvoisins.com\\\">meet.lgv.coop</a>.Chris Mann is inviting you to a meeting.Join the meeting: <a href=\\\"https://8x8.vc/vpaas-magic-cookie-15b65cc344ea4050aa054030a02658aa/SampleAppMajorFootballsResearchWhere?ref=blog.lesgrandsvoisins.com\\\">https://8x8.vc/vpaas-magic-cookie-15b65cc344ea4050aa054030a02658aa/SampleAppMajorFootballsResearchWhere</a>To join by phone instead, tap this: +33 9 70 73 18 64,3144964135# +33 9 70 73 18 64,3144964135#Looking for a different dial-in number? See meeting dial-in numbers: <a href=\\\"https://8x8.vc/vpaas-magic-cookie-15b65cc344ea4050aa054030a02658aa/static/dialInInfo.html?room=SampleAppMajorFootballsResearchWhere&amp;ref=blog.lesgrandsvoisins.com\\\">https://8x8.vc/vpaas-magic-cookie-15b65cc344ea4050aa054030a02658aa/static/dialInInfo.html?room=SampleAppMajorFootballsResearchWhere</a>If also dialing-in through a room phone, join without connecting to audio: <a href=\\\"https://8x8.vc/vpaas-magic-cookie-15b65cc344ea4050aa054030a02658aa/SampleAppMajorFootballsResearchWhere?ref=blog.lesgrandsvoisins.com#config.startSilent=true\\\">https://8x8.vc/vpaas-magic-cookie-15b65cc344ea4050aa054030a02658aa/SampleAppMajorFootballsResearchWhere#config.startSilent=true</a></li></ol><p data-block-key=\\\"3mh11\\\"></p>\", \"id\": \"a08e95fa-e9de-4acb-bc32-9a3eda33f407\"}]", "date": "2023-02-25T11:22:00Z", "live": true, "path": "000100020003000S", "slug": "16e-conseil-des-voisins", "depth": 4, "owner": 1, "title": "16e Conseil des Voisins", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/16e-conseil-des-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "16e Conseil des Voisins", "entry_tags": [], "go_live_at": null, "draft_title": "16e Conseil des Voisins", "content_type": 71, "header_image": 58, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "9c22475f-8494-428d-be69-bb3ad39f8cfe", "entry_categories": [{"pk": 29, "page": 41, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "green-archipel", "search_description": "Paris 18e, ancien magasin Tati Barbès, samedi le 25 février de 16h à 17h15 se tient le 16e Conseil des Voisins pour suivre et diriger notre association coopérative (voir notre avancement dans l’ordre du jour ci-dessous). Le lieux est magnifique et éphémère, vers sa fin. Ce serait court, cordial, convivial, engagé et tolérant.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '41', 1, 71, 1, '16e Conseil des Voisins');
INSERT INTO public.wagtailcore_revision VALUES (113, '2024-05-08 16:28:10.689841+00', '{"pk": 42, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"tdk6q\\\">L\\u2019incident qui opposait un ancien cheminot et une communaut\\u00e9 kurde dans le 10e arrondissement de Paris renforce le besoin aujourd\\u2019hui plus que jamais le besoin de la proposition Les Grands Voisins. A quoi pouvons-nous nous attacher ? Comment se m\\u00ealent les id\\u00e9es identitaires et de nous-m\\u00eames individuellement et des communaut\\u00e9s dans lesquelles nous nous engageons ? Comme dans un jardin, Les Grands Voisins est aujourd\\u2019hui une association qui se veut \\u00eatre terrain o\\u00f9 des r\\u00e9ponses toutes diff\\u00e9rentes peuvent s\\u2019\\u00e9panouir comme des plantes diff\\u00e9rentes.</p><p data-block-key=\\\"erhht\\\">Aujourd\\u2019hui m\\u00eame, alors qu\\u2019aux USA au moment o\\u00f9 je vous \\u00e9cris, la chambre basse am\\u00e9ricaine ne se constitue toujours pas pour la deuxi\\u00e8me fois, chose qui n\\u2019est pas arriv\\u00e9e depuis environ au temps de la guerre civile am\\u00e9ricaine, cela fait exactement deux ans que le Capitole des USA a \\u00e9t\\u00e9 envahi par une tribu soud\\u00e9e de \\u00ab patriotes \\u00bb. Ecoutez ma question et \\u00e9coutez la r\\u00e9ponse de l\\u2019ancien Ministre Julien DENORMANDIE au sujet de la stabilit\\u00e9 des institutions de la Cinqui\\u00e8me R\\u00e9publique au regard d\\u2019abord du 11 septembre 2001 et puis le 6 janvier 2021, puis au regard de la guerre en Ukraine.</p><p data-block-key=\\\"rg6\\\"><a href=\\\"https://www.youtube.com/watch?v=kRGHzXqJmoE&amp;ref=blog.lesgrandsvoisins.com\\\">FAIRE TIERS-LIEUX AU CESE</a></p><p data-block-key=\\\"6o4j2\\\">Nous sommes aussi face \\u00e0 des mod\\u00e8les mixtes mises en question pour l\\u2019objectif des Grands Voisins autant en France qu\\u2019aux USA (sans parler de la Russie). Ma pr\\u00e9occupation personnelle, et une contribution parmi d\\u2019autres que je souhaiterais vous proposer \\u00e0 travers Les Grands Voisins, est une pr\\u00e9occupation sur les ph\\u00e9nom\\u00e8nes de la mise en tribus. Il est r\\u00e9confortant d\\u2019\\u00eatre avec des semblables, certes. Il est riche d\\u2019\\u00eatre en diversit\\u00e9 locale.</p><p data-block-key=\\\"c97d3\\\">Cette ann\\u00e9e 2023, elle est la v\\u00f4tre, et je vous la souhaite la meilleure ann\\u00e9e 2023 de votre vie et de la vie des diff\\u00e9rentes communaut\\u00e9s, y compris vos employeurs et vos professions. Que vous essayez cette ann\\u00e9e. Que vous \\u00e9coutez cette ann\\u00e9e. Que vous soyez \\u00e9cout\\u00e9(e) cette ann\\u00e9e. Que cette ann\\u00e9e sourit sur vous et que cette ann\\u00e9e soit enrichie de votre sourire, ou juste de votre pr\\u00e9sence m\\u00eame sans sourire !</p><p data-block-key=\\\"bkp3a\\\">Les Grands Voisins, c\\u2019est tout d\\u2019abord \\u00ab accountability \\u00bb cette ann\\u00e9e. Nos nouveaux statuts pr\\u00e9sentent une incorporation, je pense de bonne foi \\u00e0 toute personne qui vient \\u00e0 une de nos r\\u00e9unions \\u00ab Conseils des Voisins \\u00bb. Nous avons certains biens et services en communs : le web du contrat, les popup-expos, l\\u2019annuaire, la salle de sociabilit\\u00e9 num\\u00e9rique du 14e (budget participatif obtenu, je n\\u2019esp\\u00e8re pas trop tard). Il y a du pain sur la planche.</p><p data-block-key=\\\"53g8k\\\">J\\u2019aimerais remercier particuli\\u00e8rement la Ressourcerie Cr\\u00e9ative et f\\u00e9licitez la cr\\u00e9ation de <a href=\\\"https://www.breastfreedom.com/?ref=blog.lesgrandsvoisins.com\\\">Breast Freedom</a> de sa cr\\u00e9ation. Il y a bien entendu beaucoup d\\u2019initiatives autres, comme <a href=\\\"https://www.weezem.com/?ref=blog.lesgrandsvoisins.com\\\">Weezem</a> qui t\\u00e9moigne le pont entre l\\u2019id\\u00e9e et la bienveillance d\\u2019envergure. Je remercie aussi Fran\\u00e7ois HOLLANDE qui m\\u2019a \\u00e9cout\\u00e9 sur le sens des Grands Voisins de 2017. Puis, la galanterie d\\u2019Audrey PULVAR en plus de sa d\\u00e9claration d\\u2019intention de soutien me touchent profond\\u00e9ment.</p><p data-block-key=\\\"7o5mr\\\">Dans notre milieu, on ne peut pas \\u00e9viter d\\u2019\\u00eatre labellis\\u00e9(e) politique. On ne peut pas \\u00e9viter l\\u2019agression contre soi. Le devoir de comprendre affaiblit et prend du temps. Pour vous, de m\\u00eame que cela est mon d\\u00e9fi, bon courage cette belle ann\\u00e9e 2023. La r\\u00e9alit\\u00e9 n\\u2019est pas l\\u2019id\\u00e9ale, mais l\\u2019id\\u00e9ale est r\\u00e9elle.</p><p data-block-key=\\\"ap48h\\\">Les Grands Voisins, c\\u2019est une association qui partage et qui propose un pouvoir aux individus depuis toutes leurs communaut\\u00e9s, y compris leurs employeurs ou les structures qu\\u2019ils repr\\u00e9sentent. L\\u2019association a pour ambition de se mettre au service avec neutralit\\u00e9 et \\u00e9coute aux usagers, aux entreprises, aux professionnels, aux artistes, aux artisans, aux institutions, aux associations, aux b\\u00e9n\\u00e9ficiaires, aux services sociaux, aux consommateurs, \\u2026 \\u00e0 travers les \\u00eatres humains qui s\\u2019y trouvent, les \\u00eatres humains qui ont tous un poids.</p><p data-block-key=\\\"4mqa9\\\">C\\u2019est une journ\\u00e9e de renouveau pour moi. Que ce soit une ann\\u00e9e de renouveau pour nous tous !</p><p data-block-key=\\\"10r8n\\\">Pr\\u00e9sident concierge<br/>Chris MANN<br/>Association Les Grands Voisins, Paris 14e et Fontenay-aux-Roses<br/><a href=\\\"https://www.lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\\\">https://www.lesgrandsvoisins.com</a> - +33 7 81 811 811 - chris@lesgrandsvoisins.com</p><p data-block-key=\\\"f63qh\\\">P.S. Merci de votre patience avec les \\u00e9ventuelles coquilles de mon expression. Bonne ann\\u00e9e ! CM</p>\", \"id\": \"75a18a53-dd5f-4f5e-823b-a442c10ae140\"}]", "date": "2023-01-11T11:26:00Z", "live": true, "path": "000100020003000T", "slug": "bonne-annee-2023", "depth": 4, "owner": 1, "title": "Bonne année 2023 !", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/bonne-annee-2023/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "", "entry_tags": [], "go_live_at": null, "draft_title": "Bonne année 2023 !", "content_type": 71, "header_image": 60, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "299c0e5e-a650-43df-9ad5-e90e7d58831c", "entry_categories": [{"pk": 30, "page": 42, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": null, "search_description": "L’incident qui opposait un ancien cheminot et une communauté kurde dans le 10e arrondissement de Paris renforce le besoin aujourd’hui […]", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '42', 1, 71, 1, 'Bonne année 2023 !');
INSERT INTO public.wagtailcore_revision VALUES (6, '2024-05-05 08:06:45.375687+00', '{"pk": 3, "body": "[{\"type\": \"multicolumns\", \"value\": {\"bg_image\": null, \"bg_color_class\": \"\", \"bg_color\": \"\", \"title\": \"\", \"heading_tag\": \"h2\", \"columns\": [{\"type\": \"text\", \"value\": \"<p data-block-key=\\\"dpxfv\\\">Venez \\u00e0 notre permanence <b>le lundi de 12h \\u00e0 19h</b><br/>en personne \\u00e0 <b>93 ave. Du G\\u00e9n. Leclerc</b> 75014 Paris (M\\u00e9tro Al\\u00e9sia, Atelier de la Ressourcerie, <i>7e boutique sur la gauche</i>) ou <a href=\\\"https://meet.lesg/\\\">meet.lesgrandsvoisins.com pour participer en hybride</a>.</p><p data-block-key=\\\"81o4v\\\"><a href=\\\"https://list.lesgrandsvoisins.com/subscription/form\\\"><b><i>Pensez \\u00e0 vous inscrire sur notre liste d''infos !</i></b></a></p><p data-block-key=\\\"62f45\\\">Nous sommes une association d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral agissant de mani\\u00e8re contr\\u00f4l\\u00e9e en subsidiarit\\u00e9 ouverte pour toute organisation et tout individu qui en fait la d\\u00e9cision pour soi-m\\u00eame principalement en ces trois domaines <a href=\\\"https://www.lesgrandsvoisins.fr/communautes/\\\">de Discussions</a>, <a href=\\\"https://www.lesgrandsvoisins.fr/arts/\\\">d''Arts plastiques</a> et <a href=\\\"https://www.lesgrandsvoisins.fr/numerique/\\\">de Num\\u00e9rique</a> selon <a href=\\\"https://www.lesgrandsvoisins.fr/mission/\\\">notre d\\u00e9claration de mission</a>.</p><p data-block-key=\\\"5k9v7\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/communautes/\\\"><b>Discussions</b></a> :</p><ul><li data-block-key=\\\"9e76v\\\">Nous proposons des conditions encourageant la compr\\u00e9hension entre individus et organisations de perspectives diff\\u00e9rentes.</li><li data-block-key=\\\"2hp8d\\\">Nous ouvrons notre gouvernance pour des votes en Conseils des Voisins.</li></ul><p data-block-key=\\\"cnme6\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/arts/\\\"><b>Arts plastiques</b></a> :</p><ul><li data-block-key=\\\"4govp\\\">Nous encourageons l\\u2019utilisation hybrides d\\u2019espace (commerces, h\\u00f4pitaux, caf\\u00e9s, etc.) aussi en gall\\u00e9ries locales.</li><li data-block-key=\\\"3ut8l\\\">Nous accompagnons des institutions pour trouver une juste place pour les arts plastiques dans leurs locaux.</li></ul><p data-block-key=\\\"euipn\\\"><a href=\\\"https://www.lesgrandsvoisins.fr/numerique/\\\"><b>Num\\u00e9rique</b></a> :</p><ul><li data-block-key=\\\"ecd1s\\\">Nous maintenons une constellation de serveurs et de services internet -- tableau de bord, cr\\u00e9ation et gestion de compte, courriel, rencontres hybrides pr\\u00e9sence-distance, listes de diffusion num\\u00e9riques, blogs, serveurs webs, gestionnaire de mot de passe, enregistrement dans le nuage, forums de discussion, gestion de la connaissance, \\u00e9dition markdown, wikis, \\u00e9dition bueautique, etc. -- pour, par, et de nous-m\\u00eames et d''autres.</li><li data-block-key=\\\"3m878\\\">Nous mettons \\u00e0 disposition des talents et des mat\\u00e9riaux dans des salles de sociabilit\\u00e9 num\\u00e9rique et dans des syst\\u00e8mes de pr\\u00eats et \\u00e9changes de mat\\u00e9riels \\u00e0 l''attention d''individus et d''organisations de tous niveaux.</li></ul><p data-block-key=\\\"4apcl\\\">Voici quelques entit\\u00e9s Grandes Voisines qui soutiennent notre d\\u00e9marche: A Good Solution, Banya Tour, Robert Baldridge, BMCP, Breast Freedom, Le Bureau du Cr\\u00e9ateur, Le Chiffon, Les Jardins du Num\\u00e9rique, Olivier Duhec, Inno Cube, Caroline Lhomme, Francesco du 59 Rivoli, Amadou Gaye, Informatruck, Inolib, KaceKode, Ada Matus, L\\u2019Odys\\u00e9e, La Ressourcerie Cr\\u00e9ative, Henry Roy, Peter Vaeth, Waka Conseil, Weezem, et peut-\\u00eatre des v\\u00f4tres \\u2026</p><p data-block-key=\\\"emgj1\\\">Pour plus d\\u2019informations, contactez-nous\\u00a0: chris@lesgrandsvoisins.com +33 7 81 811 811</p>\", \"id\": \"91ef9016-8c1b-42f5-8c50-2c0f4634d3a7\"}, {\"type\": \"video\", \"value\": {\"title\": \"Les Grands Voisins, la cit\\u00e9 r\\u00eav\\u00e9e\", \"caption\": \"c 3, 2020 R\\u00e9alisation : Bastien Simon  Pitch : Ma\\u00ebl, artiste sans papier, Adrien, luthier musicien et d''autres r\\u00e9sidents venus de tous horizons, s''organisent pour donner naissance \\u00e0 une utopie moderne en plein coeur de Paris, un village solidaire de pr\\u00e8s de 2000 personnes : Les Grands Voisins. \\u00c0 travers leurs trajectoires et celles des membres fondateurs du lieu, le film interroge notre d\\u00e9sir et notre capacit\\u00e9 \\u00e0 inventer d\\u2019autres mani\\u00e8res de vivre ensemble. Que retiendrons-nous de cette exp\\u00e9rience collective ? Pourra-t-elle perdurer, essaimer, r\\u00e9sonner ailleurs ?\", \"url\": \"https://www.youtube.com/embed/qZqErGfNF_o?feature=oembed\"}, \"id\": \"611cb4b0-aaf5-444f-b9bb-6a94b69d079d\"}]}, \"id\": \"5a90625c-ecd5-431c-a227-1b3e1fe64b58\"}, {\"type\": \"video\", \"value\": {\"title\": \"Les Grands Voisins, la cit\\u00e9 r\\u00eav\\u00e9e\", \"caption\": \"c 3, 2020 R\\u00e9alisation : Bastien Simon  Pitch : Ma\\u00ebl, artiste sans papier, Adrien, luthier musicien et d''autres r\\u00e9sidents venus de tous horizons, s''organisent pour donner naissance \\u00e0 une utopie moderne en plein coeur de Paris, un village solidaire de pr\\u00e8s de 2000 personnes : Les Grands Voisins. \\u00c0 travers leurs trajectoires et celles des membres fondateurs du lieu, le film interroge notre d\\u00e9sir et notre capacit\\u00e9 \\u00e0 inventer d\\u2019autres mani\\u00e8res de vivre ensemble. Que retiendrons-nous de cette exp\\u00e9rience collective ? Pourra-t-elle perdurer, essaimer, r\\u00e9sonner ailleurs ?\", \"url\": \"https://www.youtube.com/embed/qZqErGfNF_o?feature=oembed\"}, \"id\": \"f53195d5-0de5-479d-aef1-6151fbd266e0\"}]", "live": true, "path": "00010002", "slug": "home", "depth": 2, "owner": null, "title": "Bienvenue aux Grands Voisins", "locale": 1, "locked": false, "expired": false, "alias_of": null, "numchild": 2, "url_path": "/home/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "", "go_live_at": null, "draft_title": "Bienvenue aux Grands Voisins", "content_type": 25, "header_image": null, "header_large": false, "header_darken": false, "live_revision": 5, "show_in_menus": true, "header_cta_link": null, "header_cta_text": null, "latest_revision": 5, "translation_key": "64c061ae-6a72-45e9-bb93-2e884f237976", "contentpage_tags": [], "header_cta_label": null, "header_with_title": false, "last_published_at": "2024-05-05T08:04:49.537Z", "first_published_at": "2024-05-05T07:42:47.708Z", "header_color_class": null, "search_description": "Bienvenue ! Vous venez de créer un site utilisant le gestionnaire de contenus de l’État. Vous pouvez maintenant vous connecter […]", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T08:04:49.510Z"}', NULL, '3', 1, 25, 1, 'Bienvenue aux Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (33, '2024-05-05 17:31:56.443084+00', '{"pk": 15, "body": "[{\"type\": \"paragraph\", \"value\": \"<h2 data-block-key=\\\"sxybs\\\">Conclusions</h2><p data-block-key=\\\"d24e0\\\">Les votes connexes aux conclusions sont toujours \\u00e0 venir.</p><ol><li data-block-key=\\\"9jn2p\\\">Nous actons la d\\u00e9mission de Ma\\u00ebl et donne pouvoir \\u00e0 Chris MANN pour faire les d\\u00e9marches connexes</li><li data-block-key=\\\"4j4ig\\\">Un pr\\u00e9visionnel est \\u00e0 \\u00e9tablir avant d''engager la commande de 3000 euros aupr\\u00e8s d''Osorus (Popup Expos)</li><li data-block-key=\\\"4jqe5\\\">Une dette de 1000 euros au Bureau du Cr\\u00e9ateur est \\u00e0 accorder en priorit\\u00e9 avant l''engagement financier pour Osorus (Popup Expos)</li><li data-block-key=\\\"8pfl3\\\">Le Conseil prend note de la plainte de Chris MANN pour l''appel \\u00e0 SImon LAISNEY, Pr\\u00e9sident de la SCIC Plateau Urbain, en ligne avec les actions du collectif Aurore, Plateau-Urbain et Yes We Camp depuis 2017</li><li data-block-key=\\\"91fla\\\">Les modifications statutaires cons\\u00e9quentes \\u00e0 la d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral sont \\u00e0 pr\\u00e9senter prochainement.</li><li data-block-key=\\\"b85bf\\\">Il y a du pain sur la planche</li></ol><h2 data-block-key=\\\"30k1q\\\">D\\u00e9roulement</h2><p data-block-key=\\\"5iqvn\\\">Le 19e Conseil des voisins s''est d\\u00e9roul\\u00e9 le 7 ao\\u00fbt 2023 en deux temps selon les contraintes des participants: d''abord \\u00e0 11h puis apr\\u00e8s \\u00e0 19h15.</p><h2 data-block-key=\\\"27s0n\\\">Sujets trait\\u00e9s</h2><ol><li data-block-key=\\\"9v14k\\\">Intimidation par la gestionnaire des Arches citoyennes le 4 ao\\u00fbt</li><li data-block-key=\\\"677hl\\\">Discussion sur les activit\\u00e9s \\u00e0 continuer et \\u00e0 d\\u00e9marrer</li><li data-block-key=\\\"dggl0\\\">Les arts voisins</li><li data-block-key=\\\"b64a0\\\">Les finances</li><li data-block-key=\\\"fb6dt\\\">D''autres sujets divers</li></ol><h2 data-block-key=\\\"713us\\\">Invitation et Compte-rendu</h2><p data-block-key=\\\"2f4dm\\\"><a href=\\\"https://www.lesgrandsvoisins.com/structure/conseils-des-voisins/19e-conseil/invitation/?ref=blog.lesgrandsvoisins.com\\\">Invitation au 19e Conseil des Voisins</a></p><p data-block-key=\\\"1jlg3\\\"><a href=\\\"https://www.lesgrandsvoisins.com/structure/conseils-des-voisins/19e-conseil/compte-rendu/?ref=blog.lesgrandsvoisins.com\\\">Compte-rendu du 19e conseil des voisins</a></p><p data-block-key=\\\"e9qsb\\\"></p><embed embedtype=\\\"image\\\" format=\\\"fullwidth\\\" id=\\\"24\\\" alt=\\\"\\\"/><p data-block-key=\\\"7dfgd\\\"></p>\", \"id\": \"fca1608c-0d87-4f34-8843-ce76f8d20bfe\"}]", "date": "2023-08-23T12:00:00Z", "live": true, "path": "0001000200030009", "slug": "19e-conseil-des-voisins", "depth": 4, "owner": 1, "title": "19e Conseil des Voisins", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/19e-conseil-des-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "19e Conseil des Voisins", "entry_tags": [{"pk": 21, "tag": 10, "content_object": 15}, {"pk": 22, "tag": 15, "content_object": 15}], "go_live_at": null, "draft_title": "19e Conseil des Voisins", "content_type": 71, "header_image": 23, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "a33f2c8d-6c90-4978-83db-a2fd43e36084", "entry_categories": [{"pk": 11, "page": 15, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "blue-cumulus", "search_description": "Les votes connexes aux conclusions sont toujours à venir.\r\n\r\nNous actons la démission de Maël et donne pouvoir à Chris MANN pour faire les démarches connexes\r\nUn prévisionnel est à établir avant d''engager la commande de 3000 euros auprès d''Osorus (Popup Expos)\r\nUne dette de 1000 euros au Bureau du Créateur est à accorder en priorité avant l''engagement financier pour Osorus (Popup Expos)\r\nLe Conseil prend note de la plainte de Chris MANN pour l''appel à SImon LAISNEY, Président de la SCIC Plateau Urbain, en ligne avec les actions du collectif Aurore, Plateau-Urbain et Yes We Camp depuis 2017\r\nLes modifications statutaires conséquentes à la déclaration d''intérêt général sont à présenter prochainement.\r\nIl y a du pain sur la planche", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '15', 1, 71, 1, '19e Conseil des Voisins');
INSERT INTO public.wagtailcore_revision VALUES (46, '2024-05-05 18:09:56.445673+00', '{"pk": 20, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"ms4b9\\\">Robert Baldrige has been trotting the earth from a Quaker Center in an abandoned volcano in South America to the home to the Dali Lama for a half-century. At first, Robert''s main medium was post cards. As I understand it, should you find yourself on the list, you would receive many postcards. Today, in the land of Instagram and WhatsApp, Robert is still the adolescent storyteller, and we all may be his parents. What did you learn at school today? What friends did you make? Hang in there, fellow Robert-parents, we are in for the ride of our lives!</p><p data-block-key=\\\"2bnep\\\">So the discussion today is about more than putting the proverbial fingerpainting on the fridge. The problem is that these are good fingerprintings, well photography, not only in their own rights, but in the story that they implicitly tell. This is the point where any article about Robert usually starts the name-dropping, in perfect imitation of Robert himself, so I will name-drop too: Robert Baldridge. It''s time that on drop that name: Robert Baldridge. It is sufficient.</p><p data-block-key=\\\"8968t\\\">But what''s the point ?</p><p data-block-key=\\\"ch94e\\\">The goal of my process with Robert this morning is to figure out what to do with the fingerpaintings of great persons around great stories, be they published on the fridge or in, let''s say Vanity Times (if such a thing exists). It was tough getting into the subject because young Robert does the fingerpainting and sees the fingerpainting when in the kitchen or in Vanity Times (speaking metaphorically).</p><p data-block-key=\\\"f2v71\\\">What should you do when you meet this storyteller? Robert takes pictures. So, what? You may ask yourself what Robert would do with these photos, or you may ask what you could do with these photos.</p><p data-block-key=\\\"a779v\\\">Should you have a Robert Baldrige photo, there are two non-negotiables.</p><ol><li data-block-key=\\\"aelsd\\\">The most basic thing to do is to attribute the work to \\u00a0(c) Robert Baldrige with preferably a link to the @RobertBaldrigeOfficial Instagram account.</li><li data-block-key=\\\"4si4j\\\">The second basic thing is to allow Robert to see the use of his work, to inform him of it. Robert also avails himself to the creative process around the publication of his work, but is amenable to publishers.</li></ol><p data-block-key=\\\"64ntb\\\">That doesn''t mean you are in the clear. The two above conditions apply invariably to any use. Normal compensation considerations are applicable, and can be in cash, by check (in dollars) or by wire transfer.</p>\", \"id\": \"d5fa4f4d-617c-4f13-826a-f5bd3dc17ebc\"}, {\"type\": \"image\", \"value\": {\"title\": \"Gilles Cl\\u00e9ment par Robert Baldridge\", \"heading_tag\": \"h3\", \"image\": 34, \"alt\": \"Montage and Photos by Robert Baldrige (c) of Gilles Cl\\u00e9ment from a Thanks for Nothing event at the Louvre on May 3, 2023\", \"caption\": \"Montage and Photos by Robert Baldrige (c) of Gilles Cl\\u00e9ment from a Thanks for Nothing event at the Louvre on May 3, 2023\", \"url\": \"https://www.instagram.com/robertbaldridgeofficial/?ref=blog.lesgrandsvoisins.com\"}, \"id\": \"d57b7609-c156-4895-be2e-1c7fb91ca0bc\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"5emxu\\\">In the case above, Robert took photos of Gilles Cl\\u00e9ment, an environmental public figure, and had proposed to meet Gilles to learn more of his work. Robert was at an event with me. Gilles was initially the story-teller, but Robert began resonating Gilles story with a photographic story from Robert. (Robert sometimes calls himself a \\\"storyteller\\\".)</p><p data-block-key=\\\"jt19\\\">During the event, Robert showed me the black-and-white-on-black-background portrait (the small one) of the above montage (done from his iPhone by the way). I wondered, what is to become of this portrait. I prefer the original, but I can only crop the copy I have from the above montage below.</p><p data-block-key=\\\"b8a63\\\"></p><embed embedtype=\\\"image\\\" format=\\\"fullwidth\\\" id=\\\"35\\\" alt=\\\"photo-baldrobe-202305-smallbwportrait\\\"/><p data-block-key=\\\"52lg9\\\">However, tonight isn''t the point. The point is about an orchestra on a glacier in Norway some time ago. Robert was hired for a photo shoot of a protest orchestra somewhere in Scandinavia, I think Norway, where a whole orchestra went atop a glacier. My imagination in memory has a photo where Robert may have take a photo of the event from a hydro-carbon monstrosity known as a helicopter.</p><p data-block-key=\\\"bcq3j\\\">[Note to self, ask Robert for the Glacier Orchestra photo to put here.]</p><p data-block-key=\\\"f5nv5\\\">One of the speakers at the <a href=\\\"https://thanksfornothing.fr/project/colloque-mois-art-engagement-2023/?ref=blog.lesgrandsvoisins.com\\\">Thanks for Nothing Colloque on Arts et Engagement 2023</a>, Camille Etienne, would dance in the artic in her web production <a href=\\\"https://www.tv5mondeplus.com/fr/details/vod/107156818_74079A?ref=blog.lesgrandsvoisins.com\\\">Pourquoi on se bat</a>. That speaker reminded me of Robert''s shoot. The clich\\u00e9 isn''t on Google, shoot. I didn''t think it would be. Robert is sensitive because, and this is understandable, the viewing of Robert''s work starts in the moment the shutter stutters. The musicians knew this, but where are they?</p><p data-block-key=\\\"7140m\\\">[Maybe find the photo of the chamber-orchestra outside projected at the Louvre here]</p><p data-block-key=\\\"2fl1r\\\">All this brings us to \\\"<b>the point</b>\\\" of this article, how is a photographic artist to adapt to today''s immaterial world. Art used to be material. Photos, even two-dimensional, used to be objects. There used to be edges. Now, everything is edgy. My goal here is to curate for the wellbeing of the world, but I fear that photograph of the orchestra on the glacier for a healthy and harmonious planet, mother Earth, is as endangered as the glacier itself, or at least the photographer and the musicians de par our own mortalities. The question is useful posterity for collective prosperity, not just Robert''s.</p><p data-block-key=\\\"5fas8\\\">In order to achieve some possibility at posterity, I need the works. Most of them are on a hard drive, somewhere. \\u00a0Now, I''ve been a photographer too, and a geek, the computer kind of geek (and other kinds of geek too, but that is for another story). At Les Grands Voisins, we have launched a social computer room \\\"Creative Digital\\\" as part of <a href=\\\"https://blog.lesgrandsvoisins.com/tag/resdigita/\\\">our digital activities</a> (also called ResDigita). One reason for this is to provide digital locker rooms for the computerless or the computer-vulnerable. Thus we have the talent to be custodians.</p><p data-block-key=\\\"cpol5\\\">In Robert''s case, we are talking about work that might be bigger than Robert, and I also would like for <a href=\\\"https://blog.lesgrandsvoisins.com/tag/arts/\\\">our artistic activities</a> (also called Les Arts Voisins) to be able to curate something worthwhile both for Robert and for the world. This brings me to the heart of why I am writing this article, an article that is becoming a little longer than article I usually write.</p><p data-block-key=\\\"1t2ff\\\">How do you value \\\"a Baldridge?\\\"</p><p data-block-key=\\\"6q35f\\\">Robert may come to an agreement with you about how you may use his work. The base rules are detailed above: attribute the work to Robert, preferably with a link to the <a href=\\\"https://www.instagram.com/robertbaldridgeofficial/?ref=blog.lesgrandsvoisins.com\\\">RobertBaldridgeOfficial Instagram</a> account, and involve Robert in the publication process. But that doesn''t help much, or might not be precise enough. Let''s take this one step further.</p><p data-block-key=\\\"2ims4\\\">Engage an email exchange with Robert via RobertBaldridge at hotmail.com (I really got to help Robert use a domain name that Robert controls) asking for permission, preferable with some kind of compensation, preferably money, but it could be troc. If you agree to the troc, most likely, and by default, Robert will grant you an <a href=\\\"https://creativecommons.org/licenses/by-nc-sa/4.0/legalcode?ref=blog.lesgrandsvoisins.com\\\">CC-BY-NC-SA</a> license after you have agreed to the two terms above (I repeat):</p><ol><li data-block-key=\\\"3rpti\\\">Attribution (c) <a href=\\\"https://www.instagram.com/robertbaldridgeofficial/?ref=blog.lesgrandsvoisins.com\\\">Robert Baldridge</a> (integrated in the BY part of the CC-BY-NC-SA)</li><li data-block-key=\\\"cen09\\\">Initial publication with Robert''s involvement on some level resulting in Robert''s traditional \\\"that''s fine\\\"</li></ol><p data-block-key=\\\"3ls27\\\">You should also compensate Robert somehow: cash, check or wiretransfer in monetary terms for the time being ; or maybe some kind of barter. Other, preferably non-exclusive commercial uses are also available. I guess, if Robert grants CC-BY-NC-SA (with compensation and involvement in initial publication), one could credit with \\\"(c) <a href=\\\"https://www.instagram.com/robertbaldridgeofficial/?ref=blog.lesgrandsvoisins.com\\\">Robert Baldridge</a> 2023 <a href=\\\"https://creativecommons.org/licenses/by-nc-sa/4.0/legalcode?ref=blog.lesgrandsvoisins.com\\\">CC-BY-NC-SA</a>\\\".</p><p data-block-key=\\\"5j\\\">This legal framework for setting edges around Robert''s body of work is my proposition. I think Robert can use it. There is one sticking point in the CC-BY-NC-SA license that Robert told me he wants to use. I am not sure that Robert understands fully that after the first license is accorded, all further uses distributed by the initial licensee only need the <a href=\\\"https://creativecommons.org/licenses/by-nc-sa/4.0/legalcode?ref=blog.lesgrandsvoisins.com\\\">CC-BY-NC-SA</a> conditions. Therefor, maybe it would be better that Robert be credited without necessarily mentioning the <a href=\\\"https://creativecommons.org/licenses/by-nc-sa/4.0/legalcode?ref=blog.lesgrandsvoisins.com\\\">CC-BY-NC-SA</a> in the credit explicitly... I think there may be a creative digital-geek thing that is simpler than you would imagine here. I have an idea! For now, the \\\"(c) <a href=\\\"https://www.instagram.com/robertbaldridgeofficial/?ref=blog.lesgrandsvoisins.com\\\">Robert Baldridge</a> 2023\\\" accreditation with an implicit, non-publicized <a href=\\\"https://creativecommons.org/licenses/by-nc-sa/4.0/legalcode?ref=blog.lesgrandsvoisins.com\\\">CC-BY-NC-SA</a> is probably the better way to go about Robert''s past work. Of course, if anyone asks, <a href=\\\"https://creativecommons.org/licenses/by-nc-sa/4.0/legalcode?ref=blog.lesgrandsvoisins.com\\\">CC-BY-NC-SA</a> grants the licensee the distribution rights by the same terms even though the initial licensee probably had to agree to extra conditions to obtain the initial <a href=\\\"https://creativecommons.org/licenses/by-nc-sa/4.0/legalcode?ref=blog.lesgrandsvoisins.com\\\">CC-BY-NC-SA</a> (consideration / compensation, collaboration / creative process even though Robert is Easy ;) ).</p><p data-block-key=\\\"ar69t\\\">Robert''s work is experiential. It is also performance art. Robert is a visible photographer, exact as visible as any blind person. His art is himself at the taking of the picture. His art is in the discussion the picture with you. His art is in many different relationships. It is a beautiful art with blurred edges.</p><p data-block-key=\\\"b52h4\\\">As far as preparation for publication is concerned, Robert won''t give you a hard time.</p><p data-block-key=\\\"aq26n\\\">Enjoy.</p>\", \"id\": \"f55c3ad9-1178-4031-8838-d736c8f58ad0\"}]", "date": "2023-05-11T10:00:00Z", "live": false, "path": "000100020003000E", "slug": "thoughts-about-creative-commons-cc-by-nc-sa-en", "depth": 4, "owner": 1, "title": "Thoughts about Creative Commons CC-BY-NC-SA (EN)", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/thoughts-about-creative-commons-cc-by-nc-sa-en/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Thoughts about Creative Commons CC-BY-NC-SA (EN)", "entry_tags": [{"pk": 36, "tag": 15, "content_object": 20}], "go_live_at": null, "draft_title": "Thoughts about Creative Commons CC-BY-NC-SA (EN)", "content_type": 71, "header_image": 34, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [1], "header_cta_link": null, "header_cta_text": null, "latest_revision": 45, "translation_key": "50049e8c-02e2-4981-a186-d65241a79bd3", "entry_categories": [{"pk": 16, "page": 20, "category": 1}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "purple-glycine", "search_description": "Article about terms and conditions around storytelling incorporating photography as creative process including projection (not to say \"publication\") in the storytelling process.\r\n\r\nHow is a photographic artist to adapt to today''s immaterial world. Art used to be material. Photos, even two-dimensional, used to be objects. There used to be edges. Now, everything is edgy. My goal here is to curate for the wellbeing of the world, but I fear that photograph of the orchestra on the glacier for a healthy and harmonious planet, mother Earth, is as endangered as the glacier itself, or at least the photographer and the musicians de par our own mortalities.", "wagtail_admin_comments": [], "has_unpublished_changes": true, "latest_revision_created_at": "2024-05-05T18:08:21.753Z"}', NULL, '20', 1, 71, 1, 'Thoughts about Creative Commons CC-BY-NC-SA (EN)');
INSERT INTO public.wagtailcore_revision VALUES (53, '2024-05-05 18:37:44.605017+00', '{"pk": 23, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"qdho9\\\">Je me permets de citer Pauline du Service D\\u00e9mocratie Locale de la Mairie du 14e qi a tr\\u00e8s bien r\\u00e9sum\\u00e9 notre r\\u00e9union comme suite:</p><p data-block-key=\\\"446bu\\\">Nous avons pu \\u00e9changer sur les 3 aspects de votre projet :</p><ul><li data-block-key=\\\"fp6ap\\\">Du mat\\u00e9riel informatique pour proposer des ateliers d''accessibilit\\u00e9 num\\u00e9rique, qui soit pliable comme l''espace de la Ressourcerie d\\u00e9di\\u00e9 est polyvalent. Il servirait aussi pour \\u00e9ventuellement faire des ateliers dans d''autres structures (Table des Mati\\u00e8res, Moulin \\u00e0 Caf\\u00e9 ?). Ce mat\\u00e9riel pourrait \\u00e9ventuellement \\u00eatre r\\u00e9cup\\u00e9r\\u00e9 aupr\\u00e8s d''entreprises qui n''en ont plus l''utilit\\u00e9</li><li data-block-key=\\\"d5vv\\\">du mat\\u00e9riel de prise vid\\u00e9o, qui remplacerait l''imprimante 3D du projet initial car plus ouvert et accessible</li><li data-block-key=\\\"7ntqd\\\">des serveurs afin de proposer un service solidaire de sauvegarde s\\u00e9curis\\u00e9e des documents num\\u00e9riques, et mat\\u00e9rialiser aussi le \\u00ab cloud \\u00bb</li></ul><p data-block-key=\\\"b3i43\\\">Deux autres \\u00e9l\\u00e9ments sur lesquels Chris Mann, avec l''association les Grands Voisins, avance :</p><ul><li data-block-key=\\\"all88\\\">Chercher des pistes pour demander aux Entreprises de r\\u00e9cup\\u00e9rer une partie de leur mat\\u00e9riel quand il est renouvel\\u00e9</li><li data-block-key=\\\"62pnk\\\">Continuer \\u00e0 faire du lien avec d''autres associations \\u00e0 impliquer, notamment dans l''optique de cr\\u00e9er un socle de b\\u00e9n\\u00e9voles qui s''int\\u00e9ressent \\u00e0 la question (APRIL, fondation Valentin H\\u00fcey)</li></ul><p data-block-key=\\\"5g49b\\\">Il y a aussi des consid\\u00e9rations qui touchent aux aspects administratifs. Notre association coop\\u00e9rative, bien que nous sommes en partenariat avec la Ressourcerie Cr\\u00e9ative, est \\u00e0 l''\\u00e9cart de la dotation lors de l''arr\\u00eat\\u00e9 de Madame la Maire de Paris Hildago le 17 mars 2023.</p>\", \"id\": \"16c0ebce-ee5c-4aec-86ee-cc444b93db47\"}]", "date": "2023-04-28T10:00:00Z", "live": true, "path": "000100020003000H", "slug": "lauriat-du-budget-paritcipatif-de-paris-14e-le-numerique-creatif-des-grands-voisins", "depth": 4, "owner": 1, "title": "Lauriat du Budget Paritcipatif de Paris 14e : Le numérique créatif des Grands Voisins", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/lauriat-du-budget-paritcipatif-de-paris-14e-le-numerique-creatif-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Lauriat du Budget Paritcipatif de Paris 14e : Le numérique créatif des Grands Voisins", "entry_tags": [{"pk": 45, "tag": 29, "content_object": 23}, {"pk": 46, "tag": 15, "content_object": 23}], "go_live_at": null, "draft_title": "Lauriat du Budget Paritcipatif de Paris 14e : Le numérique créatif des Grands Voisins", "content_type": 71, "header_image": 39, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [3, 2], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "717dac9a-d8cb-4da0-bce2-2f04c6ef63f6", "entry_categories": [{"pk": 19, "page": 23, "category": 2}, {"pk": 20, "page": 23, "category": 3}], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "pink-tuile", "search_description": "Il y a aussi des considérations qui touchent aux aspects administratifs. Notre association coopérative, bien que nous sommes en partenariat avec la Ressourcerie Créative, est à l''écart de la dotation lors de l''arrêté de Madame la Maire de Paris Hidalgo le 17 mars 2023.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '23', 1, 71, 1, 'Lauriat du Budget Paritcipatif de Paris 14e : Le numérique créatif des Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (38, '2024-05-05 17:55:28.535852+00', '{"pk": 19, "body": "[{\"type\": \"quote\", \"value\": {\"image\": 29, \"quote\": \"Comment \\u00eatre d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral, vraiment ? Peu apr\\u00e8s notre d\\u00e9claration, nous faillons peut-\\u00eatre d\\u00e9j\\u00e0. C''est un cadeau pour nous apprendre !\", \"author_name\": \"Madame la Ministre avec Ma\\u00ebl, Vice-Pr\\u00e9sident des Grands Voisins, \\u00e0 Inclusiv''Day 2023\", \"author_title\": \"\", \"color\": \"brown-opera\"}, \"id\": \"48c5d31b-88c6-442f-8046-f7c5bb180cb7\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"o42mj\\\">Madame la Maire du 14e arrondissement conseille de laisser tomber l''argumentaire dit \\u00ab de responsabilit\\u00e9 de la marque \\u00bb Les Grands Voisins. J''avais utilis\\u00e9 l''argument le 25 mars 2023 sur l''ancien site Saint Vincent de Paul lorsque un cadre sup\\u00e9rieur de l''association Aurore m''a publiquement snob\\u00e9. Je n''\\u00e9tais pas en forme et l''article de blog \\u00e0 ce sujet \\u00e9tait aussi \\u00e9crit d''une posture d\\u00e9fensive.</p><p data-block-key=\\\"bjvv\\\">Je retiens de la suggestion une posture qui se base plus sur le pr\\u00e9sent et sur l''avenir !</p><p data-block-key=\\\"5gcf3\\\">Nous avons vot\\u00e9 une prise de position forte au 17e Conseil des Voisins pour nous d\\u00e9clarer organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. La d\\u00e9claration d''organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral requiert trois conditions:</p><ol><li data-block-key=\\\"ffudt\\\">une gestion d\\u00e9sint\\u00e9ress\\u00e9e,</li><li data-block-key=\\\"b8cbc\\\">pas d''activit\\u00e9 lucrative,</li><li data-block-key=\\\"eif2t\\\">pas de fonctionnement au profit d''un cercle restreint.</li></ol><p data-block-key=\\\"focnb\\\">Pas plus tard que le 10 mai, d\\u00e9j\\u00e0 nous sommes test\\u00e9s sur les points 1 et 3 de par la visite de Madame la Ministre d\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq \\u00e0 Inclusiv''Day.</p>\", \"id\": \"68fe7661-6097-45c9-b54c-7c670b184e0f\"}, {\"type\": \"image\", \"value\": {\"title\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"heading_tag\": \"h3\", \"image\": 29, \"alt\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"caption\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"url\": \"\"}, \"id\": \"6c1bd7dc-4478-4ecf-8536-e4348a009777\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"503h4\\\">Ma\\u00ebl, Vice Pr\\u00e9sident des Grands Voisins, et \\u00e0 Inclusiv''Day pour Les Grands Voisins a envoy\\u00e9 une lettre de sollicitation pour le compte de son association La Caravane CECAI un soutien, un financement, et un encouragement de financement de tiers.</p><p data-block-key=\\\"5o9uj\\\">Cette action m''incite \\u00e0 proposer urgemment un organe de contr\\u00f4le de notre adh\\u00e9rence \\u00e0 nos d\\u00e9clarations (Contrat pour le Web, Coop, Int\\u00e9r\\u00eat G\\u00e9n\\u00e9ral). Ma\\u00ebl pourrait \\u00eatre alors en erreur au regard de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral.</p><p data-block-key=\\\"blc0r\\\">La discussion \\u00e0 avoir pour Les Grands Voisins avec Mme. la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap tourne autour du <a href=\\\"https://blog.lesgrandsvoisins.com/manifeste\\\">Manifeste des Grands Voisins</a>. C''est pour cela que nous aurions besoin d''un comit\\u00e9 de contr\\u00f4le de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Pour comprendre l''erreur, imaginez que vous faite partie d''un organisme qui pr\\u00e9tend \\u00eatre d\\u00e9sint\\u00e9ress\\u00e9, puis vous apprenez que les dirigeants obtiennent des avantages pour des organismes tiers dans leur gestion de l''organisme.</p><p data-block-key=\\\"e1kvg\\\">Comme toute Grande Voisine et tout Grand Voisin, Ma\\u00ebl A.N.C. et la Caravane CECAI ont des projets louables, mais Les Grands Voisins ne peuvent pas soutenir directement ou indirectement aucune des initiatives. Soutenir une initiative porte automatiquement pr\\u00e9judice, \\u00e0 mon sens, \\u00e0 toutes les initiatives que nous ne soutenons pas. Malheureusement, cette tendance a une historique dans les Grands Voisins.</p>\", \"id\": \"ecf3d596-bcf9-4d59-b8dd-ccd1f6171916\"}]", "date": "2024-05-05T17:45:00Z", "live": false, "path": "000100020003000D", "slug": "la-suite-des-grands-voisins", "depth": 4, "owner": 1, "title": "La Suite des Grands Voisins", "locale": 1, "locked": false, "authors": [], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/la-suite-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "La Suite des Grands Voisins", "entry_tags": [], "go_live_at": null, "draft_title": "La Suite des Grands Voisins", "content_type": 71, "header_image": null, "header_large": false, "header_darken": false, "live_revision": null, "show_in_menus": false, "blog_categories": [], "header_cta_link": null, "header_cta_text": null, "latest_revision": 37, "translation_key": "100abfb8-1567-430c-8be0-f29103bb14e5", "entry_categories": [], "header_cta_label": null, "header_with_title": false, "last_published_at": null, "first_published_at": null, "header_color_class": null, "search_description": "Comment être d''intérêt général, vraiment ? Peu après notre déclaration, nous faillons peut-être déjà. C''est un cadeau pour nous apprendre !", "wagtail_admin_comments": [], "has_unpublished_changes": true, "latest_revision_created_at": "2024-05-05T17:54:58.351Z"}', NULL, '19', 1, 71, 1, 'La Suite des Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (42, '2024-05-05 18:02:12.908805+00', '{"pk": 19, "body": "[{\"type\": \"quote\", \"value\": {\"image\": 29, \"quote\": \"Comment \\u00eatre d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral, vraiment ? Peu apr\\u00e8s notre d\\u00e9claration, nous faillons peut-\\u00eatre d\\u00e9j\\u00e0. C''est un cadeau pour nous apprendre !\", \"author_name\": \"Madame la Ministre avec Ma\\u00ebl, Vice-Pr\\u00e9sident des Grands Voisins, \\u00e0 Inclusiv''Day 2023\", \"author_title\": \"\", \"color\": \"brown-opera\"}, \"id\": \"48c5d31b-88c6-442f-8046-f7c5bb180cb7\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"o42mj\\\">Madame la Maire du 14e arrondissement conseille de laisser tomber l''argumentaire dit \\u00ab de responsabilit\\u00e9 de la marque \\u00bb Les Grands Voisins. J''avais utilis\\u00e9 l''argument le 25 mars 2023 sur l''ancien site Saint Vincent de Paul lorsque un cadre sup\\u00e9rieur de l''association Aurore m''a publiquement snob\\u00e9. Je n''\\u00e9tais pas en forme et l''article de blog \\u00e0 ce sujet \\u00e9tait aussi \\u00e9crit d''une posture d\\u00e9fensive.</p><p data-block-key=\\\"bjvv\\\">Je retiens de la suggestion une posture qui se base plus sur le pr\\u00e9sent et sur l''avenir !</p><p data-block-key=\\\"5gcf3\\\">Nous avons vot\\u00e9 une prise de position forte au 17e Conseil des Voisins pour nous d\\u00e9clarer organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. La d\\u00e9claration d''organisme d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral requiert trois conditions:</p><ol><li data-block-key=\\\"ffudt\\\">une gestion d\\u00e9sint\\u00e9ress\\u00e9e,</li><li data-block-key=\\\"b8cbc\\\">pas d''activit\\u00e9 lucrative,</li><li data-block-key=\\\"eif2t\\\">pas de fonctionnement au profit d''un cercle restreint.</li></ol><p data-block-key=\\\"focnb\\\">Pas plus tard que le 10 mai, d\\u00e9j\\u00e0 nous sommes test\\u00e9s sur les points 1 et 3 de par la visite de Madame la Ministre d\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq \\u00e0 Inclusiv''Day.</p>\", \"id\": \"68fe7661-6097-45c9-b54c-7c670b184e0f\"}, {\"type\": \"image\", \"value\": {\"title\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"heading_tag\": \"h3\", \"image\": 29, \"alt\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"caption\": \"Ma\\u00ebl A\\u00efnine N\\u00e9ma Ch\\u00e9rif pour Les Grands Voisins avec Madame la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap, Genevi\\u00e8ve Darrieussecq\", \"url\": \"\"}, \"id\": \"6c1bd7dc-4478-4ecf-8536-e4348a009777\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"503h4\\\">Ma\\u00ebl, Vice Pr\\u00e9sident des Grands Voisins, et \\u00e0 Inclusiv''Day pour Les Grands Voisins a envoy\\u00e9 une lettre de sollicitation pour le compte de son association La Caravane CECAI un soutien, un financement, et un encouragement de financement de tiers.</p><p data-block-key=\\\"5o9uj\\\">Cette action m''incite \\u00e0 proposer urgemment un organe de contr\\u00f4le de notre adh\\u00e9rence \\u00e0 nos d\\u00e9clarations (Contrat pour le Web, Coop, Int\\u00e9r\\u00eat G\\u00e9n\\u00e9ral). Ma\\u00ebl pourrait \\u00eatre alors en erreur au regard de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral.</p><p data-block-key=\\\"blc0r\\\">La discussion \\u00e0 avoir pour Les Grands Voisins avec Mme. la Ministre D\\u00e9l\\u00e9gu\\u00e9e au Handicap tourne autour du <a href=\\\"https://blog.lesgrandsvoisins.com/manifeste\\\">Manifeste des Grands Voisins</a>. C''est pour cela que nous aurions besoin d''un comit\\u00e9 de contr\\u00f4le de notre d\\u00e9claration d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Pour comprendre l''erreur, imaginez que vous faite partie d''un organisme qui pr\\u00e9tend \\u00eatre d\\u00e9sint\\u00e9ress\\u00e9, puis vous apprenez que les dirigeants obtiennent des avantages pour des organismes tiers dans leur gestion de l''organisme.</p><p data-block-key=\\\"e1kvg\\\">Comme toute Grande Voisine et tout Grand Voisin, Ma\\u00ebl A.N.C. et la Caravane CECAI ont des projets louables, mais Les Grands Voisins ne peuvent pas soutenir directement ou indirectement aucune des initiatives. Soutenir une initiative porte automatiquement pr\\u00e9judice, \\u00e0 mon sens, \\u00e0 toutes les initiatives que nous ne soutenons pas. Malheureusement, cette tendance a une historique dans les Grands Voisins.</p>\", \"id\": \"ecf3d596-bcf9-4d59-b8dd-ccd1f6171916\"}, {\"type\": \"image\", \"value\": {\"title\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"heading_tag\": \"h3\", \"image\": 30, \"alt\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"caption\": \"Une initiative \\u00ab SitesFaciles des Grands Voisins \\u00bb en avril 2019\", \"url\": \"\"}, \"id\": \"e77fdcd9-a5bb-4b89-8c1d-36f064548769\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"u2nv8\\\">En avril 2019, certains acteurs ont propos\\u00e9 un \\u00ab SitesFaciles des Grands Voisins \\u00bb dont on comptait princiaplement Les Petits D\\u00e9brouillard et Fr\\u00e9d\\u00e9ric G.. Ce projet avait, \\u00e0 mon sens, probablement d\\u00e9j\\u00e0 cette notion de profiter \\u00e0 un cercle restreint de personnes. Il avait pr\\u00e9vu explicitement un budget de 20.000 euros pour un financement de salaire \\u00e0 mi-temps pour Ma\\u00ebl ANC. Les montants de cette initiative l\\u00e0 \\u00e9taient petits.</p><p data-block-key=\\\"c65u5\\\">Notre d\\u00e9fi est grand. L''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral aurait besoin d''un comit\\u00e9 de contr\\u00f4le. Pour cela, la robustesse de la part de la contribution de Ma\\u00ebl ANC a apport\\u00e9 ses fruits.</p><p data-block-key=\\\"2mg4p\\\">Mon autre vice-pr\\u00e9sident me pose une question par rapport \\u00e0 un projet accessoire que nous avions avec Ma\\u00ebl \\u2013 le <a href=\\\"https://web.resdigita.com/?ref=blog.lesgrandsvoisins.com\\\">web fait main fait maison</a> \\u2013 qui \\u00e9tait con\\u00e7u pour financer notre pr\\u00e9sence \\u00e0 Inclusiv''Day. (A savoir que l''avance nous permettant \\u00e0 aller \\u00e0 Inclusv''Day venait de Ma\\u00ebl.) L''observation de Caroline \\u00e9tait que ce projet aurait pu enfreindre la condition 2 d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Chapeau Caroline !</p>\", \"id\": \"4246292d-7239-4a7f-9644-08e26324869c\"}, {\"type\": \"image\", \"value\": {\"title\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"heading_tag\": \"h3\", \"image\": 31, \"alt\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"caption\": \"Mon erreur probable de proposer en kermesse les activit\\u00e9s b\\u00e9n\\u00e9voles pour le b\\u00e9n\\u00e9fice des Grands Voisins\", \"url\": \"\"}, \"id\": \"8f1f3001-6ec5-4ee3-87a4-c1541d48cea1\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"amgnp\\\">L''id\\u00e9e que j''avais pour le web fait main \\u00e9tait plus \\u00e0 l''image de kermesse d''\\u00e9cole. C''\\u00e9tait une activit\\u00e9 plus symbolique. Pourtant, la question de Caroline ouvre la question exactement o\\u00f9 elle doit aller ! (Et, effectivement, l''activit\\u00e9 de b\\u00e9n\\u00e9volat aurait besoin d''\\u00eatre scrut\\u00e9e.)</p><p data-block-key=\\\"73po8\\\">Il n''y auraient peut-\\u00eatre pas de probl\\u00e8me \\u00e0 mon sens que les acteurs se revendiquent des Grands Voisins dans leurs activit\\u00e9s. C''est un honneur pour Les Grands Voisins. Le probl\\u00e8me vient quand Les Grands Voisins utilise sa position pour promouvoir un acteur. La question est d''actualit\\u00e9 pour le sujet du budget participatif de Paris \\u00ab <a href=\\\"https://blog.lesgrandsvoisins.com/lancement-du-budget-participatif/\\\">le num\\u00e9rique cr\\u00e9atif des Grands Voisins</a> \\u00bb.</p><p data-block-key=\\\"7obtv\\\">Je dis que je suis laur\\u00e9at, car il s''agit d''une activit\\u00e9 de b\\u00e9n\\u00e9volat qui m''est personnelle sur le site Saint Vincent de Paul, \\u00e0 une \\u00e9glise depuis 2016 et au sein d''une resourcerie \\u00e0 Arcueil. C''est une chose que j''aime faire: \\u00e9quiper et animer des salles de sociabilit\\u00e9 num\\u00e9rique. On y aborde d''abord la sociabilit\\u00e9 et apr\\u00e8s le num\\u00e9rique afin de d\\u00e9stigmatiser le num\\u00e9rique. C''est vraiment splendide pour moi et pour beaucoup. Pourtant, effectivement, c''est mon kiffe, mon kiffe perso.</p>\", \"id\": \"f0595ab5-89af-44b0-afc5-552f5af5db1e\"}, {\"type\": \"image\", \"value\": {\"title\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"heading_tag\": \"h3\", \"image\": 32, \"alt\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"caption\": \"La Mairie du 14e en avril 2022 lors de la consid\\u00e9ration du num\\u00e9rique cr\\u00e9atif des Grands Voisins\", \"url\": \"\"}, \"id\": \"98a7f474-4a3d-4001-8607-df497475cf88\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"a0a0n\\\">Entre le 13 et le 17 mars 2023 a eu lieu le Conseil de Paris. Un d\\u00e9bat a eu lieu qui a, \\u00e0 mon sens, contrari\\u00e9 les conditions du budget participatif du 14e arrondissement. En parall\\u00e8le depuis des ann\\u00e9es, il y aurait, je pense, une activit\\u00e9 contre moi au sujet de la marque Les Grands Voisins. Le d\\u00e9bat au Conseil de Paris semble avoir plac\\u00e9 la Mairie du 14e et/ou la Mairie de Paris dans un effort probable de certains pour \\u00ab tuer la marque \\u00bb (je site un membre du bureau de l''association France Tiers Lieux qui donne une bonne caract\\u00e9risation de la mouvance contre les Grands Voisins telle qu''ici). La d\\u00e9lib\\u00e9ration sign\\u00e9e par Madame la Maire de Paris (Mme. HILDAGO) avait effectivement redirig\\u00e9 (je pense, d''apr\\u00e8s ce que je vois et les d\\u00e9bats) le financement du num\\u00e9rique cr\\u00e9atif des Grands Voisins \\u00e0 un projet diff\\u00e9rent. (Le projet diff\\u00e9rent aurait \\u00e9t\\u00e9 l''agrandissement de l''atelier de la Resourcerie Cr\\u00e9ative, lorsque le projet \\u00ab <a href=\\\"https://blog.lesgrandsvoisins.com/lancement-du-budget-participatif/\\\">le num\\u00e9rique cr\\u00e9atif des Grands Voisins</a> \\u00bb concernant entre autres <a href=\\\"https://blog.lesgrandsvoisins.com/budget-participatif-avec-la-mairie-du-14e-et-la-ressourcerie-creative/\\\">l''adaptation d''un atelier existant de la Ressourcerie Cr\\u00e9ative dans une approche</a> sp\\u00e9cifique.)</p>\", \"id\": \"ee086cc5-3732-4cdc-bbad-6f856660edcb\"}, {\"type\": \"image\", \"value\": {\"title\": \"Extrait du Conseil de Paris\", \"heading_tag\": \"h3\", \"image\": 33, \"alt\": \"Extrait du Conseil de Paris\", \"caption\": \"Extrait du Conseil de Paris\", \"url\": \"\"}, \"id\": \"96a1b111-502f-42a5-91af-90f551ad7879\"}, {\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"a0a0n\\\">Quand le budget participatif \\u00ab <a href=\\\"https://blog.lesgrandsvoisins.com/lancement-du-budget-participatif/\\\">le num\\u00e9rique cr\\u00e9atif des Grands Voisins</a> \\u00bb \\u00e9tait propos\\u00e9, nous n''avions pas encore pris la partie d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral. Il convient aussi de regarder le budget participatif pour lequel plus de 4000 parisiens du 14e ont vot\\u00e9 pour v\\u00e9rifier son int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral.</p><p data-block-key=\\\"s40q\\\">Il y a aussi la question du <a href=\\\"https://blog.lesgrandsvoisins.com/16e-conseil-des-voisins/\\\">16e Conseil des voisins</a> qui avait vot\\u00e9 pour nommer \\u00ab <a href=\\\"https://www.resdigita.com/?ref=blog.lesgrandsvoisins.com\\\">ResDigita</a> \\u00bb les activit\\u00e9s num\\u00e9riques des Grands Voisins. Comme je suis moi m\\u00eame d\\u00e9brouillard informatique, et pourrais (je ne le fais pas maintenant) y gagner ma vie, il convient aussi une vigilance \\u00e0 cela. Je pense qu''il convient de d\\u00e9sinvestir Les Grands Voisins de la notion de ResDigita.</p><p data-block-key=\\\"baou6\\\">A mon sens l''avenir des Grands Voisins, association d''int\\u00e9r\\u00eat g\\u00e9n\\u00e9ral coop\\u00e9rative, tient en une valeur ajout\\u00e9e: la neutralit\\u00e9 et la transparence avec personnalit\\u00e9, la personnalit\\u00e9 de notre manifeste. Voil\\u00e0 ce que je propose pour le <a href=\\\"https://blog.lesgrandsvoisins.com/18e-conseil/\\\">18e Conseil des Voisins</a> !</p>\", \"id\": \"0b8b04a6-0d57-41f3-a36c-d9890eac9f9e\"}]", "date": "2023-05-23T10:00:00Z", "live": true, "path": "000100020003000D", "slug": "la-suite-des-grands-voisins", "depth": 4, "owner": 1, "title": "La Suite des Grands Voisins", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/la-suite-des-grands-voisins/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "La Suite des Grands Voisins", "entry_tags": [{"pk": null, "tag": 25, "content_object": 19}, {"pk": null, "tag": 15, "content_object": 19}], "go_live_at": null, "draft_title": "La Suite des Grands Voisins", "content_type": 71, "header_image": 29, "header_large": true, "header_darken": true, "live_revision": 41, "show_in_menus": true, "blog_categories": [3], "header_cta_link": null, "header_cta_text": null, "latest_revision": 41, "translation_key": "100abfb8-1567-430c-8be0-f29103bb14e5", "entry_categories": [], "header_cta_label": null, "header_with_title": true, "last_published_at": "2024-05-05T18:01:07.129Z", "first_published_at": "2024-05-05T18:01:07.129Z", "header_color_class": "red-marianne", "search_description": "Comment être d''intérêt général, vraiment ? Peu après notre déclaration, nous faillons peut-être déjà. C''est un cadeau pour nous apprendre !", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-05T18:01:07.105Z"}', NULL, '19', 1, 71, 1, 'La Suite des Grands Voisins');
INSERT INTO public.wagtailcore_revision VALUES (60, '2024-05-06 03:08:44.816885+00', '{"pk": 26, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"7et42\\\">Nous, Les Grands Voisins, avons beaucoup \\u00e0 offrir. Privil\\u00e9giez les services des <a href=\\\"https://www.lesgrandsvoisins.com/dir?ref=blog.lesgrandsvoisins.com\\\">entit\\u00e9s Les Grands Voisins</a>, mais aussi les membres de bureau aimeraient vous offrir avec vos donations quelques services en remerciement dans l''unique objectif de financer nos frais courants, notamment des sites, des expos et de la communication faits main / faits maison avec nos talents individuels ensemble: digital, arts, communication.</p><p data-block-key=\\\"2mge2\\\"><a href=\\\"https://www.lesgrandsvoisins.com/fr/contactus?ref=blog.lesgrandsvoisins.com\\\">Contactez-nous</a></p><h2 data-block-key=\\\"7s065\\\">Dons</h2><p data-block-key=\\\"fbjpt\\\">Les Grands Voisins re\\u00e7oit des donations d\\u00e9ductibles des imp\\u00f4ts. Nous vous \\u00e9tablissons une d\\u00e9claration en fin d''ann\\u00e9e.</p><p data-block-key=\\\"4aafd\\\">Nous lan\\u00e7ons \\u00e9galement une <a href=\\\"https://www.lesgrandsvoisins.com/fun?ref=blog.lesgrandsvoisins.com\\\">campagne de prestations Fun(d)-Raising</a> (dont une partie n''est pas d\\u00e9ductible si vous profitez directement de nos services g\\u00e9niaux).</p><p data-block-key=\\\"dfg4q\\\">Vous pouvez <a href=\\\"https://www.paypal.com/donate/?hosted_button_id=ZPSK2ZPXFEVHG&amp;ref=blog.lesgrandsvoisins.com\\\">d\\u00e9frayer directement sur notre compte paypal</a>. \\u00a0Chris MANN qui avance entre 100 \\u20ac et 200 \\u20ac par mois pour notre socle informatique.</p><p data-block-key=\\\"6hotb\\\"><a href=\\\"https://www.paypal.com/donate/?hosted_button_id=ZPSK2ZPXFEVHG&amp;ref=blog.lesgrandsvoisins.com\\\">Donate via Paypal</a></p><p data-block-key=\\\"aiqng\\\">Voir <a href=\\\"https://www.lesgrandsvoisins.com/services?ref=blog.lesgrandsvoisins.com\\\">notre campagne Fun(d)-raising</a> o\\u00f9 vous pouvez <b>b\\u00e9n\\u00e9ficier des services</b> des administrateurs Les Grands Voisins. Pensez aussi au services des <a href=\\\"https://www.lesgrandsvoisins.com/dir?ref=blog.lesgrandsvoisins.com\\\">entit\\u00e9s Les Grands Voisins</a>.</p><p data-block-key=\\\"2r693\\\">Nous accemptions aujourdhui des dons en liquide ou en ch\\u00e8que adress\\u00e9 \\u00e0 Les Grands Voisins.</p><p data-block-key=\\\"80nei\\\"><a href=\\\"https://www.lesgrandsvoisins.com/fr/contactus?ref=blog.lesgrandsvoisins.com\\\">Contactez-nous pour toute question</a></p><h2 data-block-key=\\\"d6l02\\\">Travail pour financement</h2><p data-block-key=\\\"69k0e\\\">Participer au financement de l''association coop\\u00e9rative ne vous apporterait pas plus individuellement que ne pas participer au financement. C''est notre objectif. N\\u00e9anmoins, nous avons des frais importants en informatique, en communication, et on esp\\u00e8re bient\\u00f4t en location de lieu. En cons\\u00e9quence, en fonction de vos dons, nous aimerions vous assister pour des choses que vous pouvez aussi faire sans nous.</p><ul><li data-block-key=\\\"adpmm\\\">Digital<br/>Chris MANN peut vous aider avec votre ma\\u00eetrise de num\\u00e9rique et l''installation par exemple de tout dispositif que nous utilisons pour vous (logiciels libres).</li><li data-block-key=\\\"cru0a\\\">Arts<br/>Ma\\u00ebl ANC peut organiser une expo de son art ou d''autres artistes dans votre espace, et de prestations de m\\u00e9diation culturelle et artistique.</li><li data-block-key=\\\"93qb2\\\">Communication<br/>Caroline LHOMME peut \\u00e9crire votre paragraphe \\u00e0 sa mani\\u00e8re sur votre listing d''annuaire Les Grands Voisins. (Le listing dans l''annuaire est disponible \\u00e0 toute entit\\u00e9 Grande Voisine)</li></ul><p data-block-key=\\\"108tl\\\">Ensemble, nous pouvons nous pencher sur les sujets que vous voulez.</p>\", \"id\": \"9d55ee0b-e703-48e3-98fc-ba5600a96ec1\"}]", "date": "2023-04-26T06:00:00Z", "live": true, "path": "000100020003000K", "slug": "appel-a-fonds-avec-fun", "depth": 4, "owner": 1, "title": "Appel à Fonds avec Fun", "locale": 1, "locked": false, "authors": [1], "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/blog/appel-a-fonds-avec-fun/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "Appel à Fonds avec Fun", "entry_tags": [], "go_live_at": null, "draft_title": "Appel à Fonds avec Fun", "content_type": 71, "header_image": 44, "header_large": true, "header_darken": true, "live_revision": null, "show_in_menus": true, "blog_categories": [], "header_cta_link": null, "header_cta_text": null, "latest_revision": null, "translation_key": "521e32a1-4a1b-44af-9e6f-e6fd5189f733", "entry_categories": [], "header_cta_label": null, "header_with_title": true, "last_published_at": null, "first_published_at": null, "header_color_class": "pink-macaron", "search_description": "LesGrandsVoisins.com appel à funs ! Vos dons, mécenants, achats de services éventuels etc. sont nécessaires à la survie de notre association coopérative.", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": null}', NULL, '26', 1, 71, 1, 'Appel à Fonds avec Fun');
INSERT INTO public.wagtailcore_revision VALUES (170, '2024-05-26 16:06:39.583011+00', '{"pk": 36, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"kokwt\\\">Aux Grands Voisins nous avons <a linktype=\\\"page\\\" id=\\\"30\\\">un manifeste</a> \\u00e9dit\\u00e9e par Chris Mann avec des contributions de plus de cent personnes sur deux grands ateliers et plusieurs s\\u00e9ances de travail sur une p\\u00e9riode d''un mois en 2017. Chacun a un avis qui est le sien sur ce que c''est \\u00eatre Grands Voisins et chacun a raison.</p><p data-block-key=\\\"b4dti\\\">L''objectif des activit\\u00e9s dans notre cellule civisme est de promouvoir ce qu''il faut pour avoir les possibilit\\u00e9s de rencontres ou non. Cela d\\u00e9coule m\\u00eame de l''all\\u00e9gorie du voisin ; l''on peut rester chez soi, l''on peut sortir, l''on peut inviter d''autres.</p><p data-block-key=\\\"aovvo\\\">Concr\\u00e8tement, nous pouvons commencer par un annuaire QuiQuoiEtc qui permet de r\\u00e9f\\u00e9rencer les voisins qui veulent bien. Cela permet une communititude et des possibilit\\u00e9s de compr\\u00e9hensions, empathies et entraides int\\u00e9ressantes.</p><p data-block-key=\\\"4j94g\\\"></p>\", \"id\": \"e569e14e-92d4-476c-96b3-2b221f2ce890\"}]", "live": true, "path": "000100020007", "slug": "civisme", "depth": 3, "owner": 1, "title": "Civisme", "locale": 1, "locked": false, "expired": false, "alias_of": null, "numchild": 1, "url_path": "/home/civisme/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "", "go_live_at": null, "draft_title": "Civisme", "content_type": 25, "header_image": null, "header_large": false, "header_darken": false, "live_revision": 162, "show_in_menus": true, "header_cta_link": null, "header_cta_text": null, "latest_revision": 162, "translation_key": "8a173765-a6a8-40e1-89ea-de7f1986d532", "contentpage_tags": [], "header_cta_label": null, "header_with_title": false, "last_published_at": "2024-05-24T14:55:42.145Z", "first_published_at": "2024-05-07T07:28:54.538Z", "header_color_class": null, "search_description": "Aux Grands Voisins nous avons un manifeste éditée par Chris Mann avec des contributions de plus de cent personnes sur […]", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-24T14:55:42.122Z"}', NULL, '36', 1, 25, 1, 'Civisme');
INSERT INTO public.wagtailcore_revision VALUES (152, '2024-05-24 11:52:47.384259+00', '{"pk": 50, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"upcmn\\\">Venez assister \\u00e0 notre \\u00e9v\\u00e9nement magique de rencontres, d\\u00e9bats et partages, pour notre 22\\u00e8me Conseils des Grands Voisins. Nous vous attendons nombreux pour ce moment inoubliable au caf\\u00e9 LPP dans le 14\\u00e8me arrondissements de paris! Gr\\u00e2ce \\u00e0 vous nos projets deviendront r\\u00e9alit\\u00e9!</p>\", \"id\": \"ac7533ae-c366-4707-bc22-a4939919e736\"}, {\"type\": \"callout\", \"value\": {\"title\": \"Ordre du jour\", \"text\": \"Num\\u00e9rique\\r\\nCivisme\\r\\nCommunication\", \"heading_tag\": \"h3\"}, \"id\": \"3e989a28-129f-44ab-9d9a-1087fc93dcd6\"}, {\"type\": \"multicolumns\", \"value\": {\"bg_image\": 58, \"bg_color_class\": \"green-tilleul-verveine\", \"bg_color\": \"\", \"title\": \"Nous tiendrions le 22e Conseil des Voisins sur trois rencontres :\", \"heading_tag\": \"h2\", \"columns\": [{\"type\": \"text_cta\", \"value\": {\"text\": \"<p data-block-key=\\\"w66ax\\\">Lundi le 10 juin de 19h45 \\u00e0 21h15</p>\", \"cta_label\": \"Choisir lundi\", \"cta_url\": \"https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins\"}, \"id\": \"1a34efa4-2f1d-46ac-8502-87af2f2fab69\"}, {\"type\": \"text_cta\", \"value\": {\"text\": \"<p data-block-key=\\\"w66ax\\\">Jeudi le 13 juin de 15h15 \\u00e0 16h45</p>\", \"cta_label\": \"Choisir jeudi\", \"cta_url\": \"https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins-1\"}, \"id\": \"1dada3fc-265e-4f23-92d8-fb3c2a0eacdb\"}, {\"type\": \"text_cta\", \"value\": {\"text\": \"<p data-block-key=\\\"w66ax\\\">Samedi le 15 juin de 10h15 \\u00e0 11h45</p>\", \"cta_label\": \"Choisir samedi\", \"cta_url\": \"https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins-2\"}, \"id\": \"880db029-c159-435d-89d9-63b9fa252354\"}]},