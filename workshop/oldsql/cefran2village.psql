INSERT INTO public.sites_faciles_contentpage VALUES (4, '[{"id": "3c0406e1-9c83-40fe-b9d1-5f24ad2ca71a", "type": "alert", "value": {"level": "info", "title": "Mentions légales", "description": "Entrez ici les mentions légales du site.<br />\n                    <a href=\"https://www.francenum.gouv.fr/guides-et-conseils/developpement-commercial/site-web/quelles-sont-les-mentions-legales-pour-un-site\">\n                    Que doivent-elles obligatoirement contenir ?</a>", "heading_tag": "h2"}}, {"id": "2f1ba0f0-e45b-4023-8091-394a5657cd34", "type": "paragraph", "value": "\n                <p>D’après la <a href=\"https://www.systeme-de-design.gouv.fr/elements-d-interface/composants/pied-de-page\">documentation du système de design</a>,\n                le pied de page doit contenir a minima les quatre liens suivants :</p>\n                <ul>\n                    <li>Accessibilité : non/partiellement/totalement conforme</li>\n                    <li>Mentions légales</li>\n                    <li>Données personnelles</li>\n                    <li>Gestion des cookies</li>\n                </ul>\n                <p>Ces deux derniers peuvent pointer vers des pages à part entière ou des sections de cette page.</p>\n                "}]', NULL, NULL, NULL, false, NULL, false, false, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (5, '[{"id": "b007227b-f66a-4855-b781-d72d8a9e493f", "type": "alert", "value": {"level": "info", "title": "Déclaration d’accessibilité", "description": "Entrez ici la déclaration d’accessibilité.<br />\n                    <a href=\"https://betagouv.github.io/a11y-generateur-declaration/#create\">\n                    Générateur de déclaration d’accessibilité</a>", "heading_tag": "h2"}}]', NULL, NULL, NULL, false, NULL, false, false, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (30, '[{"id": "a7fb2ebf-8e0d-401c-b11e-1929d826d8fa", "type": "paragraph", "value": "<p data-block-key=\"7uuv2\">Ce manifeste, imparfait, reflète les espérances, les souffrances et les incompréhensions de ceux qui l''ont insufflé. Les Grands Voisins sont nés à l''hôpital Saint-Vincent-de-Paul dans le 14e arrondissement à Paris, entre Port royal et les Catacombes. Nous sommes issus des failles dans le béton et dans les politiques publiques. Nous sommes au service de l''accueil de tous et croyons aux ponts entre utopie et réalité.</p><p data-block-key=\"esgvl\">Comme toutes bonnes voisines et tous bons voisins, nous ne sommes pas toujours d''accord entre nous, mais nous défendons :</p><ul><li data-block-key=\"2i4sq\">la parité dans la gouvernance</li><li data-block-key=\"d2f7m\">des solutions collectives en bonne intelligence</li><li data-block-key=\"d9u2e\">l''expression libre</li><li data-block-key=\"ard5v\">l''exercice artistique</li><li data-block-key=\"1sv90\">l''apprentissage tout au long de la vie</li><li data-block-key=\"64jcl\">la valorisation de toutes formes de travail.</li></ul><p data-block-key=\"6q0us\">Aux Grands Voisins, nous vivons, travaillons, dormons, mangeons, nous soignons, achetons, pleurons, rions, et tout cela dans toutes les langues. L''inclusion et l''exclusion, toutes les deux, peuvent nous éloigner de l''Autre qui est une chance.</p><p data-block-key=\"3khnu\">Nous réclamons des lieux de mixité sociale qui permettent la valorisation et le partage des savoirs et des savoir-faire de toutes et de tous. Nous réclamons des lieux de mixité sociale qui favorisent l''écoute et les possibilités de rencontre, conditions visant à l''abolition de la peur de l''autre. Nous réclamons des lieux de mixité sociale qui luttent contre « l''entre-soi », l''isolement et la réduction des individus à des cases.</p><p data-block-key=\"47p68\">Nous avons besoin de lieux et de temps pour faire précédent, pouvoir inventer d''autres formes de société en harmonie avec les écosystèmes. Nous avons toutes et tous à donner et nous pouvons toutes et tous recevoir.</p>"}]', 'blue-ecume', NULL, NULL, true, 48, true, true, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (32, '[{"id": "ae777dbd-9dc5-4522-8bc5-b72770ba005d", "type": "paragraph", "value": "<p data-block-key=\"ojebn\">Voici des activités artistiques des Grands Voisins :</p><ol><li data-block-key=\"fop5i\">Popup-Expos : L''art partout et pour tout le monde</li><li data-block-key=\"2gfrn\">Mise en valeur des artistes</li><li data-block-key=\"7ito5\">Promotion d''expos</li></ol><p data-block-key=\"3c5gm\">Pour Les Grands Voisins par <a href=\"https://www.lebureauducreateur.fr/?ref=blog.lesgrandsvoisins.com\">Le Bureau du Créateur</a> et <a href=\"https://www.maelanc.com/?ref=blog.lesgrandsvoisins.com\">Mael ANC</a></p><h2 data-block-key=\"7nr4q\">Articles de blog sur les activités artistiques des Grands Voisins</h2><p data-block-key=\"3hjma\"><a href=\"https://blog.lesgrandsvoisins.com/tag/lesartsvoisins/\">Voici les articles de blog sur nos activités artistiques.</a></p><h2 data-block-key=\"9nr51\">La Galerie Les Arts Voisins</h2><p data-block-key=\"4r5t5\">Exposition, création et vente d''oeuvres qui rassemble un collectif pluridisciplinaire des Grands Voisins dont le point commun est de participer « au mieux vivre ensemble », via une galerie d’art alternative et solidaire.</p><h2 data-block-key=\"a0qu4\">Les Popup Expos</h2><p data-block-key=\"f8g12\">Inviter l''art hors les murs, c''est-à-dire dans des lieux où on ne l''attend pas :</p><ul><li data-block-key=\"c8cbb\">donne une autre vision du quotidien</li><li data-block-key=\"57p18\">anime les murs</li><li data-block-key=\"3sr8l\">propose une évasion</li><li data-block-key=\"38s4q\">valorise nos patrimoines, nos cultures et nos artistes</li></ul><p data-block-key=\"eh55d\">Animer les murs en organisant une exposition peut avoir de nombreux avantages, à la fois pour les publics, les commerçants, et les artistes.</p><p data-block-key=\"2f9dl\">Notre démarche de popup expos favorise la vie de quartier par l''invitation de l’art dans le quotidien des publics en cassant les idées reçues. L''art pour la population crée une synergie entre le commerce et la culture, met en lumière les artistes locaux et les aide à gagner en visibilité tout en dynamisant l’économie locale avec l''accès à la culture, l''apaisement, la détente, l''étonnement ...</p><p data-block-key=\"etjha\">Les lieux du commerçant se voient affecté aussi un caractère hybride, car ils deviennent parfois, temporairement, aussi des galléries artistiques.</p><p data-block-key=\"dtd6m\"><a href=\"https://www.lesgrandsvoisins.fr/arts/lespopupexpos/\"><b>Cliquer ici pour une présentation des Popup Expos sur une page.</b></a></p>"}]', 'green-emeraude', NULL, NULL, true, 50, true, true, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (31, '[{"id": "9c96dd65-5feb-4d6b-874d-cdc6bd72a5df", "type": "paragraph", "value": "<p data-block-key=\"71524\">Poussés par la Mission des Grands Voisins et le <a href=\"https://contractfortheweb.org/fr/?ref=blog.lesgrandsvoisins.com\">Contrat pour le Web</a>, nous avançons avec assurance dans <b>le numérique interdépendant et autonome</b> des Grands Voisins.</p><ul><li data-block-key=\"1mlno\"><a href=\"https://blog.lesgrandsvoisins.com/salle-de-sociabilite-numerique/\">Le numérique créatif des Grands Voisins à Paris le lundi de 16h à 21h30</a></li><li data-block-key=\"9pddv\"><a href=\"https://homepage-dashboard.resdigita.com/\">homepage-dashboard.ResDigita.Com</a></li></ul><h2 data-block-key=\"be8ku\">Articles de blog sur le numérique</h2><p data-block-key=\"f0sfl\"><a href=\"https://blog.lesgrandsvoisins.com/tag/resdigita/\">Voici des articles de blog sur le numérique</a></p><h2 data-block-key=\"fbrjt\">Interdépendance numérique</h2><p data-block-key=\"d47rp\">Nous aimerions aussi dans notre IT mettre en place un principe d’équilibre entre l’autonomie et l’intégration. Nous sommes assez fiers de faire notre propre serveur de courriels. Nous aimons les logiciels libres. La manière dont nous faisons est déjà ce que nous faisons.</p><p data-block-key=\"a80nv\">Avec votre compte vous avez une identité centralisée et contrôlée à partir de laquelle vous pouvez accéder à tout ce qui est <a href=\"https://lesgrandsvoisins.com/?ref=blog.lesgrandsvoisins.com\">lesgrandsvoisins.com:</a> dispositifs de communication, courriel dont listes de diffusion, le tout dans les serveurs autonomes pour la plupart. Nous facteurs envoient les courriels directement aux destinataires sans passer par aucun intermédiaire autre. <a href=\"https://www.idgouv.com/?ref=blog.lesgrandsvoisins.com\">Votre compte et mot de passe</a> sont stockés dans un annuaire LDAP avec un accès à authentification unique. L''association se met en posture d''être directement redevable envers chaque usager, et c''est tellement important dans ce monde numérique !</p><h2 data-block-key=\"63eiq\">Le Numérique Créatif des Grands Voisins</h2><p data-block-key=\"daglo\">Suite à une rencontre de voeux organisée par <a href=\"https://mairie14.paris.fr/pages/maison-des-associations-11177\">la mairie et la MVAC du 14e arrondissement</a>, nous avons lancé avec <a href=\"https://www.laressourceriecreative.com/\">la Ressourcerie Créative</a> <a href=\"https://budgetparticipatif.paris.fr/bp/jsp/site/Portal.jsp?page=idee&amp;campagne=H&amp;idee=1445\">un projet de budget participatif de Paris « Le numérique créatif » </a>. Nous sollicitons le soutien de la <a href=\"https://mairie14.paris.fr/\">Mairie du 14e</a> et d’autres associations et partenaires potentiels.</p><p data-block-key=\"dam56\">Le projet de numérique créatif est une proposition de jardinage numérique en deux volets: un volet serveur et un volet équipement de salle pouvant être ponctuellement transformé dans un lieu social informatique. L’objectif est d’encourager l’épanouissement digital local sur la base des missions et <a href=\"https://lesgrandsvoisins.com/\">lesgrandsvoisins.com</a> et <a href=\"https://contractfortheweb.org/fr\">contractfortheweb.org</a>.</p><p data-block-key=\"fgeot\">Le côté serveur est une armoire fermée à clef avec des ressources qui soient disponibles via l’internet aux usagers à partir de logiciels libres.</p><p data-block-key=\"60ons\">La construction de la salle consiste en un nombre d’ordinateurs portables et d’équipements informatiques dans une salle qui peuvent être sortis et utilisé ponctuellement pour des besoins variables depuis le franchissement de la fracture numérique jusqu’au développement vidéo, design, programmation, 3D, administration, etc.</p><p data-block-key=\"2ikvn\">Le projet avait été déposé avec un partenariat avec La Ressourcerie Créative, soit point d’accès numérique au 14ième. Aujourd’hui, je souhaite aussi solliciter l’intérêt d’autres associations telles que &amp;aprilorg, &amp;lespetitsdebrouillardsorg et &amp;antanakcom.</p><p data-block-key=\"5rs20\"><a href=\"https://wiki.lesgrandsvoisins.com/fr/acts/budgetpartparis\">Découvrir</a></p><ul><li data-block-key=\"ell5j\">Salle de sociabilité numérique<ul><li data-block-key=\"d5n00\">Atelier multi-niveaux, multi-compétences</li><li data-block-key=\"mrk8\">Le lundi de 16h à 21h30 à l''Atelier de la Ressourcerie Créative<br/>au 93 ave du Général Leclerc 75014 PARIS</li></ul></li><li data-block-key=\"5r8p7\">Serveurs de sociabilité numérique<ul><li data-block-key=\"at3k3\">Serveurs hébergés et redondés pour les activités en lien avec la Salle</li></ul></li><li data-block-key=\"9muko\">Aide matériel<ul><li data-block-key=\"ea75f\">Ce projet amène du matériel à partager entre associations<br/>(montage audio-visuel, etc.)</li><li data-block-key=\"3kjd8\">Trouver une solution pour les maux de passe, etc.</li><li data-block-key=\"b615g\">Accompagner même des projets pointus</li></ul></li><li data-block-key=\"ahj44\">Mécénat<ul><li data-block-key=\"251rs\">Mécénat de compétences<br/>Nous avons besoin de gens pour organiser les activités</li><li data-block-key=\"807k8\">Dons<br/>Nous mettons à bon usage vos contributions financier en levier du Lauréat</li><li data-block-key=\"eqcs6\">Mécénat en nature<br/>Dons d''informatique, audio-visuel, etc. Services, ... Tout sera valorisé.</li></ul></li><li data-block-key=\"9prgn\">Accompagnement numérique<ul><li data-block-key=\"5ln8a\">Aider les commerçants et indépendants, des marginalisés et des pros</li><li data-block-key=\"7pfjt\">Adhésions au Contrat pour le web dans tout ce dont nous faisons</li></ul></li></ul><h2 data-block-key=\"52vg8\">ResDigita</h2><p data-block-key=\"6mjam\">Nous avions préféré dénommer notre activité numérique « ResDigita » lors d''un Conseil des Voisins en février 2023. Voici nos différentes activités en lien avec le numérique.</p><ul><li data-block-key=\"7rqbs\">Serveur Email Public<br/>Nous avons un serveur public de courriel (dite SMTP) qui livre et reçoit à notre place nos courriels, que ce soit de nos boîtes individuelles (votre domaine si vous voulez) ou de nos listes de diffusion (attention aux usages svp).</li><li data-block-key=\"b6reg\">Serveur de publication et de gestion de contenus webs</li><li data-block-key=\"56nm9\">Serveur de Webmail, de calendrier et de listes de diffusion</li><li data-block-key=\"1hf3d\">Serveurs ERP</li><li data-block-key=\"djpc5\">Serveurs de gestion de mots de passe</li></ul>"}]', 'pink-macaron', 'ResDigita.com', 'https://www.resdigita.com', true, 49, true, true, 'Venez vous inscrire à Res Digita, le bras informatique des Grands Voisins');
INSERT INTO public.sites_faciles_contentpage VALUES (43, '[{"id": "02e50820-4427-4b83-aa0f-338a1db71c04", "type": "paragraph", "value": "<h2 data-block-key=\"intc1\">Salle de réunion</h2><p data-block-key=\"8anir\"><a href=\"https://bbb.paquerette.eu/b/chr-tb1-cgf-3tr\"><b>https://bbb.paquerette.eu/b/chr-tb1-cgf-3tr</b></a></p><p data-block-key=\"1do1\"><b>Merci d''utiliser le nouveau lien ci-dessus pour vous connecter à la réunion.</b> Penser à activer votre caméra et microphone svp pour le confort, l''accueil et la sécurité de toutes et de tous.</p><h2 data-block-key=\"2d8q7\">Une note sur la sécurité, le confort et l''accueil de tous</h2><p data-block-key=\"akr1v\">Ni l''audio ni la vidéo de cette réunion n''est enregistrée par personne : ni par l''association coopérative Les Grands Voisins (.com), ni par aucun participant. Le texte peut est enregistré et accessible par tout participant. Merci de ne pas y inscrire des informations personnelles dans la partie publique et de ne pas partager ses contenus.</p><p data-block-key=\"d8n28\">Votre activation de votre son et de votre vidéo renforce le sentiment de sécurité pour tous. Selon des circonstances, les animateurs peuvent conseiller l''activation de la sourdine, vous mettre en sourdine, suggérer un minuteur, ou tout autre dispositif. Merci d''être patients avec les évolutions structurelles éventuelles du groupe et indulgents envers les bénévoles.</p>"}]', NULL, NULL, NULL, false, NULL, false, false, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (36, '[{"id": "e569e14e-92d4-476c-96b3-2b221f2ce890", "type": "paragraph", "value": "<p data-block-key=\"kokwt\">Aux Grands Voisins nous avons <a linktype=\"page\" id=\"30\">un manifeste</a> éditée par Chris Mann avec des contributions de plus de cent personnes sur deux grands ateliers et plusieurs séances de travail sur une période d''un mois en 2017. Chacun a un avis qui est le sien sur ce que c''est être Grands Voisins et chacun a raison.</p><p data-block-key=\"b4dti\">L''objectif des activités dans notre cellule civisme est de promouvoir ce qu''il faut pour avoir les possibilités de rencontres ou non. Cela découle même de l''allégorie du voisin ; l''on peut rester chez soi, l''on peut sortir, l''on peut inviter d''autres.</p><p data-block-key=\"aovvo\">Concrètement, nous pouvons commencer par un annuaire QuiQuoiEtc qui permet de référencer les voisins qui veulent bien. Cela permet une communititude et des possibilités de compréhensions, empathies et entraides intéressantes.</p><p data-block-key=\"4j94g\"></p>"}]', NULL, NULL, NULL, false, NULL, false, false, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (44, '[{"id": "2324c983-6ed1-45c1-9970-2d712a67677c", "type": "paragraph", "value": "<h2 data-block-key=\"56he6\">Les Popup Expos</h2><p data-block-key=\"9dlj6\">Inviter l''art hors les murs, c''est-à-dire dans des lieux où on ne l''attend pas :</p><ul><li data-block-key=\"60bjf\">donne une autre vision du quotidien</li><li data-block-key=\"1q19v\">anime les murs</li><li data-block-key=\"dtj5n\">propose une évasion</li><li data-block-key=\"2mt9r\">valorise nos patrimoines, nos cultures et nos artistes</li></ul><p data-block-key=\"bhq6a\">Animer les murs en organisant une exposition peut avoir de nombreux avantages, à la fois pour les publics, les commerçants, et les artistes.</p><p data-block-key=\"2o0l5\">Notre démarche de popup expos favorise la vie de quartier par l''invitation de l’art dans le quotidien des publics en cassant les idées reçues. L''art pour la population crée une synergie entre le commerce et la culture, met en lumière les artistes locaux et les aide à gagner en visibilité tout en dynamisant l’économie locale avec l''accès à la culture, l''apaisement, la détente, l''étonnement ...</p><p data-block-key=\"fg0nm\">Les lieux du commerçant se voient affecté aussi un caractère hybride, car ils deviennent parfois, temporairement, aussi des galléries artistiques.</p><h3 data-block-key=\"6fofg\">Plus de détails</h3><p data-block-key=\"5hkmj\">Inviter l''art hors les murs, c''est-à-dire dans des lieux où on ne l''attend pas :</p><ul><li data-block-key=\"b63mj\">donne une autre vision du quotidien</li><li data-block-key=\"5ov26\">anime les murs</li><li data-block-key=\"clg4q\">propose une évasion</li><li data-block-key=\"f3a8u\">valorise nos patrimoines, nos cultures et nos artistes</li></ul><p data-block-key=\"34dar\">Animer les murs en organisant une exposition peut avoir de nombreux avantages, à la fois pour les publics, les commerçants, et les artistes.</p><p data-block-key=\"2vg4d\">Notre démarche de popup expos favorise la vie de quartier par l''invitation de l’art dans le quotidien des publics en cassant les idées reçues. L''art pour la population crée une synergie entre le commerce et la culture, met en lumière les artistes locaux et les aide à gagner en visibilité tout en dynamisant l’économie locale avec l''accès à la culture, l''apaisement, la détente, l''étonnement ...</p><p data-block-key=\"c6jbp\">Les lieux du commerçant se voient affecté aussi un caractère hybride, car ils deviennent parfois, temporairement, aussi des galléries artistiques.</p><p data-block-key=\"4sdud\">Les publics :</p><ul><li data-block-key=\"fmsjg\">s''engagent avec les œuvres des popup expos, chose qui peut remuer les méninges ou apaiser selon les cas</li><li data-block-key=\"1sqf9\">interagissent avec le métier du commerçant aussi de manière augmentée par les expos,</li><li data-block-key=\"80iom\">s''éduquent sur un sujet connexe au métier du commerçant si l''expo en est en lien,</li><li data-block-key=\"dttaa\">se retrouvent dans un milieu d''avantage créatif</li><li data-block-key=\"fqtcl\">ont des outils pour renforcer le bien-être, en complément de la prestation du commerçant</li><li data-block-key=\"6uc3e\">peuvent acquérir aux œuvres en personne ou en ligne.</li><li data-block-key=\"9coe0\">peuvent interagir avec les artistes lors des animations prévues,</li><li data-block-key=\"5m1jv\">peuvent rencontrer des nouveaux publics chez le commerçant.</li></ul><p data-block-key=\"2uf5j\">Les commerçants :</p><ul><li data-block-key=\"6ons2\">disposent d''un contrôle sur ce qui est affiché dans leurs espaces</li><li data-block-key=\"ddrd9\">diversifient leurs clientèles</li><li data-block-key=\"117ej\">permettent l''organisation de vernissages, de décrochages, et/ou éventuellement des ateliers ponctuels</li><li data-block-key=\"19htf\">s''enracinent plus profondément dans le tissu local</li><li data-block-key=\"5fg7e\">rencontrent des artistes géniaux</li><li data-block-key=\"2oe1u\">valorisent les artistes avec une contribution financière et éventuellement aussi en nature</li><li data-block-key=\"8qms7\">prévoient avec les artistes les responsabilités en cas de (on espère que cela n''arrive pas) vol ou dégradation des œuvres</li><li data-block-key=\"2he85\">enrichissent leurs liens avec toute personne qui vient sur place</li><li data-block-key=\"2g1vn\">pourraient éventuellement orienter les invitations d''expos pour sensibiliser le public sur les questions spécifiques à leurs métiers</li><li data-block-key=\"2ig8g\">bénéficient d''un cadre agréable des popup expos, y compris des nouvelles rencontres</li></ul><p data-block-key=\"5stho\">Les artistes :</p><ul><li data-block-key=\"db6ph\">augmentent la visibilité</li><li data-block-key=\"22cup\">se soutiennent dans leurs ventes d''œuvres</li><li data-block-key=\"7unud\">prévoient les éventualités en cas de vol ou dégradation (et pour lesquels nous pennons des précautions)</li><li data-block-key=\"55gai\">sont valorisés selon les moyens du commerçant</li><li data-block-key=\"5klt0\">communiquent autour de leurs œuvres dans des galléries hybrides</li><li data-block-key=\"16ct8\">rencontrent des nouvelles personnes</li><li data-block-key=\"es27l\">participent au tissu économique autrement</li><li data-block-key=\"6glmd\">peuvent vendre leurs œuvres en ligne ou en personne</li><li data-block-key=\"37q01\">enrichissent le patrimoine activement avec vos œuvres</li></ul><p data-block-key=\"a890h\">Selon le principe de subsidiarité, Les Grands Voisins (Les Arts Voisins) peuvent :</p><ul><li data-block-key=\"b9fuc\">rechercher d''artistes</li><li data-block-key=\"1i6gj\">rechercher des commerçants / lieux</li><li data-block-key=\"96qha\">gérer des candidatures</li><li data-block-key=\"8p5gk\">proposer d''un projet articulé autour des artistes et leur créativité et le lieu d''exposition</li><li data-block-key=\"bf44j\">s''occuper de la logistique</li><li data-block-key=\"9u5u\">organiser les accrochages (vernissages) et décrochages</li><li data-block-key=\"6aka3\">organiser de rencontres avec les artistes</li><li data-block-key=\"ctpjh\">intermédier pour faciliter la vente des œuvres</li><li data-block-key=\"d9dd4\">écrire des articles sur les popup expos, les ateliers, les vernissages et des décrochages</li><li data-block-key=\"55hvs\">éventuellement souscrire une assurance explicite (aucune garantie implicite n''est assumée)</li></ul>"}]', NULL, NULL, NULL, false, NULL, false, false, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (45, '[{"id": "87907458-90d4-488d-9104-e687407ca69e", "type": "paragraph", "value": "<p data-block-key=\"805qw\">En général, les Popup Expos des GV servent à inviter l''art hors les murs, c''est à dire dans des lieux où on ne l''attend pas pour :</p><ul><li data-block-key=\"divs7\">Favoriser une vie de quartier selon les principes des Grands Voisins</li><li data-block-key=\"dhu08\">Inviter l’art dans le quotidien du grand public en cassant les idées reçues que l’art est réservé à une partie de la population</li><li data-block-key=\"8qh4r\">Créer une synergie entre le commerce et la culture</li><li data-block-key=\"6n85i\">Mettre en lumière les artistes locaux et les aider à gagner en visibilité</li><li data-block-key=\"bg48g\">Dynamiser l’économie locale</li><li data-block-key=\"5qlf5\">Valoriser notre patrimoine et culture français et nos artistes</li></ul><p data-block-key=\"dbc0q\">Et favoriser ainsi l''accès à la culture, l''apaisement, la détente, l''étonnement...</p><p data-block-key=\"6sfu5\"><i>Animer l''IRMA Valenton en organisant une exposition peut avoir de nombreux avantages, à la fois pour l''hôpital et les patients.</i></p><h2 data-block-key=\"1og05\">Les Popup expos pour les patients</h2><p data-block-key=\"4fl3e\">Nous proposons d’inscrire les Popup expos dans le parcours patient, participant à décentrer la douleur physique et psychologique des patients. En leur donnant une autre vision de leur nouveau quotidien, nous souhaitons participer à favoriser leur acceptation de leur nouveau corps grâce à un environnement toujours plus humain et agréable.</p><ul><li data-block-key=\"5cq54\">Distraction positive :<br/>Offrir aux patients une évasion au cours de leur séjour médical, contribuant à réduire le stress et l’anxiété associés à l’hospitalisation</li><li data-block-key=\"fr8cd\">Stimulation mentale :<br/>Stimuler l’esprit des patients en les aidant à rester mentalement actifs et engagés</li><li data-block-key=\"bm8l6\">Education :<br/>Le choix des sujets des expositions peut aider les patients à mieux comprendre leur condition et s’approprier mieux leur nouveau corps</li><li data-block-key=\"1d8bc\">Créativité et expression :<br/>Favoriser l’expression créative et émotionnelle des patients</li><li data-block-key=\"fojgu\">Interaction sociale :<br/>Créer un environnement propice à la rencontre et à l’échange entre les patients, les familles, le personnel et contribuer ainsi à réduire l’isolement</li></ul><h2 data-block-key=\"a9i9g\">Les Popup expos pour l’IRMA</h2><p data-block-key=\"4vhk4\">Nous proposons d’inscrire les Popup expos dans le projet de l''hôpital</p><ul><li data-block-key=\"3ec59\">Promotion de la santé et sensibilisation :<br/>Les expos peuvent être conçues pour sensibiliser le public sur les questions de santé spécifiques, telles que la prévention des maladies, promotion d’habitudes et mode de vie sains ou encore la sensibilisation à leurs conditions médicales</li><li data-block-key=\"6drii\">Renforcement de l’image de l''hôpital :<br/>Organiser des événements culturels et artistiques peut participer à l’amélioration de l’image de l''hôpital, soucieux non seulement du traitement médical mais aussi du bien-être de la communauté</li><li data-block-key=\"3j21e\">Engagement :<br/>Opportunité de s''impliquer et de s’inscrire davantage dans l’environnement local</li><li data-block-key=\"1ug1s\">Création de lien :<br/>Les expositions peuvent attirer un large éventail de participants, y compris des experts médicaux, des artistes locaux et d''autres professionnels. Cela peut favoriser la création de partenariats bénéfiques</li><li data-block-key=\"2hdso\">Diversification des activités de l''hôpital :<br/>En offrant des activités culturelles et artistiques en plus des soins médicaux et paramédicaux, l''hôpital peut diversifier ses services et devenir un lieu plus inclusif.</li></ul>"}, {"id": "96822e24-d8ec-48fa-922c-d2a42f8e7a3d", "type": "paragraph", "value": "<h2 data-block-key=\"805qw\">Nos actions concrètes</h2><p data-block-key=\"coceh\">Nos actions</p><ul><li data-block-key=\"1b2em\">Recherches d''artistes</li><li data-block-key=\"6b2q9\">Gestion des candidatures</li><li data-block-key=\"d5cgl\">Proposition d''un projet articulé autour des artistes et leur créativité et le lieu d''exposition</li><li data-block-key=\"c5vdg\">Logistique</li><li data-block-key=\"2tk9g\">Accrochage/Décrochage</li><li data-block-key=\"9o9uo\">Organisation de rencontres avec les artistes</li></ul><h2 data-block-key=\"29r09\">Métamorphose Artistique : Émotions et transformations</h2><p data-block-key=\"a5abi\">Cette exposition unique offre une plongée artistique au cœur de l''expérience humaine, explorant la résilience, l''espoir et la reconstruction. Chaque œuvre exposée est une célébration de la force intérieure et de la beauté qui émerge de la transformation après des moments difficiles vécus par les artistes et qui racontent des histoires de courage, d''adaptation et de renouveau.</p><p data-block-key=\"4g6ql\">Le choix du sujet repose sur la conviction que l''art peut être un catalyseur puissant pour l''expression émotionnelle, la compréhension mutuelle et la transformation.</p><p data-block-key=\"aacii\">L''exposition vise à humaniser l''expérience de l''amputation en mettant en avant les récits individuels à travers des œuvres d''art visuelles, sculpturales, et d''autres formes artistiques. En montrant la force, la vulnérabilité et la beauté dans la diversité des expériences humaines, nous espérons créer un espace où patients, familles et personnel médical peuvent se connecter et trouver réconfort.</p><h2 data-block-key=\"2nubp\">Détails</h2><h3 data-block-key=\"b4d2k\">Œuvres Multidisciplinaires</h3><p data-block-key=\"9doak\">Les visiteurs auront l''occasion d''explorer une variété d''expressions artistiques, y compris la peinture, la sculpture et la photographie</p><h3 data-block-key=\"f31lv\">Métamorphose Artistique</h3><p data-block-key=\"bu8ap\">Émotions et Reconstructions\" célèbre la créativité en tant que catalyseur de guérison et d''unité. Nous invitons la communauté à se joindre à cette exploration visuelle de la résilience humaine, à travers les yeux d''artistes exceptionnels qui transforment l''expérience de l''amputation en une œuvre d''art vivante.</p><h2 data-block-key=\"a47gv\">Le champ des possibles</h2><h3 data-block-key=\"csar3\">Ateliers de création artistique</h3><p data-block-key=\"50b66\">Organiser des ateliers de peinture, de sculpture, de dessin ou d''autres formes d''expression artistique pour les patients. Cela peut être particulièrement bénéfique pour ceux qui sont en phase de rétablissement ou qui font face à des problèmes de santé mentale.</p><h3 data-block-key=\"l317\">Mur d''art communautaire</h3><p data-block-key=\"b5am9\">Créer un espace où les patients, les visiteurs et le personnel peuvent contribuer à une œuvre d''art collective. Cela favorise un sentiment d''appartenance et de communauté.</p><h3 data-block-key=\"83l2i\">Expositions artistiques</h3><p data-block-key=\"23qh2\">Organiser des expositions d''œuvres créées par les patients. Cela peut aider à sensibiliser à certaines conditions médicales et à déstigmatiser les maladies mentales.</p><h3 data-block-key=\"c01hk\">Ateliers de gestion du stress</h3><p data-block-key=\"9qfas\">Offrir des séances de création artistique pour le personnel médical afin de les aider à gérer le stress lié à leur travail.</p><h3 data-block-key=\"9iu73\">Décoration artistique</h3><p data-block-key=\"dk473\">Embellir les espaces hospitaliers avec des œuvres d''art, des peintures murales, des sculptures, contribuant ainsi à créer un environnement plus accueillant et apaisant.</p><h3 data-block-key=\"9b1tb\">Jardins thérapeutiques</h3><p data-block-key=\"9fm3h\">Aménager des espaces extérieurs avec des éléments artistiques, favorisant la relaxation et la guérison.</p><h3 data-block-key=\"570a4\">Collaborations artistiques</h3><p data-block-key=\"8m8pk\">Établir des partenariats avec des artistes locaux pour des résidences artistiques ou des interventions spécifiques à l''hôpital.</p><h3 data-block-key=\"18q5l\">Intégration du spectacle vivant</h3><p data-block-key=\"9f453\">Offrir un moment d’évasion et de divertissement</p>"}]', NULL, NULL, NULL, false, NULL, false, false, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (3, '[{"id": "c58f3abe-971d-41a7-ad49-f547da49dd04", "type": "multicolumns", "value": {"title": "", "columns": [{"id": "fa6cb962-ce70-49d7-8be1-782246d16d32", "type": "image", "value": {"alt": "Inscription sur les Grands Voisins", "url": "https://www.village.ngo/fr/civisme/invitation-au-22%C3%A8me-conseil-des-grands-voisins/", "image": 63, "title": "22e Conseil des Voisins du 10 au 16 juin 2024", "caption": "Je m''y incris !", "heading_tag": "h3"}}, {"id": "9595d5cd-cdc2-4c52-a93c-5614abc35e97", "type": "text", "value": "<p data-block-key=\"4mc6m\">Nous tiendrions le 22e Conseil des Voisins sur trois rencontres :</p><ul><li data-block-key=\"sqrq\"><a href=\"https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins\">Lundi le 10 juin de 19h45 à 21h25</a></li><li data-block-key=\"5sba\"><a href=\"https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins-1\">Jeudi le 13 juin de 15h15 à 16h45</a></li><li data-block-key=\"fabkv\"><a href=\"https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins-2\">Samedi le le 15 juin de 10h15 à 11h45</a></li></ul><p data-block-key=\"3m03\"></p><p data-block-key=\"a6q9u\">Ordre du jour :</p><ol><li data-block-key=\"43o40\">Numérique</li><li data-block-key=\"oioh\">Éventuellement Civisme</li><li data-block-key=\"7l6q8\">Communication</li></ol>"}, {"id": "460c495c-476c-43f1-bb69-aff7bf1509ea", "type": "text", "value": "<h3 data-block-key=\"avd09\"><b>Permanence le lundi de midi à 17h</b></h3><p data-block-key=\"e0hvf\"></p><embed embedtype=\"image\" format=\"left\" id=\"52\" alt=\"Abaker-Atelier\"/><p data-block-key=\"5c5gg\"></p><p data-block-key=\"dqj9a\">Venez à notre permanence <b>le lundi de 12h à 17h</b><br/>en personne à <b>93 ave. Du Gén. Leclerc</b> 75014 Paris (Métro Alésia, Atelier de la Ressourcerie, <i>7e boutique sur la gauche</i>) ou <a href=\"https://meet.lesgrandsvoisins.com\">meet.lesgrandsvoisins.com pour participer en hybride</a>.</p>"}, {"id": "6fc069f3-372a-447a-8d33-fd26389bdac8", "type": "text", "value": "<h3 data-block-key=\"avd09\"><a href=\"https://www.lesgrandsvoisins.fr/numerique/numerique-creatif/sociabilite-numerique/\"><b>Sociabilité numérique le lundi de 17h à 21h30</b></a></h3><embed embedtype=\"image\" format=\"right\" id=\"22\" alt=\"numasso\"/><p data-block-key=\"cda90\"><b>Le lundi de 17h à 21h30</b> se tient un atelier d’entraide numérique à l’Atelier au <b>93 ave. du Gén. Leclerc 75014 PARIS</b> à l’intérieur du <i>centre commercial 7e boutique sur la gauche</i></p>"}], "bg_color": "", "bg_image": null, "heading_tag": "h2", "bg_color_class": ""}}, {"id": "d3e4fb2e-7b28-4da0-99d3-09c2868a013f", "type": "paragraph", "value": "<p data-block-key=\"q7an3\"><a href=\"https://list.lesgrandsvoisins.com/subscription/form\"><b><i>Pensez à vous inscrire sur notre liste d''infos !</i></b></a></p><p data-block-key=\"evpog\">Nous sommes une association d''intérêt général agissant de manière contrôlée en subsidiarité ouverte pour toute organisation et tout individu qui en fait la décision pour soi-même principalement en ces trois domaines <a href=\"https://www.lesgrandsvoisins.fr/communautes/\">de Discussions</a>, <a href=\"https://www.lesgrandsvoisins.fr/arts/\">d''Arts plastiques</a> et <a href=\"https://www.lesgrandsvoisins.fr/numerique/\">de Numérique</a> selon <a href=\"https://www.lesgrandsvoisins.fr/mission/\">notre déclaration de mission</a>.</p><p data-block-key=\"bj8u6\"><a href=\"https://www.lesgrandsvoisins.fr/communautes/\"><b>Discussions</b></a> :</p><ul><li data-block-key=\"61obe\">Nous proposons des conditions encourageant la compréhension entre individus et organisations de perspectives différentes.</li><li data-block-key=\"9oi9e\">Nous ouvrons notre gouvernance pour des votes en Conseils des Voisins.</li></ul><p data-block-key=\"65va7\"><a href=\"https://www.lesgrandsvoisins.fr/arts/\"><b>Arts plastiques</b></a> :</p><ul><li data-block-key=\"a2qag\">Nous encourageons l’utilisation hybrides d’espace (commerces, hôpitaux, cafés, etc.) aussi en galléries locales.</li><li data-block-key=\"2ek3r\">Nous accompagnons des institutions pour trouver une juste place pour les arts plastiques dans leurs locaux.</li></ul><p data-block-key=\"91unk\"><a href=\"https://www.lesgrandsvoisins.fr/numerique/\"><b>Numérique</b></a> :</p><ul><li data-block-key=\"afesu\">Nous maintenons une constellation de serveurs et de services internet -- tableau de bord, création et gestion de compte, courriel, rencontres hybrides présence-distance, listes de diffusion numériques, blogs, serveurs webs, gestionnaire de mot de passe, enregistrement dans le nuage, forums de discussion, gestion de la connaissance, édition markdown, wikis, édition bueautique, etc. -- pour, par, et de nous-mêmes et d''autres.</li><li data-block-key=\"ej6ca\">Nous mettons à disposition des talents et des matériaux dans des salles de sociabilité numérique et dans des systèmes de prêts et échanges de matériels à l''attention d''individus et d''organisations de tous niveaux.</li></ul><p data-block-key=\"apjqo\">Voici quelques entités Grandes Voisines qui soutiennent notre démarche: A Good Solution, Banya Tour, Robert Baldridge, BMCP, Breast Freedom, Le Bureau du Créateur, Le Chiffon, Les Jardins du Numérique, Olivier Duhec, Inno Cube, Caroline Lhomme, Francesco du 59 Rivoli, Amadou Gaye, Informatruck, Inolib, KaceKode, Ada Matus, L’Odysée, La Ressourcerie Créative, Henry Roy, Peter Vaeth, Waka Conseil, Weezem, et peut-être des vôtres …</p><p data-block-key=\"at7dp\">Pour plus d’informations, contactez-nous : chris@lesgrandsvoisins.com +33 7 81 811 811</p><p data-block-key=\"4coel\"></p>"}, {"id": "f53195d5-0de5-479d-aef1-6151fbd266e0", "type": "video", "value": {"url": "https://www.youtube.com/embed/qZqErGfNF_o?feature=oembed", "title": "Les Grands Voisins, la cité rêvée", "caption": "c 3, 2020 Réalisation : Bastien Simon  Pitch : Maël, artiste sans papier, Adrien, luthier musicien et d''autres résidents venus de tous horizons, s''organisent pour donner naissance à une utopie moderne en plein coeur de Paris, un village solidaire de près de 2000 personnes : Les Grands Voisins. À travers leurs trajectoires et celles des membres fondateurs du lieu, le film interroge notre désir et notre capacité à inventer d’autres manières de vivre ensemble. Que retiendrons-nous de cette expérience collective ? Pourra-t-elle perdurer, essaimer, résonner ailleurs ?"}}, {"id": "03449e53-e17e-4ae3-ae63-7d9140be0b08", "type": "accordions", "value": [{"id": "6c6ec6eb-93af-4f4a-97e4-2f91d2498c57", "type": "title", "value": "Les Grands Voisins"}, {"id": "d2cecf58-044d-441c-849d-2d98d2085f7f", "type": "accordion", "value": {"title": "Sites Webs des Grands Voisins", "content": "<p data-block-key=\"h0w28\"><a href=\"https://www.lesgrandsvoisins.fr\">LesGrandsVoisins.fr</a> : Le site public principal des Grands Voisins</p><p data-block-key=\"6dm50\"><a href=\"https://www.resdigita.com\">ResDigita.com</a> : Le numérique des Grands Voisins se nomme ResDigita.</p><p data-block-key=\"3ntu0\"><a href=\"https://blog.lesgrandsvoisins.com\">Blog.LesGrandsVoisins.com</a> : Notre blog (accessible depuis le site principal)</p>"}}, {"id": "e02f2223-5134-4693-ac2b-639a3ec6f68a", "type": "accordion", "value": {"title": "Médias Sociaux des Grands Voisins", "content": "<p data-block-key=\"h0w28\"><a href=\"https://www.instagram.com/les_grands_voisins/\">Instagram@les_grands_voisins</a></p><p data-block-key=\"eseqg\">Facebook</p><p data-block-key=\"2vnko\">LinkedIn</p>"}}]}, {"id": "0e4ded21-5eb7-43a5-8cc1-db70172bb3c7", "type": "accordions", "value": [{"id": "05c4ba85-425d-4aa5-8176-489343275054", "type": "title", "value": "ResDigita, le numérique des Grands Voisins"}, {"id": "8d7c6179-ed2f-4faf-9283-7ef63fe2653d", "type": "accordion", "value": {"title": "Points de départs", "content": "<p data-block-key=\"lofgf\"><a href=\"https://homepage-dashboard.resdigita.com\">Homepage-Dashboard</a> : le tableau de bord avec nos applications principales</p><p data-block-key=\"38lbi\"><a href=\"https://guichet.resdigita.com\">Guichet (Login)</a> : Gérer votre compte (guichet est un logiciel développé par Les Grands Voisins)</p><p data-block-key=\"l50c\"><a href=\"https://guichet.resdigita.com/user/new\">Guichet (Inscription)</a> : Créer votre compte</p><p data-block-key=\"bkgrv\"><a href=\"https://www.lesgrandsvoisins.fr/numerique/\">LesGrandsVoisins.fr/numerique</a> : La description de nos activités numériques</p><p data-block-key=\"5s7rb\"><a href=\"https://quartz.resdigita.com\">Quartz</a> : Consulter notre documentation sur les outils numériques (par exemple, configurer votre courriel)</p><p data-block-key=\"3j715\"><a href=\"https://uptime-kuma.resdigita.com/status/gv\">Uptime-Kuma</a> : Vérifier l''état de nos services</p>"}}, {"id": "8d7d90ad-f09d-4d22-a218-975740578810", "type": "accordion", "value": {"title": "Applications unifiées", "content": "<p data-block-key=\"lofgf\"><a href=\"https://hedgedoc.resdigita.com\">Hedgedoc</a> : Edition en temps réel de documents partagés</p><p data-block-key=\"4u0e9\"><a href=\"https://vikunja.resdigita.com\">Vikunja</a> : Centraliser des informations sur des tous doux lisses</p>"}}, {"id": "b2f6f9ce-8042-4d8a-9536-b400b6987f6f", "type": "accordion", "value": {"title": "Courriel", "content": "<p data-block-key=\"lofgf\"><a href=\"https://mail.lesgrandsvoisins.com\">Roundcube Webmail</a> : Accéder à vos mels par le web</p><p data-block-key=\"1sbfd\">Le courriel est créé dans <a href=\"https://guichet.resdigita.com/user\">Guichet dans la partie « modifier mon profil »</a>. Chaque courriel que vous ajoutez sera un login distinct avec le même mot de passe que vous utilisez pour Guichet.</p><p data-block-key=\"47eua\">La <a href=\"https://quartz.resdigita.com/config/services/email\">documentation dans Quartz</a> peut vous aider à la configuration de votre courriel (sur smartphone par exemple).</p><p data-block-key=\"75pbq\">La <a href=\"https://guichet.resdigita.com/user/new\">création de compte sur Guichet</a> crée automatiquement un courriel correspondant.</p><p data-block-key=\"9gmov\"><a href=\"https://list.lesgrandsvoisins.com\">ListMonk</a> : Envois de Newsletters et gestions d''abonnements</p>"}}, {"id": "c3e087d4-583c-4d58-b836-c5f3c2f9dbc5", "type": "accordion", "value": {"title": "Mot de passe", "content": "<p data-block-key=\"lofgf\"><a href=\"https://vaultwarden.resdigita.com\">Vaultwarden</a> : Gestion de vos mots de passe</p><p data-block-key=\"f2iiu\">Le login et le mot de passe sont indépendants et sécurisés comme nous le pouvons.</p><p data-block-key=\"5nr4m\">Ce dispositif est compatible avec tous <a href=\"https://bitwarden.com/fr-fr/download/\">les clients BitWarden</a> (télédéchagement pour ios, Android, Windows, Linux et Apple). Mettez « self-hosted » ou « auto-hébergé » avec l''adresse https://vaultwarden.resdigita.com.</p><p data-block-key=\"evsfi\"><a href=\"https://vaultwarden.resdigita.com\">https://vaultwarden.resdigita.com</a></p>"}}, {"id": "2d1f059c-8b24-491f-bdfe-7561b031fa1d", "type": "accordion", "value": {"title": "Administration", "content": "<p data-block-key=\"lofgf\"><a href=\"https://wagtail.resdigita.com/admin/\">Wagtail.Resdigita.Com</a> : Editer nos sites webs principaux</p><p data-block-key=\"8vkvq\"><a href=\"https://blog.lesgrandsvoisins.com/ghost/\">blog.lesgrandsvoisins.com/ghost</a> : Editer notre blog</p><p data-block-key=\"1giee\"><a href=\"https://list.lesgrandsvoisins.com/\">List Monk list</a> : Envoyer des newsletters</p><p data-block-key=\"an73n\"><a href=\"https://uptime-kuma.resdigita.com/\">Uptime Kuma admin</a> : Adminsitrer le tableau des statuts des services</p>"}}]}, {"id": "20903fac-1172-4545-8e5e-36ab6e02cefb", "type": "accordions", "value": [{"id": "27daad70-e84d-4489-b7f3-61f325c143c8", "type": "title", "value": "Outils régaliens pour des sites citoyens"}, {"id": "e76becb0-f82b-41ba-8723-df1c2fa89511", "type": "accordion", "value": {"title": "Wagtail-Fastoche pour vos chaînes d''édition numériques", "content": "<p data-block-key=\"lofgf\">site-exemple : <a href=\"https://wagtail.village.org\">wagtail.village.org</a><br/>dépôt source : <a href=\"https://github.com/chris2fr/wagtail-village\">github.com/chris2fr/wagtail-village</a><br/>bifurqué depuis <a href=\"https://github.com/nurmerique-gouv/sites-faciles\">github.com/nurmerique-gouv/sites-facile</a></p>"}}, {"id": "4d87391d-4540-4149-8804-2591ad569288", "type": "accordion", "value": {"title": "Django-Fastoche pour vos applications webs", "content": "<p data-block-key=\"lofgf\">site-exemple : <a href=\"https://django.village.org\">django.village.org</a><br/>dépôt source : <a href=\"https://github.com/chris2fr/django-village\">github.com/chris2fr/django-village</a><br/>bifurqué depuis <a href=\"https://github.com/nurmerique-gouv/django-dsfr\">github.com/nurmerique-gouv/django-dsfr</a></p>"}}, {"id": "57f4c3db-5ee4-4934-a4d6-a10ef4a293fd", "type": "accordion", "value": {"title": "Fabrique-Fastoche pour vos systèmes de design numériques", "content": "<p data-block-key=\"lofgf\">site-exemple : fabrique.village.org/example/ (prochainement)<br/>dépôt source : <a href=\"https://github.com/chris2fr/fabrique-village\">github.com/chris2fr/fabrique-village</a><br/>bifurqué depuis <a href=\"https://github.com/GouvernementFR/dsfr\">github.com/GouvernementFR/dsfr</a></p>"}}]}, {"id": "53834137-fc01-4885-a6a0-d58a562984d1", "type": "multicolumns", "value": {"title": "", "columns": [{"id": "a7c575ad-1bac-41e2-bc66-7642c0474a42", "type": "text_cta", "value": {"text": "<p data-block-key=\"fkikp\">ResDigita est le cellule numérique des Grands Voisins qui peut vous permettre un ensemble d''outils en logiciels libres info-gérés par notre association coopérative (ou par vous-mêmes).</p>", "cta_url": "https://www.resdigita.com", "cta_label": "Explorer l''alternative ResDigita !"}}, {"id": "e0146b05-bf9f-4f20-b188-3c3b0c27ca39", "type": "text_cta", "value": {"text": "<p data-block-key=\"fkikp\">Les Grands Voisins est une association dirigée par tout individu qui veut bien pour l''entraide et le partage de biens communs entre artistes, artisans, professionnels, associations, institutions et entreprises. Nous nous concentrons sur les arts plastiques, le numérique et le civisme.</p>", "cta_url": "https://www.lesgrandsvoisins.fr", "cta_label": "Rencontrer Les Grands Voisins !"}}, {"id": "464f14ec-569f-4378-88e9-ff180b0232a9", "type": "text_cta", "value": {"text": "<p data-block-key=\"fkikp\">Web-Fastoche est le fait de rendre génériques un ensemble de logiciels libres édités et exploités par l''Etat français pour ses sites .gouv.fr (dont certains outils pour plus de 20.000 noms de domaine).</p>", "cta_url": "https://web.village.org", "cta_label": "Développez votre site Web-Fastoche !"}}], "bg_color": "", "bg_image": null, "heading_tag": "h2", "bg_color_class": ""}}]', 'blue-france', 'Lire la mission des Grands Voisins', 'https://facile.lesgrandsvoisins.com/mission', true, 7, true, true, 'L''autre est une chance.');
INSERT INTO public.sites_faciles_contentpage VALUES (46, '[{"id": "da90273d-0467-4286-bf0f-e26029fe18e1", "type": "multicolumns", "value": {"title": "", "columns": [{"id": "a0b8fad7-01ee-4aac-94c8-71798d29635c", "type": "text", "value": "<h2 data-block-key=\"gnv4d\">Gestion des mots de passe</h2><p data-block-key=\"8t0k1\"><a href=\"https://vaultwarden.resdigita.com/\">https://vaultwarden.resdigita.com/</a></p><p data-block-key=\"3emie\"></p>"}], "bg_color": "", "bg_image": null, "heading_tag": "h2", "bg_color_class": ""}}]', NULL, NULL, NULL, false, NULL, false, false, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (47, '[{"id": "0e24b542-6a04-482d-8a1e-ef93d2ee07a9", "type": "paragraph", "value": "<p data-block-key=\"fggdt\"><a href=\"https://guichet.resdigita.com/\">Connecter à son compte</a> </p><p data-block-key=\"8gtcb\"></p><p data-block-key=\"3s4bk\"><a href=\"https://guichet.resdigita.com/user/new\">Créer un nouveau compte</a></p>"}]', NULL, NULL, NULL, false, NULL, false, false, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (48, '[{"id": "67c34257-588f-4b11-8055-bb10287eb3c8", "type": "imageandtext", "value": {"link": {"page": null, "text": "", "external_url": ""}, "page": null, "text": "<p data-block-key=\"zqptc\">Sur la base de l''application gestionnaire de contenu CMS et blog sous technologie Wagtail <a href=\"https://github.com/numerique-gouv/sites-faciles\">sites-faciles de numerique.gouv.fr</a>, elle-même à base de l''application de mise en page de site dynamique Django <a href=\"https://github.com/numerique-gouv/django-dsfr\">django-dsfr</a>, elle même basé sur le <a href=\"https://github.com/GouvernementFR/dsfr/\">Design Système de l''Etat ou DSFR du gouvernement français</a>, cette application gestionnaire de contenu dit CMS et blog <a href=\"https://www.village.org/\">Web Fastoche</a> propose de capitaliser sur le travail de fond pour le traitement d''information et de la communication en rendant les composants génériques.</p><p data-block-key=\"1ac8v\">Voici <a href=\"https://www.village.org/medias/images/architecture_htM7iUg.original.svg\">un diagramme</a> autour de l''idée de faire un outil publiquement utilisable <a href=\"https://github.com/chris2fr/wagtail-village\">Wagtail-Fastoche</a> avec <a href=\"https://github.com/chris2fr/django-village\">Django-Fastoche</a> <a href=\"https://pypi.org/project/django-village/\">Module Python Django-Fastoche</a> et <a href=\"https://github.com/chris2fr/fabrique-village\">Fabrique-Fastoche</a>.</p>", "image": 62, "link_url": "", "image_side": "right", "link_label": "", "image_ratio": "6"}}]', NULL, NULL, NULL, false, NULL, false, false, NULL);
INSERT INTO public.sites_faciles_contentpage VALUES (51, '[{"id": "c58f3abe-971d-41a7-ad49-f547da49dd04", "type": "multicolumns", "value": {"title": "", "columns": [{"id": "81d80008-6286-46b4-800e-edf8d4fce080", "type": "text", "value": "<h3 data-block-key=\"wopaa\">22nd Conseil of Les Grands Voisins</h3><p data-block-key=\"9sn6r\"></p><embed alt=\"flyer-20240506-conseildesgv22\" embedtype=\"image\" format=\"left\" id=\"54\"/><p data-block-key=\"4jn74\"></p><p data-block-key=\"f38i5\">Nous tiendrions le 22e Conseil des Voisins sur trois rencontres :</p><ul><li data-block-key=\"sqrq\">Lundi le 10 juin de 18h à 19h30</li><li data-block-key=\"5sba\">Jeudi le 13 juin de 19h à 20h30</li><li data-block-key=\"fabkv\">Samedi le le 15 juin de 10h30 à midi</li></ul><p data-block-key=\"a6q9u\">Ordre du jour :</p><ol><li data-block-key=\"43o40\">Numérique</li><li data-block-key=\"oioh\">Éventuellement Civisme</li><li data-block-key=\"7l6q8\">Communication</li></ol>"}, {"id": "460c495c-476c-43f1-bb69-aff7bf1509ea", "type": "text", "value": "<h3 data-block-key=\"avd09\"><b>Permanence le lundi de midi à 17h</b></h3><p data-block-key=\"e0hvf\"></p><embed alt=\"Abaker-Atelier\" embedtype=\"image\" format=\"left\" id=\"52\"/><p data-block-key=\"5c5gg\"></p><p data-block-key=\"dqj9a\">Venez à notre permanence <b>le lundi de 12h à 17h</b><br/>en personne à <b>93 ave. Du Gén. Leclerc</b> 75014 Paris (Métro Alésia, Atelier de la Ressourcerie, <i>7e boutique sur la gauche</i>) ou <a href=\"https://meet.lesg/\">meet.lesgrandsvoisins.com pour participer en hybride</a>.</p>"}, {"id": "6fc069f3-372a-447a-8d33-fd26389bdac8", "type": "text", "value": "<h3 data-block-key=\"avd09\"><a href=\"https://www.lesgrandsvoisins.fr/numerique/numerique-creatif/sociabilite-numerique/\"><b>Sociabilité numérique le lundi de 17h à 21h30</b></a></h3><embed alt=\"numasso\" embedtype=\"image\" format=\"right\" id=\"22\"/><p data-block-key=\"cda90\"><b>Le lundi de 17h à 21h30</b> se tient un atelier d’entraide numérique à l’Atelier au <b>93 ave. du Gén. Leclerc 75014 PARIS</b> à l’intérieur du <i>centre commercial 7e boutique sur la gauche</i></p>"}], "bg_color": "", "bg_image": null, "heading_tag": "h2", "bg_color_class": ""}}, {"id": "d3e4fb2e-7b28-4da0-99d3-09c2868a013f", "type": "paragraph", "value": "<p data-block-key=\"q7an3\"><a href=\"https://list.lesgrandsvoisins.com/subscription/form\"><b><i>Pensez à vous inscrire sur notre liste d''infos !</i></b></a></p><p data-block-key=\"evpog\">Nous sommes une association d''intérêt général agissant de manière contrôlée en subsidiarité ouverte pour toute organisation et tout individu qui en fait la décision pour soi-même principalement en ces trois domaines <a href=\"https://www.lesgrandsvoisins.fr/communautes/\">de Discussions</a>, <a href=\"https://www.lesgrandsvoisins.fr/arts/\">d''Arts plastiques</a> et <a href=\"https://www.lesgrandsvoisins.fr/numerique/\">de Numérique</a> selon <a href=\"https://www.lesgrandsvoisins.fr/mission/\">notre déclaration de mission</a>.</p><p data-block-key=\"bj8u6\"><a href=\"https://www.lesgrandsvoisins.fr/communautes/\"><b>Discussions</b></a> :</p><ul><li data-block-key=\"61obe\">Nous proposons des conditions encourageant la compréhension entre individus et organisations de perspectives différentes.</li><li data-block-key=\"9oi9e\">Nous ouvrons notre gouvernance pour des votes en Conseils des Voisins.</li></ul><p data-block-key=\"65va7\"><a href=\"https://www.lesgrandsvoisins.fr/arts/\"><b>Arts plastiques</b></a> :</p><ul><li data-block-key=\"a2qag\">Nous encourageons l’utilisation hybrides d’espace (commerces, hôpitaux, cafés, etc.) aussi en galléries locales.</li><li data-block-key=\"2ek3r\">Nous accompagnons des institutions pour trouver une juste place pour les arts plastiques dans leurs locaux.</li></ul><p data-block-key=\"91unk\"><a href=\"https://www.lesgrandsvoisins.fr/numerique/\"><b>Numérique</b></a> :</p><ul><li data-block-key=\"afesu\">Nous maintenons une constellation de serveurs et de services internet -- tableau de bord, création et gestion de compte, courriel, rencontres hybrides présence-distance, listes de diffusion numériques, blogs, serveurs webs, gestionnaire de mot de passe, enregistrement dans le nuage, forums de discussion, gestion de la connaissance, édition markdown, wikis, édition bueautique, etc. -- pour, par, et de nous-mêmes et d''autres.</li><li data-block-key=\"ej6ca\">Nous mettons à disposition des talents et des matériaux dans des salles de sociabilité numérique et dans des systèmes de prêts et échanges de matériels à l''attention d''individus et d''organisations de tous niveaux.</li></ul><p data-block-key=\"apjqo\">Voici quelques entités Grandes Voisines qui soutiennent notre démarche: A Good Solution, Banya Tour, Robert Baldridge, BMCP, Breast Freedom, Le Bureau du Créateur, Le Chiffon, Les Jardins du Numérique, Olivier Duhec, Inno Cube, Caroline Lhomme, Francesco du 59 Rivoli, Amadou Gaye, Informatruck, Inolib, KaceKode, Ada Matus, L’Odysée, La Ressourcerie Créative, Henry Roy, Peter Vaeth, Waka Conseil, Weezem, et peut-être des vôtres …</p><p data-block-key=\"at7dp\">Pour plus d’informations, contactez-nous : chris@lesgrandsvoisins.com +33 7 81 811 811</p><p data-block-key=\"4coel\"></p>"}, {"id": "f53195d5-0de5-479d-aef1-6151fbd266e0", "type": "video", "value": {"url": "https://www.youtube.com/embed/qZqErGfNF_o?feature=oembed", "title": "Les Grands Voisins, la cité rêvée", "caption": "c 3, 2020 Réalisation : Bastien Simon  Pitch : Maël, artiste sans papier, Adrien, luthier musicien et d''autres résidents venus de tous horizons, s''organisent pour donner naissance à une utopie moderne en plein coeur de Paris, un village solidaire de près de 2000 personnes : Les Grands Voisins. À travers leurs trajectoires et celles des membres fondateurs du lieu, le film interroge notre désir et notre capacité à inventer d’autres manières de vivre ensemble. Que retiendrons-nous de cette expérience collective ? Pourra-t-elle perdurer, essaimer, résonner ailleurs ?"}}, {"id": "03449e53-e17e-4ae3-ae63-7d9140be0b08", "type": "accordions", "value": [{"id": "6c6ec6eb-93af-4f4a-97e4-2f91d2498c57", "type": "title", "value": "Les Grands Voisins"}, {"id": "d2cecf58-044d-441c-849d-2d98d2085f7f", "type": "accordion", "value": {"title": "Sites Webs des Grands Voisins", "content": "<p data-block-key=\"h0w28\"><a href=\"https://www.lesgrandsvoisins.fr\">LesGrandsVoisins.fr</a> : Le site public principal des Grands Voisins</p><p data-block-key=\"6dm50\"><a href=\"https://www.resdigita.com\">ResDigita.com</a> : Le numérique des Grands Voisins se nomme ResDigita.</p><p data-block-key=\"3ntu0\"><a href=\"https://blog.lesgrandsvoisins.com\">Blog.LesGrandsVoisins.com</a> : Notre blog (accessible depuis le site principal)</p>"}}, {"id": "e02f2223-5134-4693-ac2b-639a3ec6f68a", "type": "accordion", "value": {"title": "Médias Sociaux des Grands Voisins", "content": "<p data-block-key=\"h0w28\"><a href=\"https://www.instagram.com/les_grands_voisins/\">Instagram@les_grands_voisins</a></p><p data-block-key=\"eseqg\">Facebook</p><p data-block-key=\"2vnko\">LinkedIn</p>"}}]}, {"id": "0e4ded21-5eb7-43a5-8cc1-db70172bb3c7", "type": "accordions", "value": [{"id": "05c4ba85-425d-4aa5-8176-489343275054", "type": "title", "value": "ResDigita, le numérique des Grands Voisins"}, {"id": "8d7c6179-ed2f-4faf-9283-7ef63fe2653d", "type": "accordion", "value": {"title": "Points de départs", "content": "<p data-block-key=\"lofgf\"><a href=\"https://homepage-dashboard.resdigita.com\">Homepage-Dashboard</a> : le tableau de bord avec nos applications principales</p><p data-block-key=\"38lbi\"><a href=\"https://guichet.resdigita.com\">Guichet (Login)</a> : Gérer votre compte (guichet est un logiciel développé par Les Grands Voisins)</p><p data-block-key=\"l50c\"><a href=\"https://guichet.resdigita.com/user/new\">Guichet (Inscription)</a> : Créer votre compte</p><p data-block-key=\"bkgrv\"><a href=\"https://www.lesgrandsvoisins.fr/numerique/\">LesGrandsVoisins.fr/numerique</a> : La description de nos activités numériques</p><p data-block-key=\"5s7rb\"><a href=\"https://quartz.resdigita.com\">Quartz</a> : Consulter notre documentation sur les outils numériques (par exemple, configurer votre courriel)</p><p data-block-key=\"3j715\"><a href=\"https://uptime-kuma.resdigita.com/status/gv\">Uptime-Kuma</a> : Vérifier l''état de nos services</p>"}}, {"id": "8d7d90ad-f09d-4d22-a218-975740578810", "type": "accordion", "value": {"title": "Applications unifiées", "content": "<p data-block-key=\"lofgf\"><a href=\"https://hedgedoc.resdigita.com\">Hedgedoc</a> : Edition en temps réel de documents partagés</p><p data-block-key=\"4u0e9\"><a href=\"https://vikunja.resdigita.com\">Vikunja</a> : Centraliser des informations sur des tous doux lisses</p>"}}, {"id": "b2f6f9ce-8042-4d8a-9536-b400b6987f6f", "type": "accordion", "value": {"title": "Courriel", "content": "<p data-block-key=\"lofgf\"><a href=\"https://mail.lesgrandsvoisins.com\">Roundcube Webmail</a> : Accéder à vos mels par le web</p><p data-block-key=\"1sbfd\">Le courriel est créé dans <a href=\"https://guichet.resdigita.com/user\">Guichet dans la partie « modifier mon profil »</a>. Chaque courriel que vous ajoutez sera un login distinct avec le même mot de passe que vous utilisez pour Guichet.</p><p data-block-key=\"47eua\">La <a href=\"https://quartz.resdigita.com/config/services/email\">documentation dans Quartz</a> peut vous aider à la configuration de votre courriel (sur smartphone par exemple).</p><p data-block-key=\"75pbq\">La <a href=\"https://guichet.resdigita.com/user/new\">création de compte sur Guichet</a> crée automatiquement un courriel correspondant.</p><p data-block-key=\"9gmov\"><a href=\"https://list.lesgrandsvoisins.com\">ListMonk</a> : Envois de Newsletters et gestions d''abonnements</p>"}}, {"id": "c3e087d4-583c-4d58-b836-c5f3c2f9dbc5", "type": "accordion", "value": {"title": "Mot de passe", "content": "<p data-block-key=\"lofgf\"><a href=\"https://vaultwarden.resdigita.com\">Vaultwarden</a> : Gestion de vos mots de passe</p><p data-block-key=\"f2iiu\">Le login et le mot de passe sont indépendants et sécurisés comme nous le pouvons.</p><p data-block-key=\"5nr4m\">Ce dispositif est compatible avec tous <a href=\"https://bitwarden.com/fr-fr/download/\">les clients BitWarden</a> (télédéchagement pour ios, Android, Windows, Linux et Apple). Mettez « self-hosted » ou « auto-hébergé » avec l''adresse https://vaultwarden.resdigita.com.</p><p data-block-key=\"evsfi\"><a href=\"https://vaultwarden.resdigita.com\">https://vaultwarden.resdigita.com</a></p>"}}, {"id": "2d1f059c-8b24-491f-bdfe-7561b031fa1d", "type": "accordion", "value": {"title": "Administration", "content": "<p data-block-key=\"lofgf\"><a href=\"https://wagtail.resdigita.com/admin/\">Wagtail.Resdigita.Com</a> : Editer nos sites webs principaux</p><p data-block-key=\"8vkvq\"><a href=\"https://blog.lesgrandsvoisins.com/ghost/\">blog.lesgrandsvoisins.com/ghost</a> : Editer notre blog</p><p data-block-key=\"1giee\"><a href=\"https://list.lesgrandsvoisins.com/\">List Monk list</a> : Envoyer des newsletters</p><p data-block-key=\"an73n\"><a href=\"https://uptime-kuma.resdigita.com/\">Uptime Kuma admin</a> : Adminsitrer le tableau des statuts des services</p>"}}]}, {"id": "20903fac-1172-4545-8e5e-36ab6e02cefb", "type": "accordions", "value": [{"id": "27daad70-e84d-4489-b7f3-61f325c143c8", "type": "title", "value": "Outils régaliens pour des sites citoyens"}, {"id": "e76becb0-f82b-41ba-8723-df1c2fa89511", "type": "accordion", "value": {"title": "Wagtail-Fastoche pour vos chaînes d''édition numériques", "content": "<p data-block-key=\"lofgf\">site-exemple : <a href=\"https://wagtail.village.org\">wagtail.village.org</a><br/>dépôt source : <a href=\"https://github.com/chris2fr/wagtail-village\">github.com/chris2fr/wagtail-village</a><br/>bifurqué depuis <a href=\"https://github.com/nurmerique-gouv/sites-faciles\">github.com/nurmerique-gouv/sites-facile</a></p>"}}, {"id": "4d87391d-4540-4149-8804-2591ad569288", "type": "accordion", "value": {"title": "Django-Fastoche pour vos applications webs", "content": "<p data-block-key=\"lofgf\">site-exemple : <a href=\"https://django.village.org\">django.village.org</a><br/>dépôt source : <a href=\"https://github.com/chris2fr/django-village\">github.com/chris2fr/django-village</a><br/>bifurqué depuis <a href=\"https://github.com/nurmerique-gouv/django-dsfr\">github.com/nurmerique-gouv/django-dsfr</a></p>"}}, {"id": "57f4c3db-5ee4-4934-a4d6-a10ef4a293fd", "type": "accordion", "value": {"title": "Fabrique-Fastoche pour vos systèmes de design numériques", "content": "<p data-block-key=\"lofgf\">site-exemple : fabrique.village.org/example/ (prochainement)<br/>dépôt source : <a href=\"https://github.com/chris2fr/fabrique-village\">github.com/chris2fr/fabrique-village</a><br/>bifurqué depuis <a href=\"https://github.com/GouvernementFR/dsfr\">github.com/GouvernementFR/dsfr</a></p>"}}]}, {"id": "53834137-fc01-4885-a6a0-d58a562984d1", "type": "multicolumns", "value": {"title": "", "columns": [{"id": "a7c575ad-1bac-41e2-bc66-7642c0474a42", "type": "text_cta", "value": {"text": "<p data-block-key=\"fkikp\">ResDigita est le cellule numérique des Grands Voisins qui peut vous permettre un ensemble d''outils en logiciels libres info-gérés par notre association coopérative (ou par vous-mêmes).</p>", "cta_url": "https://www.resdigita.com", "cta_label": "Explorer l''alternative ResDigita !"}}, {"id": "e0146b05-bf9f-4f20-b188-3c3b0c27ca39", "type": "text_cta", "value": {"text": "<p data-block-key=\"fkikp\">Les Grands Voisins est une association dirigée par tout individu qui veut bien pour l''entraide et le partage de biens communs entre artistes, artisans, professionnels, associations, institutions et entreprises. Nous nous concentrons sur les arts plastiques, le numérique et le civisme.</p>", "cta_url": "https://www.lesgrandsvoisins.fr", "cta_label": "Rencontrer Les Grands Voisins !"}}, {"id": "464f14ec-569f-4378-88e9-ff180b0232a9", "type": "text_cta", "value": {"text": "<p data-block-key=\"fkikp\">Web-Fastoche est le fait de rendre génériques un ensemble de logiciels libres édités et exploités par l''Etat français pour ses sites .gouv.fr (dont certains outils pour plus de 20.000 noms de domaine).</p>", "cta_url": "https://web.village.org", "cta_label": "Développez votre site Web-Fastoche !"}}], "bg_color": "", "bg_image": null, "heading_tag": "h2", "bg_color_class": ""}}]', 'blue-france', 'Lire la mission des Grands Voisins', 'https://facile.lesgrandsvoisins.com/mission', true, 7, true, true, 'L''autre est une chance.');
INSERT INTO public.sites_faciles_contentpage VALUES (50, '[{"id": "42657c06-8cf0-48f4-a0e0-9516b7eefacf", "type": "imageandtext", "value": {"link": {"page": null, "text": "Je m''inscris pour venir à distance ou en présence !", "external_url": "https://www.helloasso.com/associations/les-grands-voisins/#event"}, "page": null, "text": "<p data-block-key=\"h3np9\">Venez assister à notre événement magique de rencontres, débats et partages, pour notre 22ième Conseils des Grands Voisins. Nous vous attendons nombreux pour ce moment inoubliable au café LPP dans le 14ème arrondissements de paris! Grâce à vous nos projets deviendront réalité!</p><p data-block-key=\"3rr48\">Nous tiendrions le 22e Conseil des Voisins sur trois rencontres :</p><ul><li data-block-key=\"9s2me\"><a href=\"https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins\">Lundi le 10 juin de 19h45 à 21h25</a></li><li data-block-key=\"dofhv\"><a href=\"https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins-1\">Jeudi le 13 juin de 15h15 à 16h45</a></li><li data-block-key=\"5dfoa\"><a href=\"https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins-2\">Samedi le le 15 juin de 10h15 à 11h45</a></li></ul><h3 data-block-key=\"3upfj\">Sujets à l''ordre du jour</h3><ol><li data-block-key=\"77v0b\">Numérique</li><li data-block-key=\"lf41\">Civisme</li><li data-block-key=\"cg5q6\">Communication</li></ol>", "image": 63, "link_url": "", "image_side": "right", "link_label": "", "image_ratio": "3"}}, {"id": "94da2fec-0e86-4221-81c9-84d5e5add70e", "type": "multicolumns", "value": {"title": "Nous tiendrions le 22e Conseil des Voisins sur trois rencontres :", "columns": [{"id": "1a34efa4-2f1d-46ac-8502-87af2f2fab69", "type": "text_cta", "value": {"text": "<p data-block-key=\"w66ax\">Lundi le 10 juin de 19h45 à 21h15</p>", "cta_url": "https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins", "cta_label": "Je viens lundi !"}}, {"id": "1dada3fc-265e-4f23-92d8-fb3c2a0eacdb", "type": "text_cta", "value": {"text": "<p data-block-key=\"w66ax\">Jeudi le 13 juin de 15h15 à 16h45</p>", "cta_url": "https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins-1", "cta_label": "Choisir jeudi"}}, {"id": "880db029-c159-435d-89d9-63b9fa252354", "type": "text_cta", "value": {"text": "<p data-block-key=\"w66ax\">Samedi le 15 juin de 10h15 à 11h45</p>", "cta_url": "https://www.helloasso.com/associations/les-grands-voisins/evenements/les-grands-voisins-2", "cta_label": "Choisir samedi"}}], "bg_color": "", "bg_image": 63, "heading_tag": "h2", "bg_color_class": "green-tilleul-verveine"}}]', 'blue-france', 'Inscriptions', 'https://www.helloasso.com/associations/les-grands-voisins/#event', false, 63, false, false, 'Je m''inscris pour une des dates (en présence OU à distance)');


