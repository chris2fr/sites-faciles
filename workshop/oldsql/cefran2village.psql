INSERT INTO public.wagtailcore_revision VALUES (106, '2024-05-07 14:46:21.487353+00', '{"pk": 36, "body": "[{\"type\": \"paragraph\", \"value\": \"<p data-block-key=\\\"kokwt\\\">Aux Grands Voisins nous avons <a linktype=\\\"page\\\" id=\\\"30\\\">un manifeste</a> \\u00e9dit\\u00e9e par Chris Mann avec des contributions de plus de cent personnes sur deux grands ateliers et plusieurs s\\u00e9ances de travail sur une p\\u00e9riode d''un mois en 2017. Chacun a un avis qui est le sien sur ce que c''est \\u00eatre Grands Voisins et chacun a raison.</p><p data-block-key=\\\"b4dti\\\">L''objectif des activit\\u00e9s dans notre cellule civisme est de promouvoir ce qu''il faut pour avoir les possibilit\\u00e9s de rencontres ou non. Cela d\\u00e9coule m\\u00eame de l''all\\u00e9gorie du voisin ; l''on peut rester chez soi, l''on peut sortir, l''on peut inviter d''autres.</p><p data-block-key=\\\"aovvo\\\">Concr\\u00e8tement, nous pouvons commencer par un annuaire QuiQuoiEtc qui permet de r\\u00e9f\\u00e9rencer les voisins qui veulent bien. Cela permet une communititude et des possibilit\\u00e9s de compr\\u00e9hensions, empathies et entraides int\\u00e9ressantes.</p><p data-block-key=\\\"4j94g\\\"></p>\", \"id\": \"e569e14e-92d4-476c-96b3-2b221f2ce890\"}]", "live": true, "path": "000100020007", "slug": "civisme", "depth": 3, "owner": 1, "title": "Civisme", "locale": 1, "locked": false, "expired": false, "alias_of": null, "numchild": 0, "url_path": "/home/civisme/", "expire_at": null, "locked_at": null, "locked_by": null, "seo_title": "", "go_live_at": null, "draft_title": "Civisme", "content_type": 25, "header_image": null, "header_large": false, "header_darken": false, "live_revision": 105, "show_in_menus": false, "header_cta_link": null, "header_cta_text": null, "latest_revision": 105, "translation_key": "8a173765-a6a8-40e1-89ea-de7f1986d532", "contentpage_tags": [], "header_cta_label": null, "header_with_title": false, "last_published_at": "2024-05-07T07:28:54.538Z", "first_published_at": "2024-05-07T07:28:54.538Z", "header_color_class": null, "search_description": "Aux Grands Voisins nous avons un manifeste éditée par Chris Mann avec des contributions de plus de cent personnes sur […]", "wagtail_admin_comments": [], "has_unpublished_changes": false, "latest_revision_created_at": "2024-05-07T07:28:54.511Z"}', NULL, '36', 1, 25, 1, 'Civisme');
INSERT INTO public.wagtailcore_revision VALUES (137, '2024-05-22 15:53:00.48361+00', '{"pk": 48, "body": "[{\"type\": \"imageandtext\", \"value\": {\"image\": 62, \"image_side\": \"right\", \"image_ratio\": \"6\", \"text\": \"<p data-block-key=\\\"zqptc\\\">Sur la base de l''application gestionnaire de contenu CMS et blog sous technologie Wagtail <a href=\\\"https://github.com/numerique-gouv/sites-faciles\\\">sites-faciles de numerique.gouv.fr</a>, elle-m\\u00eame \\u00e0 base de l''application de mise en page de site dynamique Django <a href=\\\"https://github.com/numerique-gouv/django-dsfr\\\">django-dsfr</a>, elle m\\u00eame bas\\u00e9 sur le <a href=\\\"https://github.com/GouvernementFR/dsfr/\\\">Design Syst\\u00e8me de l''Etat ou DSFR du gouvernement fran\\u00e7ais</a>, cette application gestionnaire de contenu dit CMS et blog <a href=\\\"https://www.village.org/\\\">Web Fastoche</a> propose de capitaliser sur le travail de fond pour le traitement d''information et de la communication en rendant les composants g\\u00e9n\\u00e9riques.</p><p data-block-key=\\\"1ac8v\\\">Voici <a href=\\\"https://www.village.org/medias/images/architecture_htM7iUg.original.svg\\\">un diagramme</a> autour de l''id\\u00e9e de faire un outil publiquement utilisable <a href=\\\"https://github.com/chris2fr/wagtail-village\\\">Wagtail-Fastoche</a> avec <a href=\\\"https://github.com/chris2fr/django-village\\\">Django-Fastoche</a> <a href=\\\"https://pypi.org/project/django-village/\\\">Module Python Django-Fastoche</a> et <a href=\\\"https://github.com/chris2fr/fabrique-village\\\">Fabrique-Fastoche</a>.</p>\", \"link\": {\"page\": null, \"external_url\": \"\", \"text\": \"\"}, \"link_label\": \"\", \"page\": null, \"link_url\": \"\"}, \"id\": \"67c34257-588f-4b11-8055-bb10287eb3c8\"}]", "live": true, "path": "0001000200050004", "slug": "fabrique-django-wagtail", "depth": 4, "owner": 1, "title": "Web Fastoche à partir de Gouv.FR", "locale": 1, "locked": false, "expired": false, "alias_of